             /* <uniquifier>：使用唯一且具有描述性的类名
             <weight>：使用 200 到 1000 之间的值*/

          /* latin */
          @font-face {
            font-family: "Mulish";
            font-style: normal;
            font-weight: 200 1000;
            font-display: swap;
            src: url({{ 'mulish.woff2' | file_url }})
              format("woff2");
            /* 这段代码定义了字体支持的Unicode字符范围:
               - U+0000-00FF: 基本拉丁字符和扩展拉丁字符
               - U+0131: 土耳其小写字母i
               - U+0152-0153: 拉丁字母OE连字
               - U+02BB-02BC: 修饰符号字母
               - U+02C6: 扬抑符号
               - U+02DA: 圆圈上标
               - U+02DC: 小波浪线
               - U+0304,U+0308,U+0329: 组合变音标记
               - U+2000-206F: 常用标点符号
               - U+20AC: 欧元符号
               - U+2122: 商标符号
               - U+2191,U+2193: 上下箭头
               - U+2212: 减号
               - U+2215: 除号斜线
               - U+FEFF: 零宽不换行空格
               - U+FFFD: 替换字符
            */
            unicode-range: U+0000-00FF,
              /* 基本拉丁字母 - 英语、德语、法语、意大利语 */ U+0100-024F,
              /* 扩展拉丁字母 - 德语、法语、意大利语 */ U+0370-03FF,
              /* 希腊字母 */ U+0400-04FF, /* 西里尔字母 */ U+3040-309F,
              /* 平假名 - 日语 */ U+30A0-30FF, /* 片假名 - 日语 */ U+3130-318F,
              /* 谚文兼容字母 - 韩语 */ U+AC00-D7AF,
              /* 谚文音节 - 韩语 */ U+4E00-9FFF,
              /* 中日韩统一表意文字 - 中文、日文汉字 */ U+0131, U+0152-0153,
              U+02BB-02BC, U+02C6, U+02DA, U+02DC, U+0304, U+0308, U+0329,
              U+2000-206F, U+20AC, U+2122, U+2191, U+2193, U+2212, U+2215, U+FEFF,
              U+FFFD;
          }
     /* -- font change end -- */

