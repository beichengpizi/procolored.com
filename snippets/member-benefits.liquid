{%- assign type = type -%}
<style>
  .member-benefits-container-{{ section.id }}  .member-benefits-content  .item-{{ block.id }}{
    {%- if block.settings.bg_image -%}
      background: url({{ block.settings.bg_image | image_url }}) {{ section.settings.item_bg_color }} no-repeat center / cover;
    {%- else -%}
      background-color: {{ section.settings.item_bg_color }};
  {%- endif %}
  }
  @media screen and (min-width: 320px) and (max-width: 480px){
    .member-benefits-container-{{ section.id }}  .member-benefits-content  .item-{{ block.id }}{
      {%- if block.settings.bg_image_mobile -%}
        background: url({{ block.settings.bg_image_mobile | image_url }}) {{ section.settings.item_bg_color }} no-repeat center / cover;
    
    {%- endif %}
    }

  }
</style>
{%- case type -%}
  {%- when 'member' -%}
  <{%- if customer -%}div{%- else -%}modal-login client:idle{%- endif %} class="item item-{{ block.id }}  relative   flex flex-col items-center w-full transition-colors" >
    {%- if section.settings.show_label and block.settings.label_text != blank -%} 
      <div class="label-wrap">
        <div class="label-container">
