          </svg>
          <div class="period-title">
            <div class="period-title-text">Discount</div>
            <div class="period-price">{{- deposit_price -}}</div>
          </div>
        </div>
        <div class="desc-area">
          <ul class="desc-list">
            <li>
              {%- if section.settings.description_heading != blank -%}
                <div class="desc-heading">
                  {{- section.settings.description_heading -}}
                </div>
              {%- endif -%}
              {%- if section.settings.description_description != blank -%}
                <div class="desc-text">
                  {{- section.settings.description_description -}}
                </div>
              {%- endif -%}
              {%- if section.settings.description_note != blank -%}
                <div class="note">
                  {{- section.settings.description_note -}}
                </div>
              {%- endif -%}
            </li>
          </ul>
        </div>
      </div>
    </div>
    {%- if section.settings.product_list != blank -%}
      <div class="Slider-{{ section.id }}">
        {%- assign product_list = section.settings.product_list -%}
        {%- assign product_count = section.settings.product_list.count -%}

        <div class="swiper-wrapper ">
          {%- for product in product_list -%}
            <div class="swiper-slide">
              <div class="Slide-product-card-{{ section.id }}">
                <div class="lable">- {{ deposit_price -}}</div>
                <a
                  href="{{ product.url | split: '?' | first }}"
                  title="{{- product.title | escape -}}"
                  class="product-img"
                >
                  {% assign image = product.metafields.custom.transparent_product_image
                    | default: product.featured_image
                  %}
                  {% assign size = 100 | divided_by: product_count | round %}
                  <img
                    style="aspect-ratio:{{image.aspect_ratio}};"
                    data-sizes="{{ size }}vw"
                    data-srcset="
                      {%- if image.width >= 200 -%}{{ image | image_url: width: 200 }} 200w,{%- endif -%}
                      {%- if image.width >= 375 -%}{{ image | image_url: width: 375 }} 375w,{%- endif -%}
                      {%- if image.width >= 550 -%}{{ image | image_url: width: 550 }} 550w,{%- endif -%}
                      {%- if image.width >= 750 -%}{{ image | image_url: width: 750 }} 750w,{%- endif -%}
                      {%- if image.width >= 1100 -%}{{ image | image_url: width: 1100 }} 1100w,{%- endif -%}
                      {%- if image.width >= 1500 -%}{{ image | image_url: width: 1500 }} 1500w,{%- endif -%}
                      {%- if image.width >= 1780 -%}{{ image | image_url: width: 1780 }} 1780w,{%- endif -%}
                      {%- if image.width >= 2000 -%}{{ image | image_url: width: 2000 }} 2000w,{%- endif -%}
                      {%- if image.width >= 3000 -%}{{ image | image_url: width: 3000 }} 3000w,{%- endif -%}
                      {%- if image.width >= 3840 -%}{{ image | image_url: width: 3840 }} 3840w,{%- endif -%}
                      {{ image | image_url }} {{ image.width }}w
                    "
                    data-src="{{ image | image_url:width:750 }}"
                    class="lazyload "
                    alt="{{ product.title | escape }}"
                  >
                </a>
                <div class="product-name" title="{{- product.title | escape -}}">
                  {{- product.title | escape -}}
                </div>
                <!-- 价格显示 -->
                <div class="product-price">
                  <span class="price-sale">{{ product.price | money }}</span>
                  {%- if product.compare_at_price > product.price -%}
                    <span class="price-regular">{{ product.compare_at_price | money }}</span>
                  {%- endif -%}
                </div>
                {%- if section.settings.button_text != blank and section.settings.button_link -%}
                  <a class="product-button" href="{{ section.settings.button_link }}" target="_blank">
                    {{- section.settings.button_text -}}
                  </a>
                {%- endif -%}
              </div>
            </div>
          {%- endfor -%}
        </div>
        <div class="swiper-pagination swiper-pagination-{{ section.id }}"></div>
      </div>
    {%- endif -%}
  </div>
</div>
<script language="javascript">
  new Swiper('.Slider-{{ section.id }}', {
     slidesPerView: {{ product_count }},
     spaceBetween: 16,
     preventClicksPropagation: false,
     observer: true,
     observeParents: true,
     pagination: {
        el: '.swiper-pagination-{{ section.id }}',
        clickable: true, // 确保分页是可点击的
      },
     on: {
       init: function (swiper) {
         if (!isMobile()) {
           console.log('禁止拖动');
           // 禁止拖动
           this.allowTouchMove = false;
         } else {
           // 允许拖动
           console.log('允许拖动');
           this.allowTouchMove = true;
         }
       },
     },
     breakpoints: {
       //当宽度小于等于768时显示
       768: {
         slidesPerView: 1.3,
         spaceBetween: 10,
         slidesOffsetBefore : 10,
         slidesOffsetAfter : 10,
       },
       1024: {
         slidesPerView: 3,
         spaceBetween: 10,
         slidesOffsetBefore : 10,
         slidesOffsetAfter : 10,
       },
     },
   });
</script>
