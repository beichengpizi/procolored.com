{%- liquid
  assign product = section.settings.product
  assign image = section.settings.image | default: product.featured_image
  assign title = product.title
  assign subtitle = product.metafields.custom.subtitle
  assign price = product.price | default: product.metafields.custom.rent_price.value

  assign deposit = product.metafields.custom.deposit.value
-%}

<div class="rent-products-{{ section.id }}">
  <div class="main-card">
    <div class="main-card-header">
      <div class="lease-tip">
        {%- if section.settings.product_img_headding != blank -%}
          <span class="title">{{- section.settings.product_img_headding -}}</span>
        {%- endif -%}
        {%- if section.settings.product_img_subheadding != blank -%}
          <span class="sub">{{- section.settings.product_img_subheadding -}}</span>
        {%- endif -%}
      </div>
    </div>
    <div class="main-card-content">
      <div class="img-area">
        {%- if image -%}
          <div>
            <img
              data-sizes="50vw"
              data-srcset="
                {%- if image.width >= 200 -%}{{ image | image_url: width: 200 }} 200w,{%- endif -%}
                {%- if image.width >= 375 -%}{{ image | image_url: width: 375 }} 375w,{%- endif -%}
                {%- if image.width >= 550 -%}{{ image | image_url: width: 550 }} 550w,{%- endif -%}
                {%- if image.width >= 750 -%}{{ image | image_url: width: 750 }} 750w,{%- endif -%}
                {%- if image.width >= 1100 -%}{{ image | image_url: width: 1100 }} 1100w,{%- endif -%}
                {%- if image.width >= 1500 -%}{{ image | image_url: width: 1500 }} 1500w,{%- endif -%}
                {%- if image.width >= 1780 -%}{{ image | image_url: width: 1780 }} 1780w,{%- endif -%}
                {%- if image.width >= 2000 -%}{{ image | image_url: width: 2000 }} 2000w,{%- endif -%}
                {%- if image.width >= 3000 -%}{{ image | image_url: width: 3000 }} 3000w,{%- endif -%}
                {%- if image.width >= 3840 -%}{{ image | image_url: width: 3840 }} 3840w,{%- endif -%}
                {{ image | image_url }} {{ image.width }}w
              "
              data-src="{{ image | image_url:width:750 }}"
              class="lazyload "
            >
          </div>
        {%- else -%}
          {{ 'product-1' | placeholder_svg_tag: 'placeholder_svg_tag' }}
        {%- endif -%}
      </div>
      <!-- 信息区域 -->
      {% assign deposit_price = null %}
      {% for deposit_as_discount in shop.metaobjects.deposit_as_discount.values limit: 1 %}
        {% assign deposit_price = deposit_as_discount.deposit.value | money_without_trailing_zeros %}
        {% assign original_deposit = deposit_as_discount.original_deposit.value | money_without_trailing_zeros %}
      {% endfor %}

      <div class="info-area">
        <div class="period">
          <div class="period-title">
            <div class="period-title-text">Deposit</div>
            <div class="period-price">{{- original_deposit -}}</div>
