<style>
  .coupon-wrap {
    background-color: #000;
  }
  .coupon-wrap .title {
    width: 100%;
    font-family: ItalianPlateNo2Expanded-Bold !important;
    font-weight: bold;
    font-size: 40px;
    color: #fece67;
    line-height: 1;
    text-align: center;
    font-style: normal;
    text-transform: none;
    margin-bottom: 24px;
  }
  .coupon-wrap .subtitle {
    width: 100%;
    font-family: ItalianPlateNo2Expanded-Regular !important;
    font-weight: 400;
    font-size: 24px;
    color: #ffffff;
    line-height: 1;
    text-align: center;
    font-style: normal;
    text-transform: none;
    margin-bottom: 32px;
  }
  .coupon-wrap .desc {
    width: 100%;
    font-family: ItalianPlateNo2Expanded-Regular !important;
    font-weight: 400;
    font-size: 16px;
    color: #848484;
    line-height: 1;
    text-align: center;
    font-style: normal;
    text-transform: none;
    margin-bottom: 45px;
  }
  .coupon-wrap .coupon-container {
    display: flex;
    flex-wrap: wrap;
    max-width: 1200px;
    margin: 0 auto;
    gap: 20px;
    font-family: ItalianPlateNo2Expanded-Bold !important;
    font-weight: 700 !important;
  }

  .coupon-item {
    position: relative;
    box-sizing: border-box;
    text-align: center;
    height: 200px;
    width: 100%;
    flex: 1 1 calc(16.666% - 40px);
  }

  .bg-before,
  .bg-after {
    width: 100%;
    height: 100%;
  }
  .bg-after {
    display: none;
  }
  .coupon-bg-container {
    position: absolute;
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
  }
  .coupon-content {
    position: absolute;
    z-index: 1;
    padding: 24px 24px 12px 24px;
    text-align: center;
    display: flex;
    flex-direction: column;
    align-items: baseline;
    justify-content: space-between;
    row-gap: 20px;
    box-sizing: border-box;
    height: 200px;
    width: 184px;
  }
  .coupon-content-top {
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    row-gap: 5px;
    width: 100%;
  }
  .price {
    display: flex;
    align-items: start;
    line-height: 1;
  }
  .currency-symbol {
    font-size: 18px;
    height: 100%;
    line-height: 30px;
  }
  .discounts-price {
    font-size: 70px;
  }
  .origin-price {
    font-size: 18px;
    line-height: 1;
  }
  .coupon-content-bottom {
    width: 100%;
  }
  .coupon-content-bottom .button {
    width: 134px;
    height: 34px;
    background: #171717;
    border-radius: 25px 25px 25px 25px;
    color: #fece67;
    display: flex;
    align-items: center;
    justify-content: center;
    line-height: 1;
    font-size: 14px;
    cursor: pointer;
    padding: 0;
  }

  /* 移动端媒体查询 */
  @media (max-width: 768px) {
    .coupon-wrap {
      background-color: #000;
      padding: 0 12px;
    }
    .coupon-wrap .title {
      font-size: 30px;

      margin-bottom: 12px;
    }
    .coupon-wrap .subtitle {
      font-size: 14px;

      margin-bottom: 16px;
    }
    .coupon-wrap .desc {
      font-size: 12px;

      margin-bottom: 34px;
    }
    .coupon-wrap .coupon-container {
      display: flex;
      flex-wrap: wrap;
      max-width: 1200px;
      margin: 0 auto;
      gap: 10px;
      font-family: ItalianPlateNo2Expanded-Bold !important;
      font-weight: 700 !important;
    }

    .coupon-item {
      position: relative;
      box-sizing: border-box;
      text-align: center;
      height: 120px;
      width: 100%;
      flex: 1 1 calc(33.333% - 20px);
    }

    .bg-before,
    .bg-after {
      width: 100%;
      height: 100%;
    }

    .bg-before svg,
    .bg-after svg {
      height: 100%;
      width: 100%;
    }
    .bg-after {
      display: none;
    }
    .coupon-bg-container {
      position: absolute;
      top: 0;
      bottom: 0;
      left: 0;
      right: 0;
    }
    .coupon-content {
      position: absolute;
      z-index: 1;
      padding: 14px 15px 8px 15px;
      text-align: center;
      display: flex;
      flex-direction: column;
      align-items: center;
      justify-content: space-between;
      row-gap: 20px;
      box-sizing: border-box;
      height: 120px;
      width: 100%;
    }
    .coupon-content-top {
      display: flex;
      flex-direction: column;
      align-items: center;
      justify-content: center;
      row-gap: 5px;
      width: 100%;
    }
    .price {
      display: flex;
      align-items: start;
      line-height: 1;
    }
    .currency-symbol {
      font-size: 12px;
      height: 100%;
      line-height: 30px;
    }
    .discounts-price {
      font-size: 40px;
    }
    .origin-price {
      font-size: 12px;
      line-height: 1;
    }
    .coupon-content-bottom {
      width: auto;
    }
    .coupon-content-bottom .button {
      width: 75px;
      height: 20px;
      background: #171717;
      border-radius: 30px;
      color: #fece67;
      display: flex;
      align-items: center;
      justify-content: center;
      line-height: 1;
      font-size: 12px;
      cursor: pointer;
      padding: 0;
    }
  }
</style>
