<script>
  $(document).ready(function () {
    // 检查 localStorage 中的状态
    $('.coupon-item').each(function () {
      const couponId = $(this).attr('id');
      if (localStorage.getItem(couponId) === 'true') {
        console.log($(this), couponId);
        $(this).find('.bg-before').hide();
        $(this).find('.bg-after').show();
        $(this).find('.button').text('Collected');
        $(this).find('.button').css('cursor', 'not-allowed');
      }
    });

    // 点击事件处理
    $('.coupon-item .button').click(function () {
      const $couponItem = $(this).closest('.coupon-item');
      const couponId = $couponItem.attr('id');

      // 切换显示和隐藏
      $couponItem.find('.bg-before').hide();
      $couponItem.find('.bg-after').show();

      // 修改按钮文本
      $(this).text('Collected');
      $(this).css('cursor', 'not-allowed');

      // 保存状态到 localStorage
      localStorage.setItem(couponId, 'true');
    });
  });
</script>
