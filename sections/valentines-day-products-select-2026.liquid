                            $(function () {
                              function hasParent(ele, p) {
                                while (ele && ele !== document.body) {
                                  if (ele === p) return true;
                                  ele = ele.parentNode;
                                }
                                return false;
                              }

                              // 获取当前下拉元素
                              const dropdown = document.getElementById('valentines-day-dropdown-mock-{{ block.id }}');
                              if (!dropdown || dropdown.getAttribute('data-bind')) return;
                              dropdown.setAttribute('data-bind', '1');

                              const sectionRoot = dropdown.closest('.valentines-day-product-tab-content');
                              if (!sectionRoot) return;

                              const cardContainer = sectionRoot.querySelectorAll('.valentines-day-product-tab');
                              const selected = dropdown.querySelector('[data-selected]');
                              const optionsUl = dropdown.querySelector('[data-options]');
                              const options = optionsUl
                                ? optionsUl.querySelectorAll('.valentines-day-dropdown-mock-option')
                                : [];

                              function closeDropdown() {
                                dropdown.classList.remove('open');
                              }
                              function openDropdown() {
                                dropdown.classList.add('open');
                              }

                              // 点击展开收起
                              if (selected) {
                                selected.addEventListener('click', function (e) {
                                  e.preventDefault();
                                  e.stopPropagation();
                                  if (dropdown.classList.contains('open')) closeDropdown();
                                  else openDropdown();
                                });
                              }

                              // —— 关键：防止“切回第一个 tab”时页面自动上滚
                              function lockScrollAndRun(run) {
                                // 只在切换到第一个 tab 时锁滚（按你现象精准命中）
                                const scrollTop = window.pageYOffset || document.documentElement.scrollTop;

                                // 让浏览器不要为了“保持焦点可见”而自动滚（尤其是 layout 高度变化时）
                                if (document.activeElement && typeof document.activeElement.blur === 'function') {
                                  document.activeElement.blur();
                                }

                                run();

                                // 等 class 切换引发的布局变更生效后，恢复滚动位置（两帧更稳）
                                requestAnimationFrame(function () {
                                  requestAnimationFrame(function () {
                                    window.scrollTo(0, scrollTop);
                                  });
                                });
                              }

                              // 选项点击
                              options.forEach(function (option, idx) {
                                option.addEventListener('click', function (e) {
                                  e.preventDefault();
                                  e.stopPropagation();

                                  const doSwitch = function () {
                                    // 激活当前
                                    options.forEach(function (opt) {
                                      opt.classList.remove('active');
                                    });
                                    option.classList.add('active');
                                    if (selected) selected.innerText = option.innerText;
                                    closeDropdown();

                                    // 分发事件
                                    const event = new CustomEvent('valentines-dayTabCustomDropdownChange', {
                                      detail: {
                                        idx: idx,
                                        value: option.getAttribute('data-value'),
                                        text: option.innerText,
                                      },
                                      bubbles: true,
                                    });
                                    dropdown.dispatchEvent(event);

                                    // 联动select（实际是在切换 tab 内容 + 同步 dropdown 状态）
                                    try {
                                      const tabContainers = dropdown
                                        .closest('.valentines-day-product-tab-content')
                                        .querySelectorAll('.valentines-day-product-tab');

                                      cardContainer.forEach(function (tabCt, index) {
                                        if (index === idx) {
                                          tabCt.classList.add('active');

                                          const activeTabs = Array.from(tabContainers).filter(function (ct) {
                                            return ct.classList.contains('active');
                                          });

                                          activeTabs.forEach(function (activeTab) {
                                            const dropdownOptions = activeTab.querySelectorAll(
                                              '.valentines-day-dropdown-mock-option'
                                            );
                                            let dropdownMockParent = null;
                                            if (dropdownOptions.length > 0) {
                                              dropdownMockParent = dropdownOptions[0].closest(
                                                '.valentines-day-dropdown-mock'
                                              );
                                            }
                                            if (!dropdownMockParent) return;

                                            dropdownOptions.forEach(function (opt, optIdx) {
                                              if (optIdx === idx) {
                                                opt.classList.add('active');
                                                const selectedEl = dropdownMockParent.querySelector('[data-selected]');
                                                if (selectedEl) selectedEl.innerText = opt.getAttribute('data-value');
                                              } else {
                                                opt.classList.remove('active');
                                              }
                                            });
                                          });

                                          return;
                                        } else {
                                          tabCt.classList.remove('active');
                                        }
                                      });
                                    } catch (e) {}
                                  };

                                  // 只对“切到第一个 tab”做锁滚（你说只有这个会出问题）
                                  if (idx === 0) lockScrollAndRun(doSwitch);
                                  else doSwitch();
                                });
                              });

                              // 外部点击收起（用 capture 更稳，避免被 stopPropagation 影响）
                              document.addEventListener(
                                'click',
                                function (e) {
                                  if (!hasParent(e.target, dropdown)) closeDropdown();
                                },
                                true
                              );

                              // 键盘ESC收起
                              dropdown.addEventListener('keydown', function (e) {
                                if (e.key === 'Escape') closeDropdown();
                              });
                            });
                          </script>
