<script>
  // 监听页面DOM加载，初始化swiper与tab交互
  {% comment %}
    修复：之前只作用于初始active的tab，切换后没有更新active类导致jQuery选择器始终只对初始active操作。
    方案：每次select切换，都要移除所有valentines-day-product-tab的active类，再切到对应的索引。
    - 这样每次切换都会正常触发显示和隐藏，无论当前active在哪。
  {% endcomment %}

  document.addEventListener('DOMContentLoaded', function () {
    // swiper实例管理，防止重复初始化
    let swiperInstances = [];

    function initAllSwipers() {
      // 销毁旧的swiper实例
      if (swiperInstances && swiperInstances.length > 0) {
        swiperInstances.forEach(function (swiper) {
          if (swiper && typeof swiper.destroy === 'function') {
            swiper.destroy(true, true);
          }
        });
        swiperInstances = [];
      } else {
        swiperInstances = [];
      }
      // 初始化所有当前可见的swiper
      document
        .querySelectorAll('.valentines-day-product-tab-card-mySwiper-{{section.id}}')
        .forEach(function (swiperEl) {
          var swiperInstance = new Swiper(swiperEl, {
            lazy: {
              loadPrevNext: true,
            },
            loop: true,
            autoplay: {
              delay: 3000,
              stopOnLastSlide: false,
              disableOnInteraction: true,
            },
            effect: 'fade',
            fadeEffect: {
              crossFade: true,
            },
            observer: true,
            observeParents: true,
            noSwiping: true,
            slidesPerView: 1,
            spaceBetween: 0,
            pagination: {
              el: swiperEl.closest('.valentines-day-product-tab')
                ? swiperEl
                    .closest('.valentines-day-product-tab')
                    .querySelector('.swiper-pagination-{{section.id}}')
                : '.swiper-pagination-{{section.id}}',
              clickable: true,
            },
          });
          swiperInstances.push(swiperInstance);
        });
    }

    // 首次初始化swiper（页面默认active的tab）
    initAllSwipers();

    // 获取所有valentines-day-product-tab-tabs容器（有多个section时都生效）
    {% comment %} const tabContainers = document.querySelectorAll('.valentines-day-product-tab-tabs');
    if (!tabContainers || tabContainers.length === 0) return;

    tabContainers.forEach(function (tabContainer) {
      // select直接取tabContainer下的select，因为.tabs外面包裹了wrapper
      const select = tabContainer.querySelector('select.valentines-day-product-tab-tab');
      if (!select) return;

      // 取到对应的card和banner盒子，但必须限制到本section范围，否则多个section会串
      // section 元素, .valentines-day-product-tab-tabs总会在某个section的唯一实例下
      const sectionRoot =
        tabContainer.closest('section[id^="valentines-day-product-tab"]') ||
        tabContainer.closest('section');
      // 获取本section下所有tab/main tab卡片和banner
      const cardContainer = sectionRoot
        ? sectionRoot.querySelectorAll('.valentines-day-product-tab')
        : [];
      const bannerBoxes = sectionRoot
        ? sectionRoot.querySelectorAll('.valentines-day-product-tab-banner-box')
        : [];
      // 如果有额外自定义tab元素高亮也一并处理
      const tabs = tabContainer.querySelectorAll('.valentines-day-product-tab-tab');

      // 移动端 select 切换
      select.addEventListener('change', function (e) {
        const idx = select.selectedIndex;
        // 清除所有tab active
        cardContainer.forEach((cardbox, i) => {
          cardbox.classList.toggle('active', i === idx);
        });
        // 切换banner高亮
        if (bannerBoxes && bannerBoxes.length > 0) {
          bannerBoxes.forEach((bannerBox, i) => {
            bannerBox.classList.toggle('active', i === idx);
          });
        }
        // 兼容有没有可单独tab高亮元素
        if (tabs && tabs.length > 0) {
            console.log('tabs', tabs);
          tabs.forEach((t, i) => {
            // 延迟加载 option 操作，提升性能

              // 移除 t 下所有 option 的 selected 属性
            const options = t.querySelectorAll('option');
              // 仅将选中的 option 变为 selected 状态
              options.forEach((option, optIdx) => {
                if (optIdx === idx) {
                    console.log('option', option);
                  option.setAttribute('selected', 'selected');
                } else {
                  option.removeAttribute('selected');
                }
              });
              t.classList.toggle('active', i === idx);

          });
        }

      });


    }); {% endcomment %}
  });
</script>

<script>
  function formatUtcTime(v) {
    let date = new Date(v);
    return (
      date.getFullYear() +
      '/' +
      (date.getMonth() + 1) +
      '/' +
      (date.getDate() < 10 ? '0' + date.getDate() : date.getDate()) +
      ' ' +
      (date.getHours() < 10 ? '0' + date.getHours() : date.getHours()) +
      ':' +
      (date.getMinutes() < 10 ? '0' + date.getMinutes() : date.getMinutes()) +
      ':' +
      (date.getSeconds() < 10 ? '0' + date.getSeconds() : date.getSeconds())
    );
  }
  $(function () {
    if ($('.valentines-day-product-tab-{{ section.id }}  [data-countdown]')) {
      $('.valentines-day-product-tab-{{ section.id }}  [data-countdown]').each(function () {
        const UTCDate = $(this).data('countdown'); // 活动开始倒计时
        const UTCDate_end = $(this).data('countdown-end'); // 活动截止倒计时
        console.log('UTCDate-活动开始倒计时：', UTCDate);
        console.log('UTCDate_end-活动截止倒计时：', UTCDate_end);
        const finalDate = formatUtcTime(UTCDate);
        const finalDate_end = formatUtcTime(UTCDate_end);
        const finalDate1 = finalDate;
        const finalDate2 = finalDate_end ? finalDate_end : null;
        const timestamp = new Date(finalDate1).getTime();
        const timestamp_end = finalDate2 ? new Date(finalDate2).getTime() : null;
        const currentTimestamp = new Date().getTime();
        console.log('活动开始本地时间==', finalDate);
          console.log('活动截止本地时间==', finalDate_end);
        let format;
        const style = $(this).data('countdown-style');
        console.log('活动截止本地时间==', finalDate_end);
        if (timestamp > currentTimestamp){
            $(this)
            .parents('.product-flash-sale-countdown')
            .siblings('.mascot-activities-product-info-button')
            .children('product-card')
            .find('.product-card--add-to-cart-text')
            .text('Coming Soon');
            $(this)
            .parents('.product-flash-sale-countdown')
            .siblings('.mascot-activities-product-info-button')
            .children('product-card')
            .find('.mascot-activities-product-info-button-buy-now')
            .css({'visibility':'visible','background':'#FECF67','cursor':'not-allowed','pointer-events':'none','color':'#000'});
            return;
        }
        $(this)
            .parents('.product-flash-sale-countdown')
            .siblings('.mascot-activities-product-info-button')
            .children('product-card')
            .find('.mascot-activities-product-info-button-buy-now')
            .css({'visibility':'visible'});
        $(this)
          .countdown(finalDate2)
          .on('update.countdown', function (event) {
            if (style == 1) {
              format = '<span>%D</span>:<span>%H</span>:<span>%M</span>:<span>%S</span>';
            } else if (style == 2) {
              {% comment %} format = '<span>%D<span>Days</span></span>:<span>%H<span>Hrs</span></span>:<span>%M<span>Mins</span></span>:<span>%S<span>Secs</span></span>'; {% endcomment %}
              format = '<div class="time-block">' +
                        '<div class="time-value">%D</div>' +
                        '<div class="time-label">Days</div>' +
                      '</div>' +
                      '<div class="time-block">' +
                        '<div class="time-value">%H</div>' +
                        '<div class="time-label">Hrs</div>' +
                      '</div>' +
                      '<div class="time-block">' +
                        '<div class="time-value">%M</div>' +
                        '<div class="time-label">Mins</div>' +
                      '</div>' +
                      '<div class="time-block">' +
                        '<div class="time-value">%S</div>' +
                        '<div class="time-label">Secs</div>' +
                      '</div>';
            }
            $(this).html(event.strftime(format));
          })
          .on('finish.countdown', function (event) {
            {% comment %} $(this)
            .parents('.super-early-bird-countdown-date').css('display', 'none') {% endcomment %}
          });
      });
    }
  });
</script>

