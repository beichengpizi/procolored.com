{% assign product_count = 0 %}
{% for product in section.settings.product_list %}
  {% assign product_count = product_count | plus: 1 %}
{% endfor %}
<style>
    :root {
      /* Font sizes */
      --text-h0-n: 56px;
      --text-h0-sm-n: 48px;
      --text-h1-n: 40px;
      --text-h2-n: 30px;
      --text-h3-n: 26px;
      --text-h4-n: 24px;
      --text-h5-n: 20px;
      --text-h6-n: 16px;
      --text-xs-n: 12px;
      --text-sm-n: 14px;
      --text-base-n: 18px;
      --text-lg-n: 19px;
    }
    .text-xxs {
      font-size: 10px;
      line-height: 1.2;
    }

    .text-xs {
      font-size: var(--text-xs-n);
      line-height: 1.2;
    }

    .text-sm {
      font-size: var(--text-sm-n);
      line-height: 1.2;
    }

    .text-base {
      font-size: var(--text-base-n);
      line-height: 1.2;
    }
    .text-md {
      font-size: var(--text-md-n);
      line-height: 1.2;
    }

    .text-lg {
      font-size: var(--text-lg-n);
      line-height: 1.2;
    }
     /* 重置样式 start*/
     .comparison-table-wrapper-{{section.id}}  .h0 {
      font-size: var(--text-h0-n) ;
      line-height: 1;
      font-family: ItalianPlateNo2Expanded-Bold, sans-serif ;
    }
    .comparison-table-wrapper-{{section.id}}  .h0-sm {
      font-size: var(--text-h0-sm-n) ;
      line-height: 1;
      font-family: ItalianPlateNo2Expanded-Bold, sans-serif ;
    }


    .comparison-table-wrapper-{{section.id}}  h1,
    .comparison-table-wrapper-{{section.id}}  .h1,
    .comparison-table-wrapper-{{section.id}}  .prose h1:not(.h0, .h1, .h2, .h3, .h4, .h5, .h6) {
      font-size: var(--text-h1-n) ;
      line-height: 1.1;
      font-family: ItalianPlateNo2Expanded-Bold, sans-serif ;
    }

    .comparison-table-wrapper-{{section.id}}  h2,
    .comparison-table-wrapper-{{section.id}}  .h2,
    .comparison-table-wrapper-{{section.id}}  .prose h2:not(.h0, .h1, .h2, .h3, .h4, .h5, .h6) {
      font-size: var(--text-h2-n);
      line-height: 1.1;
      font-family: ItalianPlateNo2Expanded-Bold, sans-serif ;
    }

    .comparison-table-wrapper-{{section.id}}  h3,
    .comparison-table-wrapper-{{section.id}}  .h3,
    .comparison-table-wrapper-{{section.id}}  .prose h3:not(.h0, .h1, .h2, .h3, .h4, .h5, .h6) {
      font-size: var(--text-h3-n) ;
      line-height: 1.2;
    }

    .comparison-table-wrapper-{{section.id}}  h4,
    .comparison-table-wrapper-{{section.id}}  .h4,
    .comparison-table-wrapper-{{section.id}}  .prose h4:not(.h0, .h1, .h2, .h3, .h4, .h5, .h6) {
      font-size: var(--text-h4-n) ;
      line-height: 1.2;
    }

    .comparison-table-wrapper-{{section.id}}  h5,
    .comparison-table-wrapper-{{section.id}}  .h5,
    .comparison-table-wrapper-{{section.id}}  .prose h5:not(.h0, .h1, .h2, .h3, .h4, .h5, .h6) {
      font-size: var(--text-h5-n) ;
      line-height: 1.2;
    }

    .comparison-table-wrapper-{{section.id}}  h6,
    .comparison-table-wrapper-{{section.id}}  .h6,
    .comparison-table-wrapper-{{section.id}}  .prose h6:not(.h0, .h1, .h2, .h3, .h4, .h5, .h6) {
      font-size: var(--text-h6-n) ;
      line-height: 1.2;
    }
    /* 重置样式 end*/
    /* 参数表格模块 */
    .comparison-table-wrapper-{{section.id}} {
        background-color: {{section.settings.bgColor}};
        padding: {{ section.settings.padding_top }}px 24px {{ section.settings.padding_bottom }}px 24px;
      }
      .custom-title-wrap{
        {% if section.settings.section_max_width != 'none' %}
            max-width: {{ section.settings.section_max_width }}px;
            margin: 0 auto;
          {% endif %}

    }
  .comparison-table-wrapper-{{section.id}}  .comparison-table-container {
        display: grid;
        grid-template-columns: 1fr;
        grid-template-rows: repeat(9, auto);
        {% if section.settings.section_max_width != 'none' %}
            max-width: {{ section.settings.section_max_width }}px;
            margin: 0 auto;
          {% endif %}

      }
      .comparison-table-wrapper-{{section.id}} .custom-title-wrap{
        text-align: {{section.settings.title_text_alignment}};
      }
     .comparison-table-wrapper-{{section.id}}  .custom-title {
          color: {{section.settings.title_color}};
          margin-bottom: {{section.settings.title_to_content_padding}}px;
        }
        .comparison-table-wrapper-{{section.id}}  .custom-subtitle {
          color: {{section.settings.subtitle_color}};
          margin-bottom: {{section.settings.subtitle_to_content_padding}}px;
        }
  .comparison-table-wrapper-{{section.id}}  .comparison-table-item {
        background-color: #ffffff;
        border-bottom: 1px solid #f5f5f5;
        display: grid;
        grid-template-columns: repeat({{ product_count | plus: 1 }}, 1fr);

      }

  .comparison-table-wrapper-{{section.id}}  .cost-breakdown {
        text-align: {{section.settings.content_text_alignment}};
      }

  .comparison-table-wrapper-{{section.id}}  .printer-image {
        margin-bottom: 15px;
        max-width: 300px;
        width: 100%;
      }



  .comparison-table-wrapper-{{section.id}}  .printer-model,
  .comparison-table-wrapper-{{section.id}}  .printer-size {
        font-size: {{section.settings.spec_value_size}}px;
        color: #ea3f2e;
        text-align: {{section.settings.content_text_alignment}};
        width: 100%;
      }

  .comparison-table-wrapper-{{section.id}}  .printer-title {
        font-size: {{section.settings.spec_label_size}}px;
        margin-bottom: {{section.settings.table_cell_gap}}px;
        text-align: {{section.settings.content_text_alignment}};
        min-height: {{section.settings.product_name_min_height}}px;
        width: 100%;
        font-weight: bold;
      }

  .comparison-table-wrapper-{{section.id}}  .profit-amount {
        font-size: {{section.settings.spec_label_size | plus: 2}}px;;
        font-weight: bold;
      }

  .comparison-table-wrapper-{{section.id}}  .profit-section {
        padding: {{section.settings.table_cell_gap}}px;
        text-align: {{section.settings.content_text_alignment}};
      }

  .comparison-table-wrapper-{{section.id}}  .spec-column-product {
        align-items: {{section.settings.content_text_alignment}};
        display: flex;
        flex-direction: column;
        padding: {{section.settings.table_cell_gap}}px;
        width: 100%;
      }

  .comparison-table-wrapper-{{section.id}}  .spec-label {
        font-weight: bold;
        padding: {{section.settings.table_cell_gap}}px;
        text-align: {{section.settings.content_text_alignment}};
        font-size: {{section.settings.spec_label_size}}px;
      }

  .comparison-table-wrapper-{{section.id}}  .spec-label-column-product,
  .comparison-table-wrapper-{{section.id}}  .spec-column {
        padding: {{section.settings.table_cell_gap}}px;
        text-align: {{section.settings.content_text_alignment}};
        display: flex;
        align-items: center;
        font-size: {{section.settings.spec_value_size}}px;
        justify-content: {{section.settings.content_text_alignment}};
      }
      .comparison-table-wrapper-{{section.id}} .spec-row{
        display: flex;
        padding: 10px;
        flex-direction: column;
        align-items: start;
        font-size: {{section.settings.spec_value_size}}px;
        justify-content: center;
      }

  .comparison-table-wrapper-{{section.id}}  .work-day {
        font-size: {{section.settings.spec_value_size}}px;
      }
  .comparison-table-wrapper-{{section.id}}  .spec-label-mobile{
        display: none;
      }
      .comparison-table-wrapper-{{section.id}} .desktopShow{
        position: relative;
        display: block;
        }
        .comparison-table-wrapper-{{section.id}} .mobileShow{
        position: relative;
        display: none;
        }
      @media screen and (max-width: 768px) {
        .comparison-table-wrapper-{{section.id}}  {

          padding: {{ section.settings.padding_top_mobile }}px 0 {{ section.settings.padding_bottom_mobile }}px 0;
        }
        .comparison-table-wrapper-{{section.id}} .custom-title-wrap{
            text-align: {{section.settings.content_text_alignment_mobile}};
            padding:0 24px;
        }
        .comparison-table-wrapper-{{section.id}}  .comparison-table-container {
            overflow-x: scroll;
            padding:0 24px;


          }
        .comparison-table-wrapper-{{section.id}} .custom-title {
          font-size: {{ section.settings.title_size_mobile }}px;
          margin-bottom: {{section.settings.title_to_content_padding_mobile}}px;
        }
        .comparison-table-wrapper-{{section.id}} .custom-subtitle {
          font-size: {{ section.settings.subtitle_size_mobile }}px ;
          margin-bottom: {{section.settings.subtitle_to_content_padding_mobile}}px;
        }
        .comparison-table-wrapper-{{section.id}}  .spec-label-column-product,
        .comparison-table-wrapper-{{section.id}}  .spec-label {
          display: none;
        }
        .comparison-table-wrapper-{{section.id}}  .spec-column-product {
          padding: 10px;
        }
        .comparison-table-wrapper-{{section.id}}  .spec-label-mobile {
            display: block;
          font-size: 14px;
          font-weight: bold;
          margin-bottom: 2px;
          opacity:0;
        }
        .comparison-table-wrapper-{{section.id}} .spec-label-mobile.first{
          opacity:1;

      }
        .comparison-table-wrapper-{{section.id}}  .printer-title {
          font-size: 14px;
          margin-bottom: 10px;
          min-height: {{section.settings.product_name_min_height_mobile}}px;
        }
        .comparison-table-wrapper-{{section.id}}  .printer-model,
        .comparison-table-wrapper-{{section.id}}  .printer-size {
          font-size: 12px;
        }
        .comparison-table-wrapper-{{section.id}}  .printer-model {
            margin-bottom: 0;

          }
        .comparison-table-wrapper-{{section.id}}  .spec-label-column-product,
        .comparison-table-wrapper-{{section.id}}  .spec-column {
          font-size: 12px;
          padding: 10px;
          flex-direction: column;
          align-items: {{section.settings.content_text_alignment}};
        }
        .comparison-table-wrapper-{{section.id}}  .comparison-table-item {
          grid-template-columns: repeat({{ product_count }}, 150px);
        }
        .comparison-table-wrapper-{{section.id}}  .profit-section {
          padding: 10px;
        }
        .comparison-table-wrapper-{{section.id}}  .profit-amount {
          font-size: 16px;
        }
        .comparison-table-wrapper-{{section.id}}  .work-day {
          font-size: 12px;
        }
        .comparison-table-wrapper-{{section.id}} .desktopShow{
            position: relative;
            display: none;
        }
        .comparison-table-wrapper-{{section.id}} .mobileShow{
            position: relative;
            display: block;
        }
      }
</style>

<!-- 产品利润对比表格模块 -->
<div class="comparison-table-wrapper-{{section.id}}">
  <div class="custom-title-wrap">
    {% if section.settings.title != empty %}
      <div
        class="custom-title wow fadeInUp desktopShow {{ section.settings.title_size }}"
        data-wow-delay=".4s"
      >
        {{ section.settings.title }}
      </div>
      {% if section.settings.title_mobile != empty %}
        <div
          class="custom-title wow fadeInUp mobileShow {{ section.settings.title_size }}"
          data-wow-delay=".4s"
        >
          {{ section.settings.title_mobile }}
        </div>
      {% else %}
        <div
          class="custom-title wow fadeInUp mobileShow {{ section.settings.title_size }}"
          data-wow-delay=".4s"
        >
          {{ section.settings.title }}
        </div>
      {% endif %}
    {% endif %}
    {% if section.settings.subtitle != empty %}
      <div
        class="custom-subtitle wow  fadeInUp desktopShow {{ section.settings.subtitle_size }} custom-font-regular"
        data-wow-delay=".6s"
      >
        {{ section.settings.subtitle }}
      </div>
      {% if section.settings.subtitle_mobile != empty %}
        <div
          class="custom-subtitle wow  fadeInUp mobileShow {{ section.settings.subtitle_size }} custom-font-regular"
          data-wow-delay=".6s"
        >
          {{ section.settings.subtitle_mobile }}
        </div>
      {% else %}
        <div
          class="custom-subtitle wow  fadeInUp mobileShow {{ section.settings.subtitle_size }} custom-font-regular"
          data-wow-delay=".6s"
        >
          {{ section.settings.subtitle }}
        </div>
      {% endif %}
    {% endif %}
  </div>
  {% if section.settings.product_list != blank %}
    <div class="comparison-table-container">
      <!-- 表格参数名称 -->

      <div class="comparison-table-item">
        <div class="spec-label-column-product"></div>
        {% for product_item in section.settings.product_list %}
          {% assign profitable_product_name = product_item.metafields.custom.profitable_product_name
            | default: product_item.title
          %}
          {% assign profitable_product_image = product_item.metafields.custom.profitable_product_image
            | default: product_item.featured_image
          %}

          {% assign profitable_product_model_array = product_item.metafields.custom.profitable_product_model
            | split: '-'
          %}
          {% if profitable_product_model_array.size > 1 %}
            {% assign profitable_product_model = profitable_product_model_array[0] %}
            {% assign profitable_product_size = profitable_product_model_array[1] %}
          {% else %}
            {% assign profitable_product_model = profitable_product_model_array[0] %}
            {% assign profitable_product_size = '' %}
          {% endif %}

          <div class="spec-column-product">
            <div class="printer-title">{{ profitable_product_name }}</div>
            {% assign image = profitable_product_image %}
            {% assign size = 100 | divided_by: product_count | round %}

            <img
              data-sizes="{{ size }}vw"
              width="{{ size }}vw"
              data-sizes="auto"
              data-srcset="
                {%- if image.width >= 375 -%}{{ image | image_url: width: 375 }} 375w,{%- endif -%}
                {%- if image.width >= 750 -%}{{ image | image_url: width: 750 }} 750w,{%- endif -%}
                {%- if image.width >= 1100 -%}{{ image | image_url: width: 1100 }} 1100w,{%- endif -%}
                {%- if image.width >= 1500 -%}{{ image | image_url: width: 1500 }} 1500w,{%- endif -%}
                {%- if image.width >= 1780 -%}{{ image | image_url: width: 1780 }} 1780w,{%- endif -%}
                {%- if image.width >= 2000 -%}{{ image | image_url: width: 2000 }} 2000w,{%- endif -%}
                {%- if image.width >= 3000 -%}{{ image | image_url: width: 3000 }} 3000w,{%- endif -%}
                {%- if image.width >= 3840 -%}{{ image | image_url: width: 3840 }} 3840w,{%- endif -%}

                {{ image | image_url }} {{ image.width }}w
              "
              data-src="{{ image | image_url: width: 1200 }}"
              style="max-width:80%;width:100%;aspect-ratio:{{ image.aspect_ratio }}"
              class="printer-image lazyload"
              alt="{{ profitable_product_name }}"
            >

            <div class="printer-model">{{ profitable_product_model }}</div>
            <div class="printer-size">{{ profitable_product_size }}</div>
          </div>
        {% endfor %}
      </div>

      <div class="comparison-table-item">
        <div class="spec-label">T-shirts</div>
        {% for product_item in section.settings.product_list %}
          {% assign t_shirts = product_item.metafields.custom.t_shirts.value %}
          <div class="spec-column">
            <div class="spec-label-mobile {% if forloop.first %}first{% endif %}">T-shirts</div>
            <div>{{ t_shirts }}</div>
          </div>
        {% endfor %}
      </div>
      <div class="comparison-table-item">
        <div class="spec-label">Suitable Business Size</div>
        {% for product_item in section.settings.product_list %}
          {% assign printer_suitable_business_size = product_item.metafields.custom.printer_suitable_business_size.value %}
          <div class="spec-column">
            <div class="spec-label-mobile {% if forloop.first %}first{% endif %}">Suitable Business Size</div>
            <div>{{ printer_suitable_business_size }}</div>
          </div>
        {% endfor %}
      </div>
      <div class="comparison-table-item">
        <div class="spec-label">Retail Sell Price</div>
        {% for product_item in section.settings.product_list %}
          {% assign retail_sell_price = product_item.metafields.custom.retail_sell_price.value %}
          <div class="spec-column">
            <div class="spec-label-mobile {% if forloop.first %}first{% endif %}">Retail Sell Price</div>
            <div>{{ retail_sell_price }}</div>
          </div>
        {% endfor %}
      </div>

      <div class="comparison-table-item">
        <div class="spec-label">Total Cost per Unit</div>
        {% for product_item in section.settings.product_list %}
          {% assign total_cost_per_unit_array = product_item.metafields.custom.total_cost_per_unit.value %}

          <div class="spec-column">
            <div class="spec-label-mobile {% if forloop.first %}first{% endif %}">Total Cost per Unit</div>
            <div class="cost-breakdown">
              {% for total_cost_per_unit in total_cost_per_unit_array %}
                {% if forloop.last %}
                  {{ total_cost_per_unit }}
                {% else %}
                  {{ total_cost_per_unit }}
                  <br>
                {% endif %}
              {% endfor %}
            </div>
          </div>
        {% endfor %}
      </div>
      <div class="comparison-table-item">
        <div class="spec-label">
          Equipment Cost <br>
          (Profit in 6 months)
        </div>
        {% for product_item in section.settings.product_list %}
          {% assign equipment_cost_array = product_item.metafields.custom.equipment_cost.value %}
          <div class="spec-row">
            <div class="spec-label-mobile {% if forloop.first %}first{% endif %}">
              Equipment Cost (Profit in 6 months)
            </div>

            <div>{{ equipment_cost_array[0] }}</div>
            <div>{{ equipment_cost_array[1] }}</div>
          </div>
        {% endfor %}
      </div>
      <div class="comparison-table-item">
        <div class="spec-label">Average Waste</div>
        {% for product_item in section.settings.product_list %}
          {% assign average_waste = product_item.metafields.custom.average_waste.value %}
          <div class="spec-column">
            <div class="spec-label-mobile {% if forloop.first %}first{% endif %}">Average Waste</div>
            <div>{{ average_waste }}</div>
          </div>
        {% endfor %}
      </div>

      <div class="comparison-table-item">
        <div class="spec-label">Unit per Hour</div>
        {% for product_item in section.settings.product_list %}
          {% assign unit_per_hour = product_item.metafields.custom.unit_per_hour.value %}
          <div class="spec-column">
            <div class="spec-label-mobile {% if forloop.first %}first{% endif %}">Unit per Hour</div>
            <div>{{ unit_per_hour }}</div>
          </div>
        {% endfor %}
      </div>

      <div class="comparison-table-item">
        <div class="spec-label">Profit per Day</div>
        {% for product_item in section.settings.product_list %}
          {% assign profit_per_day_array = product_item.metafields.custom.profit_per_day.value %}
          <div class="profit-section">
            <div class="spec-label-mobile {% if forloop.first %}first{% endif %}">Profit per Day</div>

            <div class="profit-amount">{{ profit_per_day_array[0] }}</div>
            <div class="work-day">{{ profit_per_day_array[1] }}</div>
          </div>
        {% endfor %}
      </div>
    </div>
  {% endif %}
</div>

{% schema %}
{
  "name": "comparison table",
  "tag": "section",
  "settings": [
    {
      "type": "select",
      "id": "section_max_width",
      "label": "模块最大宽度",
      "options": [
        {
          "value": "none",
          "label": "全屏"
        },
        {
          "value": "1200",
          "label": "1200px"
        },
        {
          "value": "980",
          "label": "980px"
        }
      ],
      "default": "980"
    },
    {
      "type": "product_list",
      "id": "product_list",
      "label": "产品列表",
      "limit": 5
    },
    {
      "type": "html",
      "id": "title",
      "label": "大标题",
      "default": "Profitable Printing Solutions"
    },
    {
      "type": "html",
      "id": "title_mobile",
      "label": "大标题（移动端）"
    },

    {
      "type": "html",
      "id": "subtitle",
      "label": "小标题",
      "default": "With three models to choose from, we have the perfect printer for every scale of operation. Choose the right model for your needs and start creating stunning custom designs today!"
    },
    {
      "type": "html",
      "id": "subtitle_mobile",
      "label": "小标题（移动端）"
    },
    {
      "type": "select",
      "id": "title_text_alignment",
      "label": "大标题及小标题文本对齐方式",
      "options": [
        {
          "value": "start",
          "label": "左对齐"
        },
        {
          "value": "center",
          "label": "居中对齐"
        },
        {
          "value": "end",
          "label": "右对齐"
        }
      ],
      "default": "center"
    },
    {
      "type": "select",
      "id": "content_text_alignment_mobile",
      "label": "大标题及小标题文本对齐方式(移动端)",
      "options": [
        {
          "value": "start",
          "label": "左对齐"
        },
        {
          "value": "center",
          "label": "居中对齐"
        },
        {
          "value": "end",
          "label": "右对齐"
        }
      ],
      "default": "start"
    },
    {
      "type": "select",
      "id": "content_text_alignment",
      "label": "文本内容对齐方式",
      "options": [
        {
          "value": "start",
          "label": "左对齐"
        },
        {
          "value": "center",
          "label": "居中对齐"
        },
        {
          "value": "end",
          "label": "右对齐"
        }
      ],
      "default": "start"
    },

    {
      "type": "header",
      "content": "字体大小"
    },
    {
      "type": "select",
      "id": "title_size",
      "label": "大标题字体大小",
      "options": [
        {
          "value": "h0",
          "label": "H0"
        },
        {
          "value": "h0-sm",
          "label": "H0-sm"
        },
        {
          "value": "h1",
          "label": "H1"
        },
        {
          "value": "h2",
          "label": "H2"
        },
        {
          "value": "h3",
          "label": "H3"
        },
        {
          "value": "h4",
          "label": "H4"
        },
        {
          "value": "h5",
          "label": "H5"
        },
        {
          "value": "h6",
          "label": "H6"
        }
      ],
      "default": "h0-sm"
    },
    {
      "type": "range",
      "id": "title_size_mobile",
      "min": 10,
      "max": 100,
      "step": 1,
      "unit": "px",
      "label": "大标题字体大小（移动端）",
      "default": 22
    },
    {
