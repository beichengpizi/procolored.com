<style>
    :root {
      /* Font sizes */
      --text-h0-n: 56px;
      --text-h1-n: 40px;
      --text-h2-n: 30px;
      --text-h3-n: 26px;
      --text-h4-n: 24px;
      --text-h5-n: 20px;
      --text-h6-n: 16px;
      --text-xs-n: 12px;
      --text-sm-n: 14px;
      --text-base-n: 18px;
      --text-lg-n: 19px;
    }
    .text-xxs {
      font-size: 10px;
      line-height: 1.3;
    }

    .text-xs {
      font-size: var(--text-xs-n);
      line-height: 1.3;
    }

    .text-sm {
      font-size: var(--text-sm-n);
      line-height: 1.3;
    }

    .text-base {
      font-size: var(--text-base-n);
      line-height: 1.3;
    }
    .text-md {
      font-size: var(--text-md-n);
      line-height: 1.2;
    }

    .text-lg {
      font-size: var(--text-lg-n);
      line-height: 1.2;
    }
     /* 重置样式 start*/
     #detail-section2-{{section.id}}  .h0 {
      font-size: var(--text-h0-n) ;
      line-height: 1;
     
    }

    #detail-section2-{{section.id}}  h1,
    #detail-section2-{{section.id}}  .h1,
    #detail-section2-{{section.id}}  .prose h1:not(.h0, .h1, .h2, .h3, .h4, .h5, .h6) {
      font-size: var(--text-h1-n) ;
      line-height: 1.1;
     
    }

    #detail-section2-{{section.id}}  h2,
    #detail-section2-{{section.id}}  .h2,
    #detail-section2-{{section.id}}  .prose h2:not(.h0, .h1, .h2, .h3, .h4, .h5, .h6) {
      font-size: var(--text-h2-n);
      line-height: 1.1;
     
    }

    #detail-section2-{{section.id}}  h3,
    #detail-section2-{{section.id}}  .h3,
    #detail-section2-{{section.id}}  .prose h3:not(.h0, .h1, .h2, .h3, .h4, .h5, .h6) {
      font-size: var(--text-h3-n) ;
      line-height: 1.2;
    }

    #detail-section2-{{section.id}}  h4,
    #detail-section2-{{section.id}}  .h4,
    #detail-section2-{{section.id}}  .prose h4:not(.h0, .h1, .h2, .h3, .h4, .h5, .h6) {
      font-size: var(--text-h4-n) ;
      line-height: 1.3;
    }

    #detail-section2-{{section.id}}  h5,
    #detail-section2-{{section.id}}  .h5,
    #detail-section2-{{section.id}}  .prose h5:not(.h0, .h1, .h2, .h3, .h4, .h5, .h6) {
      font-size: var(--text-h5-n) ;
      line-height: 1.4;
    }

    #detail-section2-{{section.id}}  h6,
    #detail-section2-{{section.id}}  .h6,
    #detail-section2-{{section.id}}  .prose h6:not(.h0, .h1, .h2, .h3, .h4, .h5, .h6) {
      font-size: var(--text-h6-n) ;
      line-height: 1.4;
    }
    /* 重置样式 end*/
    /* 图片居上文字居下模块  移动端轮播切换 */
    #detail-section2-{{section.id}} {
      width: 100%;
      background-color: {{section.settings.bgColor}};
      background-image: {{ section.settings.background_gradient_color }};
      padding-top:{{ section.settings.padding_top }}px;
      padding-bottom:calc({{ section.settings.padding_bottom }}px + {{ section.settings.card_container_gap }}px);

    }
    #detail-section2-{{section.id}} .container{
      {% if section.settings.section_max_width != 'none' %}
        max-width: {{ section.settings.section_max_width }}px;
        margin: 0 auto;
      {% endif %}
      padding-right: 0;
      padding-left: 0;
    }
    #detail-section2-{{section.id}}  .custom-product-image-content{
      text-align: {{section.settings.title_text_alignment}};
    }
    #detail-section2-{{section.id}}  .content-bottom-text-wrap{
      text-align: {{ section.settings.content_text_alignment }};
      {% if section.settings.is_content_on_top %}

      padding: 0 {{ section.settings.content_padding_left_and_right }}px;
      {% else %}
      padding:0 {{ section.settings.content_padding_left_and_right }}px 0 {{ section.settings.content_padding_left_and_right }}px;
      {% endif %}
      {% if section.settings.content_in_image %}
        position: absolute;
        bottom: 0;
        left: 0;



        {% endif %}
    }
    #detail-section2-{{section.id}} .custom1-img-wrap  .video-link{
      position:relative;
      display:block;

    }
    #detail-section2-{{section.id}} .custom1-img-wrap  .video-link .czvdo-play-icon-svg{
      position:absolute;
      top:50%;
      left:50%;
      transform:translate(-50%,-50%);
      width: {{section.settings.paly_button_svg_width}}px;
      height: {{section.settings.paly_button_svg_width}}px;
      color: rgba(255, 255, 255, 0.50);

    }
    #detail-section2-{{section.id}}  .custom-heading-3 {
      color: {{section.settings.title_color}};
      margin-bottom: {{section.settings.title_to_content_padding}}px;
    }
    #detail-section2-{{section.id}}  .custom-heading-4 {
      color: {{section.settings.subtitle_color}};
      margin-bottom: {{section.settings.subtitle_to_content_padding}}px;
    }
    #detail-section2-{{section.id}} .content-bottom-text-wrap .custom-heading-3{
      min-height: {{section.settings.heading_min_height}}px;
      color:{{ section.settings.heading_color }};
      line-height: 1.2;
      margin-top: {{ section.settings.heading_margin_top }}px;
      margin-bottom: {{ section.settings.heading_margin_bottom }}px;

    }
    #detail-section2-{{section.id}} .content-bottom-text-wrap .custom-heading-4{
      min-height: {{ section.settings.subheading_min_height }}px;
      color:{{ section.settings.subheading_color }};
      margin-bottom: {{ section.settings.subheading_margin_bottom }}px;

    }
    #detail-section2-{{section.id}} .content-bottom-text-wrap .custom-decs{
      min-height: {{ section.settings.content_min_height }}px;
      color:{{ section.settings.content_color }};
      margin-bottom: {{ section.settings.content_margin_bottom }}px;
    }
    #detail-section2-{{section.id}} .content-bottom-text-wrap .serial-number{
      color:{{ section.settings.content_serial_number_color }};
      background-color:{{ section.settings.content_serial_number_bgColor }};
      border-radius: {{ section.settings.content_serial_number_border_radius }}%;
      position: absolute;
      left: 0;
      top: -{{ section.settings.content_serial_number_top }}px;
      z-index: 999;
      text-align: center;
      line-height: 1;
      padding: {{ section.settings.content_serial_number_with_padding }}px;
      aspect-ratio: 1 / 1;
      display: flex;
      align-items: center;
      justify-content: center;
      min-width: 50px;
      min-height: 50px;
      margin-bottom: 0;

    }
    #detail-section2-{{section.id}} .author-info{
      display: flex;
      align-items: center;
      column-gap: 10px;
      margin-bottom: {{ section.settings.author_info_margin_bottom }}px;

    }
    #detail-section2-{{section.id}} .author-info .author-img{
      width:48px;
    }
    #detail-section2-{{section.id}} .author-info .author-img img{
      width: 100%;
      max-width: 100%;
    }
    #detail-section2-{{section.id}} .author-info .author-details{
      display: flex;
      flex-direction: column;
      row-gap: 6px;
      line-height: 1.2;


    }
    #detail-section2-{{section.id}} .author-info .author-details .author-name{
      font-size: 16px;
      font-weight: bold;
      margin-bottom: 2px;
    }
    #detail-section2-{{section.id}} .author-info .author-details .product-name{
      font-size: 12px;
      color: #DA291C;
    }



    #detail-section2-{{section.id}} .custom1-row {
      display: flex;
      justify-content: flex-start;
      align-items: baseline;
      flex-flow: row wrap;
      gap: {{ section.settings.card_container_gap }}px; /* 上下间距 */
      margin-bottom: -{{ section.settings.card_container_gap }}px; /* 负边距以抵消底部间距 */
      {% comment %} margin-right: -{{ section.settings.card_container_gap }}px; {% endcomment %}
    }

    #detail-section2-{{section.id}} .reverse {
      flex-direction: row-reverse;
    }

    #detail-section2-{{section.id}} .custom1-column {
      flex-basis: calc((100% - {{ section.settings.display_number_per_line | minus: 1 }} * {{ section.settings.card_container_gap }}px) / {{ section.settings.display_number_per_line | plus: 0 }});
      box-sizing: border-box;
    }

    #detail-section2-{{section.id}} .custom1-media {
      position: relative;
      border-radius: {{ section.settings.content_border_radius }}px;
      display: flex;

      {% if section.settings.is_content_on_top %}
        flex-direction: column-reverse;

        {% if section.settings.has_content_border %}
          border: 2px {{section.settings.content_border_color}} solid;
        {% endif %}

        padding: {{section.settings.content_padding}}px;
      {% else %}
        flex-direction: column;


      {% endif %}
      {% comment %} overflow: hidden; {% endcomment %}
      row-gap: {{ section.settings.content_top_padding }}px;
      background-color: {{ section.settings.content_bg_color }};
      {% unless section.settings.is_show_serial_number %}
        overflow: hidden;
      {% endunless %}



    }

    #detail-section2-{{section.id}} .custom1-media img {
      width: 100%;
      height: 100%;
      border-radius: {{ section.settings.img_border_radius }}px;
      {% if section.settings.is_content_on_top %}
        border-bottom-left-radius:{{section.settings.content_border_radius}}px;
        border-bottom-right-radius:{{section.settings.content_border_radius}}px;

      {% endif %}
    }



    #detail-section2-{{section.id}} .showSwiper {
      display: none;
    }

    #detail-section2-{{section.id}} .custom1-img-wrap {
      position: relative;
      margin: 0 auto;
    }

    #detail-section2-{{section.id}} .adaption {
      padding-top: 0;
    }

    #detail-section2-{{section.id}} .custom1-media .adaption-img {
      position: relative;
      object-fit: fill;
      object-position: initial;
      vertical-align: baseline;
    }

    #detail-section2-{{section.id}} .slide-content .adaption-img {
      position: relative;
      object-fit: fill;
      object-position: initial;
      vertical-align: baseline;
    }
    #detail-section2-{{section.id}} .swiper-wrapper {
      height: auto !important;
    }



    /* 自定义分页器样式 */
    #detail-section2-{{section.id}} .swiper-pagination-bullet {
      width: 20px;
      height: 5px;
      background: rgba(0, 0, 0, 0.4);
      cursor: pointer;
      min-height: initial;
      margin: 0 5px;
      border-radius: 0;
    }

    #detail-section2-{{section.id}} .swiper-pagination-bullet-active {
      background: #000;
    }

    #detail-section2-{{section.id}} .swiper-container-horizontal > .swiper-pagination-bullets {
      bottom: 0px;
    }
    #detail-section2-{{section.id}} .desktopShow{
      position: relative;
      display: block;
  }
  #detail-section2-{{section.id}} .mobileShow{
      position: relative;
      display: none;
  }
    /* 台式机、大屏幕 */
    @media screen and (min-width: 1024px) {
    }

    /*小屏幕、笔记本电脑 */
    @media screen and (min-width: 769px) and (max-width: 1024px) {
      #detail-section2-{{section.id}} {
        /* padding: 0 15px; */
        width: auto;
      }

      #detail-section2-{{section.id}} .custom1-column {
        width: 48%;
      }

      #detail-section2-{{section.id}} .custom1-img-wrap {
        height: auto;
      }
    }

    /* iPad、平板电脑 */
    @media screen and (min-width: 481px) and (max-width: 768px) {
      #detail-section2-{{section.id}} {
        /* padding: 0 15px; */
        width: auto;
      }
      .swiper-template-detail-section2-{{section.id}}{
        {% if section.settings.is_content_on_top %}
          padding-bottom:50px;
        {% endif %}
      }
      #detail-section2-{{section.id}} .custom-heading {
        font-size: 26px;
      }

      #detail-section2-{{section.id}} .custom1-row {
        display: flex;
        flex-direction: column;
        justify-content: space-between;
        align-items: center;

      }

      #detail-section2-{{section.id}} .custom1-column {
        width: 100%;

      }

      #detail-section2-{{section.id}} .custom1-img-wrap {
        height: auto;
      }
    }

    /* 移动设备 */
    @media screen and (min-width: 320px) and (max-width: 480px) {
      #detail-section2-{{section.id}} .container{
        {% if section.settings.is_swiper_mobile %}
          overflow: hidden;
        {% endif %}

          padding-left: {{ section.settings.padding_left_and_right_mobile }}px;
          padding-right: {{ section.settings.padding_left_and_right_mobile }}px;



      }
      #detail-section2-{{section.id}}  .custom-product-image-content{

        text-align: {{section.settings.content_text_alignment_mobile}};
      }
      #detail-section2-{{section.id}}  .custom-heading-3 {
        font-size: {{ section.settings.title_size_mobile }}px;
        margin-bottom: {{section.settings.title_to_content_padding_mobile}}px;
      }
      #detail-section2-{{section.id}}  .custom-heading-4 {
        font-size: {{ section.settings.subtitle_size_mobile }}px ;
        margin-bottom: {{section.settings.subtitle_to_content_padding_mobile}}px;
      }
      #detail-section2-{{section.id}} .custom1-media {
        border-radius: {{ section.settings.content_border_radius_mobile }}px;
        display: flex;
        {% if section.settings.is_content_on_top %}
          flex-direction: column-reverse;
          padding: {{section.settings.content_padding_mobile}}px;
        {% else %}
          flex-direction: column;
        {% endif %}
        row-gap: {{ section.settings.content_top_padding_mobile }}px;



      }
      #detail-section2-{{section.id}} .custom1-media img {
        width: 100%;
        height: 100%;
        border-radius: {{ section.settings.img_border_radius_mobile }}px;

      }
      #detail-section2-{{section.id}} .custom1-img-wrap  .video-link .czvdo-play-icon-svg{

        width: {{section.settings.paly_button_svg_width_mobile}}px;
        height: {{section.settings.paly_button_svg_width_mobile}}px;


      }
      #detail-section2-{{section.id}} .showSwiper {
        {% if section.settings.is_swiper_mobile %}
          display: block;
        {% else %}

        {% endif %}

        position: relative;
      }

      .swiper-template-detail-section2-{{section.id}}{
        {% if section.settings.is_content_on_top %}
          padding-bottom:50px;
        {% endif %}
        {% if section.settings.is_swiper_pagination_mobile %}
          padding-bottom:40px;
        {% endif %}
        position: relative;


      }
      #detail-section2-{{section.id}} .swiper {
        position: relative;
      }

      #detail-section2-{{section.id}} {
        padding-top:{{ section.settings.padding_top_mobile }}px;
        padding-bottom:calc({{ section.settings.padding_bottom_mobile }}px + {{ section.settings.card_container_gap_mobile }}px);
        width: auto;

      }


      #detail-section2-{{section.id}} .content-bottom-text-wrap .custom-heading-3{
        font-size: {{ section.settings.content_heading_size_mobile }}px;
        min-height: {{ section.settings.heading_min_height_mobile }}px;
        margin-top: {{ section.settings.heading_margin_top_mobile }}px;
        margin-bottom: {{ section.settings.heading_margin_bottom_mobile }}px;
      }
      #detail-section2-{{section.id}} .content-bottom-text-wrap .custom-heading-4{
        font-size: {{ section.settings.content_subheading_size_mobile }}px;
        min-height: {{ section.settings.subheading_min_height_mobile }}px;
        margin-bottom: {{ section.settings.subheading_margin_bottom_mobile }}px;
      }
      #detail-section2-{{section.id}} .content-bottom-text-wrap .custom-decs{
        font-size: {{ section.settings.content_content_size_mobile }}px;
        min-height: {{ section.settings.content_min_height_mobile }}px;
        margin-bottom: {{ section.settings.content_margin_bottom_mobile }}px;
      }
      #detail-section2-{{section.id}} .content-bottom-text-wrap .serial-number{
        font-size: {{ section.settings.content_serial_number_size_mobile }}px;


        top: -{{ section.settings.content_serial_number_top_mobile }}px;

        padding: {{ section.settings.content_serial_number_with_padding_mobile }}px;

        min-width: 30px;
        min-height: 30px;

      }
      #detail-section2-{{section.id}}  .content-bottom-text-wrap{
        text-align: {{ section.settings.content_text_alignment }};
        {% if section.settings.is_content_on_top %}

          padding: 0 {{ section.settings.content_padding_left_and_right_mobile }}px;
        {% else %}
          padding:0 {{ section.settings.content_padding_left_and_right_mobile }}px 0 {{ section.settings.content_padding_left_and_right_mobile }}px;
        {% endif %}
      }
      #detail-section2-{{section.id}} .custom1-row {

        {% if section.settings.is_swiper_mobile %}
          display: none !important;

        {% else %}
          padding: 0;
          {% if section.blocks.size > 1 %}
            gap: {{ section.settings.card_container_gap_mobile }}px;
            margin-bottom: -{{ section.settings.card_container_gap_mobile }}px; /* 负边距以抵消底部间距 */
            margin-right: -{{ section.settings.card_container_gap_mobile }}px;
      
          {% else %}
            gap: 0;
          {% endif %}
        {% endif %}


      }
      #detail-section2-{{section.id}} .author-info{

        margin-bottom: {{ section.settings.author_info_margin_bottom_mobile }}px;

      }
      #detail-section2-{{section.id}} .author-info .author-details{
        row-gap: 0;
      }
      #detail-section2-{{section.id}} .author-info .author-img{
        width:40px;
      }
      #detail-section2-{{section.id}} .author-info .author-details .product-name{
        font-size: 14px;
      }
      #detail-section2-{{section.id}} .custom1-column {
        {% if section.settings.is_swiper_mobile %}
          width: 100%;

        {% else %}
          flex-basis: calc((100% - {{ section.settings.display_number_per_line_flex_mobile | plus: 0 }} *  {{ section.settings.card_container_gap_mobile }}px) / {{ section.settings.display_number_per_line_flex_mobile | plus: 0 }});
          box-sizing: border-box;
          border-radius: 10px;
        {% endif %}
        {% if section.settings.is_show_serial_number %}
        margin-bottom: {{ section.settings.content_serial_number_with_padding_mobile }}px;
      {% endif %}



      }

      /* 移动端轮播样式 */
      #detail-section2-{{section.id}} .slide-content {
        position: relative;

        border-radius: 10px;
        margin: 0 auto;
        width: 100%;
      }

      #detail-section2-{{section.id}} .slide-content img {
        position: absolute;
        left: 0;
        top: 0;
        width: 100%;
        height: 100%;
    
        object-fit: cover;
        object-position: center center;
        max-width: 100%;
        border: 0;
        vertical-align: middle;
      }

      #detail-section2-{{section.id}} .slide-content-dec {
        text-align: center;
        font-size: 12px;
        line-height: 1.5;
        color: {{section.settings.desc_color}};
        margin: 0 15px 20px;
      }

      /* 自定义前进按钮样式 */


      #detail-section2-{{section.id}} .swiper-button-next ,
      #detail-section2-{{section.id}} .swiper-button-prev  {
      position: relative;
      top: auto;
      width: 25px;
      height: 25px;
      margin-top: 0;
      background-color: rgba(102, 102, 102, 0.25);
      border-radius: 50%;
      display: flex ;
      align-items: center;
      justify-content: center;
      transform: none;
      left:auto;
      right:auto;



      }
      #detail-section2-{{section.id}} .swiper-button-next svg,
      #detail-section2-{{section.id}} .swiper-button-prev svg {
          fill: #000;
          width: 15px;
          height: 15px;

      }

      #detail-section2-{{section.id}} .swiper-button-wrap{
        width: 100%;
        position: absolute;


        display: flex;

        column-gap: 20px;
        {%- if section.settings.enable_slider_center

        -%}
        bottom: 50%;

        justify-content: space-between;
        transform: translate(0, 50%);
        z-index: 2;
      {%- else -%}
        bottom: 0;
        justify-content: end;
      {%- endif %}
      }
      #detail-section2-{{section.id}} .desktopShow{
        position: relative;
        display: none;
    }
    #detail-section2-{{section.id}} .mobileShow{
        position: relative;
        display: block;
    }
    }
</style>

<!-- 图片居上文字居下模块  移动端轮播切换 -->
<div id="detail-section2-{{section.id}}">
  <div class="container">
    <div class="custom-product-image-content">
      {% if section.settings.title != empty %}
        <div
          class="custom-heading-3  desktopShow {{ section.settings.title_size }} {{ section.settings.title_font_family }}"
          
        >
          {{ section.settings.title }}
        </div>
        {% if section.settings.title_mobile != empty %}
          <div
            class="custom-heading-3  mobileShow {{ section.settings.title_size }} {{ section.settings.title_font_family }}"
            
          >
            {{ section.settings.title_mobile }}
          </div>
        {% else %}
          <div
            class="custom-heading-3  mobileShow {{ section.settings.title_size }} {{ section.settings.title_font_family }}"
            
          >
            {{ section.settings.title }}
          </div>
        {% endif %}
      {% endif %}
      {% if section.settings.subtitle != empty %}
        <div
          class="custom-heading-4   {{ section.settings.subtitle_size }} {{ section.settings.subtitle_font_family }}"
          
        >
          {{ section.settings.subtitle }}
        </div>
      {% endif %}
    </div>

    <div class="showSwiper">
      <div class="swiper-template-detail-section2-{{section.id}}">
        <div class="swiper-wrapper">
          {%- for block in section.blocks -%}
            {% case block.type %}
            {%- when 'materialTextUD' -%}
            {%- if block.settings.image_mobile -%}
              {% assign image = block.settings.image_mobile %}
              {% assign size = 100 | divided_by: section.settings.display_number_per_line_mobile | round %}
              <div class="swiper-slide">
                <div class="custom1-media">
                  <div
                    class="slide-content  adaption"
                    style="aspect-ratio:{{ block.settings.image_mobile.aspect_ratio }};width: 100%;"
                  >
                    <img
                      width="{{ size }}vw"
                      data-sizes="{{ size }}vw"
                      data-srcset="
                        {%- if image.width >= 375 -%}{{ image | image_url: width: 375 }} 375w,{%- endif -%}
                        {%- if image.width >= 550 -%}{{ image | image_url: width: 550 }} 550w,{%- endif -%}
                        {%- if image.width >= 750 -%}{{ image | image_url: width: 750 }} 750w,{%- endif -%}
                        {%- if image.width >= 1100 -%}{{ image | image_url: width: 1100 }} 1100w,{%- endif -%}
                        {%- if image.width >= 1500 -%}{{ image | image_url: width: 1500 }} 1500w,{%- endif -%}
