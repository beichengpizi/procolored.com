<script>
  $(document).ready(function () {
    const is_swiper_autoplay = {{section.settings.is_swiper_autoplay}};
    const display_number_per_line = {{section.settings.display_number_per_line | plus: 0 }};
    const display_number_per_line_mobile = {{section.settings.display_number_per_line_mobile | plus: 0 }};
    const swiper_card_container_gap = {{section.settings.swiper_card_container_gap | plus: 0 }};
    const swiper_card_container_gap_mobile = {{section.settings.swiper_card_container_gap_mobile | plus: 0 }};
    new Swiper('.swiper-template-detail-section2-{{section.id}}', {
      lazy: {
        loadPrevNext: true,
      },
      observer: true, //开启动态检查器，监测swiper和slide
      noSwiping: true,
      slidesPerView: display_number_per_line,
      spaceBetween: swiper_card_container_gap,
      on:{
        init: function (swiper) {
          if (is_swiper_autoplay) {
            // 禁用自动播放
            this.autoplay.start()
          } else {
            this.autoplay.stop()
          }

        },
      },
      navigation: {
        nextEl: '.swiper-button-next-{{section.id}}',
        prevEl: '.swiper-button-prev-{{section.id}}',
      },
      pagination: {
        el: '.swiper-pagination-{{ section.id }}',
        clickable: true,
      },
      breakpoints: {
        //当宽度小于等于768时显示
        768: {
          spaceBetween: swiper_card_container_gap_mobile,
          slidesPerView: display_number_per_line_mobile,
        },
      },
    });
  });
</script>
