<style>
    :root {
      /* Font sizes */
      --text-h0-n: 56px;
      --text-h0-sm-n: 48px;
      --text-h1-n: 40px;
      --text-h2-n: 30px;
      --text-h3-n: 26px;
      --text-h4-n: 24px;
      --text-h5-n: 20px;
      --text-h6-n: 16px;
      --text-xs-n: 12px;
      --text-sm-n: 14px;
      --text-base-n: 18px;
      --text-lg-n: 19px;
    }
    .text-xxs {
      font-size: 10px;
      line-height: 1.3;
    }

    .text-xs {
      font-size: var(--text-xs-n);
      line-height: 1.3;
    }

    .text-sm {
      font-size: var(--text-sm-n);
      line-height: 1.3;
    }

    .text-base {
      font-size: var(--text-base-n);
      line-height: 1.3;
    }
    .text-md {
      font-size: var(--text-md-n);
      line-height: 1.2;
    }

    .text-lg {
      font-size: var(--text-lg-n);
      line-height: 1.2;
    }
     /* 重置样式 start*/
     #detail-section2-{{section.id}}  .h0 {
      font-size: var(--text-h0-n) ;
      line-height: 1;

    }
    #detail-section2-{{section.id}}  .h0-sm {
      font-size: var(--text-h0-sm-n) ;
      line-height: 1;

    }


    #detail-section2-{{section.id}}  h1,
    #detail-section2-{{section.id}}  .h1,
    #detail-section2-{{section.id}}  .prose h1:not(.h0, .h1, .h2, .h3, .h4, .h5, .h6) {
      font-size: var(--text-h1-n) ;
      line-height: 1.1;

    }

    #detail-section2-{{section.id}}  h2,
    #detail-section2-{{section.id}}  .h2,
    #detail-section2-{{section.id}}  .prose h2:not(.h0, .h1, .h2, .h3, .h4, .h5, .h6) {
      font-size: var(--text-h2-n);
      line-height: 1.1;

    }

    #detail-section2-{{section.id}}  h3,
    #detail-section2-{{section.id}}  .h3,
    #detail-section2-{{section.id}}  .prose h3:not(.h0, .h1, .h2, .h3, .h4, .h5, .h6) {
      font-size: var(--text-h3-n) ;
      line-height: 1.2;
    }

    #detail-section2-{{section.id}}  h4,
    #detail-section2-{{section.id}}  .h4,
    #detail-section2-{{section.id}}  .prose h4:not(.h0, .h1, .h2, .h3, .h4, .h5, .h6) {
      font-size: var(--text-h4-n) ;
      line-height: 1.2;
    }

    #detail-section2-{{section.id}}  h5,
    #detail-section2-{{section.id}}  .h5,
    #detail-section2-{{section.id}}  .prose h5:not(.h0, .h1, .h2, .h3, .h4, .h5, .h6) {
      font-size: var(--text-h5-n) ;
      line-height: 1.2;
    }

    #detail-section2-{{section.id}}  h6,
    #detail-section2-{{section.id}}  .h6,
    #detail-section2-{{section.id}}  .prose h6:not(.h0, .h1, .h2, .h3, .h4, .h5, .h6) {
      font-size: var(--text-h6-n) ;
      line-height: 1.2;
    }
    /* 重置样式 end*/
    /* 参数表格模块 */
    #detail-section2-{{section.id}} {
      width: 100%;
      background-color: {{section.settings.bgColor}};
      padding-top:{{ section.settings.padding_top }}px;
      padding-bottom:{{ section.settings.padding_bottom }}px;
      padding-left: {{ section.settings.padding_left_and_right }}px;
      padding-right: {{ section.settings.padding_left_and_right }}px;

    }
    #detail-section2-{{section.id}} .container{
      max-width: 1200px;
      padding-right: 0;
      padding-left: 0;
      margin: 0 auto;
    }
    #detail-section2-{{section.id}} .custom-title-wrap{
    text-align: {{section.settings.title_text_alignment}};
  }
    #detail-section2-{{section.id}}  .custom-title {
      color: {{section.settings.title_color}};
      margin-bottom: {{section.settings.title_to_content_padding}}px;
    }
    #detail-section2-{{section.id}}  .custom-subtitle {
      color: {{section.settings.subtitle_color}};
      margin-bottom: {{section.settings.subtitle_to_content_padding}}px;
    }

    #detail-section2-{{section.id}}  .table-wrap {
      display: flex;
      box-sizing: border-box;
      flex-wrap: wrap;
      justify-content: start;
      gap: {{section.settings.table_gap}}px;
      text-align: {{section.settings.content_text_alignment}};
      background: {{section.settings.table_bgColor}};
      overflow: hidden;
      border-radius: {{section.settings.table_border_radius}}px;
    }
    {% assign display_number_per_line = 1.0 | divided_by:section.settings.display_number_per_line | times:100| round:2 %}
    #detail-section2-{{section.id}}  .table-wrap .table-item {
      display: flex;
      overflow: hidden;
      gap: {{section.settings.table_gap}}px;
      flex: 0 0 calc({{ display_number_per_line }}% - {{section.settings.table_gap |divided_by:2 | times:section.settings.display_number_per_line |divided_by:2}}px);
    }
    #detail-section2-{{section.id}}  .table-wrap .table-item .table-item-title,#detail-section2-{{section.id}} .table-wrap .table-item .table-item-content{
      padding-right: {{section.settings.content_right_padding}}px;
      padding-left: {{section.settings.content_left_padding}}px;
      padding-top: {{section.settings.content_top_and_bottom_padding}}px;
      padding-bottom: {{section.settings.content_top_and_bottom_padding}}px;
  }

  #detail-section2-{{section.id}} .table-wrap .table-item .table-item-title {
      flex: 0 0 calc({{ section.settings.width_of_title }}% - {{section.settings.table_gap | times:section.settings.display_number_per_line}}px);
      color: {{section.settings.heading_color}};
      line-height: 1;
    }
    #detail-section2-{{section.id}} .table-wrap .table-item .table-item-content {
      flex: 1;
      color: {{section.settings.subheading_color}};
      line-height: 1;
    }

    #detail-section2-{{section.id}} .desktopShow{
      position: relative;
      display: block;
      }
      #detail-section2-{{section.id}} .mobileShow{
      position: relative;
      display: none;
      }

      #detail-section2-{{section.id}} .desktopTableShow{
        position: relative;
          display: flex;

      }
      #detail-section2-{{section.id}} .mobileTableShow{
        position: relative;
          display: none;

      }
    /* 台式机、大屏幕 */
    @media screen and (min-width: 1024px) {
    }

    /*小屏幕、笔记本电脑 */
    @media screen and (min-width: 769px) and (max-width: 1024px) {

    }

    /* iPad、平板电脑 */
    @media screen and (min-width: 481px) and (max-width: 768px) {

    }

    /* 移动设备 */
    @media screen and (min-width: 320px) and (max-width: 480px) {
      #detail-section2-{{section.id}} {

          padding-top:{{ section.settings.padding_top_mobile }}px;
          padding-bottom:{{ section.settings.padding_bottom_mobile }}px;
          padding-left: {{ section.settings.padding_left_and_right_mobile }}px;
          padding-right: {{ section.settings.padding_left_and_right_mobile }}px;

        }

      #detail-section2-{{section.id}}  .custom-title-wrap{
          text-align: {{section.settings.content_text_alignment_mobile}};
      }
      #detail-section2-{{section.id}}  .custom-title {
        font-size: {{ section.settings.title_size_mobile }}px;
        margin-bottom: {{section.settings.title_to_content_padding_mobile}}px;
      }
      #detail-section2-{{section.id}}  .custom-subtitle {
        font-size: {{ section.settings.subtitle_size_mobile }}px ;
        margin-bottom: {{section.settings.subtitle_to_content_padding_mobile}}px;
      }
      #detail-section2-{{section.id}}  .table-wrap {
          gap: {{section.settings.table_gap_mobile}}px;
          border-radius: {{section.settings.table_border_radius_mobile}}px;

        }
        {% assign display_number_per_line_flex_mobile = 1.0 | divided_by:section.settings.display_number_per_line_flex_mobile | times:100 | round:2 %}
        #detail-section2-{{section.id}}  .table-wrap .table-item {
          display: flex;
          overflow: hidden;
          {%- if section.settings.show_table_gap_mobile -%}
            gap: {{section.settings.table_gap_mobile}}px;
            flex: 0 0 calc({{ display_number_per_line_flex_mobile }}% - {{section.settings.table_gap_mobile |divided_by:2 | times:section.settings.display_number_per_line_flex_mobile}}px);
            {% else %}
                gap: 0px;
                flex: 0 0 calc({{ display_number_per_line_flex_mobile }}% - {{section.settings.table_gap_mobile | divided_by:2 | times:section.settings.display_number_per_line_flex_mobile}}px);
          {% endif %}

        }
        #detail-section2-{{section.id}}  .table-wrap .table-item .table-item-title,#detail-section2-{{section.id}} .table-wrap .table-item .table-item-content{
            padding-right: {{section.settings.content_right_padding_mobile}}px;
            padding-left: {{section.settings.content_left_padding_mobile}}px;
            padding-top: {{section.settings.content_top_and_bottom_padding_mobile}}px;
            padding-bottom: {{section.settings.content_top_and_bottom_padding_mobile}}px;
      }



      #detail-section2-{{section.id}} .table-wrap .table-item .table-item-title {
          flex: 0 0 calc({{ section.settings.width_of_title_mobile }}% - {{section.settings.table_gap_mobile | times:section.settings.display_number_per_line_flex_mobile}}px);
        font-size: {{ section.settings.content_heading_size_mobile }}px;
      }
      #detail-section2-{{section.id}}  .table-wrap .table-item .table-item-content {
        font-size: {{ section.settings.content_subheading_size_mobile }}px;
      }
      #detail-section2-{{section.id}} .desktopShow{
          position: relative;
          display: none;
      }
      #detail-section2-{{section.id}} .mobileShow{
          position: relative;
          display: block;
      }
      #detail-section2-{{section.id}} .desktopTableShow{
        position: relative;
          display: none;

      }
      #detail-section2-{{section.id}} .mobileTableShow{
        position: relative;
          display: flex;

      }




    }
</style>

<!-- 参数表格模块 -->
<div id="detail-section2-{{section.id}}">
  <div class="container">
    <div class="custom-title-wrap">
      {% if section.settings.title != empty %}
        <div
          class="custom-title wow fadeInUp desktopShow {{ section.settings.title_size }} {{ section.settings.title_font_family }}"
          data-wow-delay=".8s"
        >
          {{ section.settings.title }}
        </div>
        {% if section.settings.title_mobile != empty %}
          <div
            class="custom-title wow fadeInUp mobileShow {{ section.settings.title_size }} {{ section.settings.title_font_family }}"
            data-wow-delay=".8s"
          >
            {{ section.settings.title_mobile }}
          </div>
        {% else %}
          <div
            class="custom-title wow fadeInUp mobileShow {{ section.settings.title_size }} {{ section.settings.title_font_family }}"
            data-wow-delay=".8s"
          >
            {{ section.settings.title }}
          </div>
        {% endif %}
      {% endif %}
      {% if section.settings.subtitle != empty %}
        <div
          class="custom-subtitle wow  fadeInUp desktopShow {{ section.settings.subtitle_size }} {{ section.settings.subtitle_font_family }}"
          data-wow-delay="1s"
        >
          {{ section.settings.subtitle }}
        </div>
        {% if section.settings.subtitle_mobile != empty %}
          <div
            class="custom-subtitle wow  fadeInUp mobileShow {{ section.settings.subtitle_size }} {{ section.settings.subtitle_font_family }}"
            data-wow-delay="1s"
          >
            {{ section.settings.subtitle_mobile }}
          </div>
        {% else %}
          <div
            class="custom-subtitle wow  fadeInUp mobileShow {{ section.settings.subtitle_size }} {{ section.settings.subtitle_font_family }}"
            data-wow-delay="1s"
          >
            {{ section.settings.subtitle }}
          </div>
        {% endif %}
      {% endif %}
    </div>
    {% if section.settings.content_layout_mobile == 'sequence' %}
      <div class="table-wrap">
        {%- for block in section.blocks -%}
          <style>
            #detail-section2-{{section.id}}  .table-wrap .table-item-{{ block.id }}.table-item .table-item-title {
              background-color: {{ block.settings.content_title_bg_color }};
            }
            #detail-section2-{{section.id}}  .table-wrap .table-item-{{ block.id }}.table-item .table-item-content {
              background-color: {{ block.settings.content_value_bg_color }};
            }
            @media screen and (min-width: 320px) and (max-width: 480px) {

                #detail-section2-{{section.id}}  .table-wrap .table-item-{{ block.id }}.table-item .table-item-title {
                  background-color: {{ block.settings.content_title_bg_color_mobile }};
                }
                #detail-section2-{{section.id}}  .table-wrap .table-item-{{ block.id }}.table-item .table-item-content {
                  background-color: {{ block.settings.content_value_bg_color_mobile }};
                }
            }
          </style>
          <div class="table-item table-item-{{ block.id }}">
            <div class="table-item-title {{ section.settings.content_heading_size }} {{ section.settings.content_heading_font_family }}">
              {{- block.settings.title -}}
            </div>
            <div class="table-item-content {{ section.settings.content_subheading_size }} {{ section.settings.content_subheading_font_family }}">
              {{- block.settings.value | default: '/' -}}
            </div>
          </div>
        {% endfor %}
      </div>
    {% else %}
      <!-- PC端显示 -->
      <div class="table-wrap desktopTableShow">
        {%- for block in section.blocks -%}
          <style>
            #detail-section2-{{section.id}}  .table-wrap .table-item-{{ block.id }}.table-item .table-item-title {
              background-color: {{ block.settings.content_title_bg_color }};
            }
            #detail-section2-{{section.id}}  .table-wrap .table-item-{{ block.id }}.table-item .table-item-content {
              background-color: {{ block.settings.content_value_bg_color }};
            }
            @media screen and (min-width: 320px) and (max-width: 480px) {

                #detail-section2-{{section.id}}  .table-wrap .table-item-{{ block.id }}.table-item .table-item-title {
                  background-color: {{ block.settings.content_title_bg_color_mobile }};
                }
                #detail-section2-{{section.id}}  .table-wrap .table-item-{{ block.id }}.table-item .table-item-content {
                  background-color: {{ block.settings.content_value_bg_color_mobile }};
                }
            }
          </style>
          <div class="table-item table-item-{{ block.id }}">
            <div class="table-item-title {{ section.settings.content_heading_size }} {{ section.settings.content_heading_font_family }}">
              {{- block.settings.title -}}
            </div>
            <div class="table-item-content {{ section.settings.content_subheading_size }} {{ section.settings.content_subheading_font_family }}">
              {{- block.settings.value | default: '/' -}}
            </div>
          </div>
        {% endfor %}
      </div>
      <!-- 移动端显示 -->
      <div class="table-wrap mobileTableShow">
        {%- for block in section.blocks -%}
          <style>
            #detail-section2-{{section.id}}  .table-wrap .table-item-{{ block.id }}.table-item .table-item-title {
              background-color: {{ block.settings.content_title_bg_color }};
            }
            #detail-section2-{{section.id}}  .table-wrap .table-item-{{ block.id }}.table-item .table-item-content {
              background-color: {{ block.settings.content_value_bg_color }};
            }
            @media screen and (min-width: 320px) and (max-width: 480px) {

                #detail-section2-{{section.id}}  .table-wrap .table-item-{{ block.id }}.table-item .table-item-title {
                  background-color: {{ block.settings.content_title_bg_color_mobile }};
                }
                #detail-section2-{{section.id}}  .table-wrap .table-item-{{ block.id }}.table-item .table-item-content {
                  background-color: {{ block.settings.content_value_bg_color_mobile }};
                }
            }
          </style>
          {% assign index = forloop.index | modulo: 2 %}
          {% if index != 0 %}
            <div class="table-item table-item-{{ block.id }}">
              <div class="table-item-title {{ section.settings.content_heading_size }} {{ section.settings.content_heading_font_family }}">
                {{- block.settings.title -}}
              </div>
              <div class="table-item-content {{ section.settings.content_subheading_size }} {{ section.settings.content_subheading_font_family }}">
                {{- block.settings.value | default: '/' -}}
              </div>
            </div>
          {% endif %}
          {% if forloop.last %}
            {% for block in section.blocks %}
              {% assign index = forloop.index | modulo: 2 %}
              {% if index == 0 %}
                <div class="table-item table-item-{{ block.id }}">
                  <div class="table-item-title {{ section.settings.content_heading_size }} {{ section.settings.content_heading_font_family }}">
                    {{- block.settings.title -}}
                  </div>
                  <div class="table-item-content {{ section.settings.content_subheading_size }} {{ section.settings.content_subheading_font_family }}">
                    {{- block.settings.value | default: '/' -}}
                  </div>
                </div>
              {% endif %}
            {% endfor %}
          {% endif %}
        {% endfor %}
      </div>
    {% endif %}
  </div>
</div>

{% schema %}
{
  "name": "parameters table",
  "tag": "section",
  "settings": [
    {
      "type": "html",
      "id": "title",
      "label": "大标题",
      "default": "Parameters"
    },
    {
      "type": "html",
      "id": "title_mobile",
      "label": "大标题（移动端）"
    },

    {
      "type": "html",
      "id": "subtitle",
      "label": "小标题"
    },
    {
      "type": "html",
      "id": "subtitle_mobile",
      "label": "小标题（移动端）"
    },
    {
      "type": "select",
      "id": "display_number_per_line",
      "label": "每行显示个数(PC端)",
      "options": [
        {
          "value": "2",
          "label": "2"
        },
        {
          "value": "3",
          "label": "3"
        },
        {
          "value": "4",
          "label": "4"
        },
        {
          "value": "5",
          "label": "5"
        },
        {
          "value": "6",
          "label": "6"
        }
      ],
      "default": "2"
    },
    {
      "type": "select",
      "id": "display_number_per_line_flex_mobile",
      "label": "每行显示个数(移动端)",
      "options": [
        {
          "value": "1",
          "label": "1"
        },
        {
          "value": "2",
          "label": "2"
        },
        {
          "value": "3",
          "label": "3"
        },
        {
          "value": "4",
          "label": "4"
        }
      ],
      "default": "1"
    },
    {
      "type": "radio",
      "id": "content_layout_mobile",
      "label": "排版布局,按顺序排列或按奇偶排列(移动端生效)",
      "options": [
        {
          "value": "sequence",
          "label": "按顺序排列"
        },
        {
          "value": "odd_even",
          "label": "按奇偶排列"
        }
      ],
      "default": "odd_even"
    },
    {
      "type": "range",
      "id": "content_left_padding",
      "min": 0,
      "max": 100,
      "step": 1,
      "unit": "px",
      "label": "内容左间距",
      "default": 40
    },
    {
      "type": "range",
      "id": "content_left_padding_mobile",
      "min": 0,
      "max": 100,
      "step": 1,
      "unit": "px",
      "label": "内容左间距（移动端）",
      "default": 24
    },
    {
      "type": "range",
      "id": "content_right_padding",
      "min": 0,
      "max": 100,
      "step": 1,
      "unit": "px",
      "label": "内容右间距",
      "default": 0
    },
    {
      "type": "range",
      "id": "content_right_padding_mobile",
      "min": 0,
      "max": 100,
      "step": 1,
      "unit": "px",
      "label": "内容右间距（移动端）",
      "default": 0
    },
    {
      "type": "range",
      "id": "content_top_and_bottom_padding",
      "min": 0,
      "max": 100,
      "step": 1,
      "unit": "px",
      "label": "内容上下间距",
      "default": 20
    },
    {
      "type": "range",
      "id": "content_top_and_bottom_padding_mobile",
      "min": 0,
      "max": 100,
      "step": 1,
      "unit": "px",
      "label": "内容上下间距（移动端）",
      "default": 14
    },
    {
      "type": "range",
      "id": "width_of_title",
      "min": 0,
      "max": 100,
      "step": 1,
      "unit": "%",
      "label": "参数名称内容占比宽度",
      "default": 50
    },
    {
      "type": "range",
      "id": "width_of_title_mobile",
      "min": 0,
      "max": 100,
      "step": 1,
      "unit": "%",
      "label": "参数名称内容占比宽度（移动端）",
      "default": 50
    },
    {
      "type": "range",
      "id": "table_gap",
      "min": 0,
      "max": 100,
      "step": 1,
      "unit": "px",
      "label": "表格间隙",
      "default": 2
    },
    {
      "type": "checkbox",
      "id": "show_table_gap_mobile",
      "label": "移动端开启表格间隙",
      "default": true
    },
    {
      "type": "range",
      "id": "table_gap_mobile",
      "min": 0,
      "max": 100,
      "step": 1,
      "unit": "px",
      "label": "表格间隙（移动端）",
      "default": 2
    },
    {
      "type": "range",
      "id": "table_border_radius",
      "min": 0,
      "max": 100,
      "step": 1,
      "unit": "px",
      "label": "表格圆角半径",
      "default": 20
    },
    {
      "type": "range",
      "id": "table_border_radius_mobile",
      "min": 0,
      "max": 100,
      "step": 1,
      "unit": "px",
      "label": "表格圆角半径（移动端）",
      "default": 10
    },
    {
      "type": "select",
      "id": "title_text_alignment",
      "label": "大标题及小标题文本对齐方式",
      "options": [
        {
          "value": "start",
          "label": "左对齐"
        },
        {
          "value": "center",
          "label": "居中对齐"
        },
        {
          "value": "end",
          "label": "右对齐"
        }
      ],
      "default": "center"
    },
    {
      "type": "select",
      "id": "content_text_alignment_mobile",
