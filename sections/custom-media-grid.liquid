<style>
  :root {
    /* Font sizes */
    --text-h0-n: 56px;
    --text-h1-n: 40px;
    --text-h2-n: 30px;
    --text-h3-n: 26px;
    --text-h4-n: 24px;
    --text-h5-n: 20px;
    --text-h6-n: 16px;
    --text-xs-n: 12px;
    --text-sm-n: 14px;
    --text-base-n: 18px;
    --text-lg-n: 19px;
  }
  .text-xxs {
    font-size: 10px;
    line-height: 1.2;
  }

  .text-xs {
    font-size: var(--text-xs-n);
    line-height: 1.2;
  }

  .text-sm {
    font-size: var(--text-sm-n);
    line-height: 1.2;
  }

  .text-base {
    font-size: var(--text-base-n);
    line-height: 1.2;
  }
  .text-md {
    font-size: var(--text-md-n);
    line-height: 1.2;
  }

  .text-lg {
    font-size: var(--text-lg-n);
    line-height: 1.2;
  }
  /* 重置样式 start*/
   #section-media-grid-{{section.id}}  .h0 {
    font-size: var(--text-h0-n) ;
    line-height: 1;
    font-family: ItalianPlateNo2Expanded-Bold, sans-serif ;
  }

  #section-media-grid-{{section.id}}  h1,
  #section-media-grid-{{section.id}}  .h1,
  #section-media-grid-{{section.id}}  .prose h1:not(.h0, .h1, .h2, .h3, .h4, .h5, .h6) {
    font-size: var(--text-h1-n) ;
    line-height: 1.1;
    font-family: ItalianPlateNo2Expanded-Bold, sans-serif ;
  }

  #section-media-grid-{{section.id}}  h2,
  #section-media-grid-{{section.id}}  .h2,
  #section-media-grid-{{section.id}}  .prose h2:not(.h0, .h1, .h2, .h3, .h4, .h5, .h6) {
    font-size: var(--text-h2-n);
    line-height: 1.1;
    font-family: ItalianPlateNo2Expanded-Bold, sans-serif ;
  }

  #section-media-grid-{{section.id}}  h3,
  #section-media-grid-{{section.id}}  .h3,
  #section-media-grid-{{section.id}}  .prose h3:not(.h0, .h1, .h2, .h3, .h4, .h5, .h6) {
    font-size: var(--text-h3-n) ;
    line-height: 1.2;
  }

  #section-media-grid-{{section.id}}  h4,
  #section-media-grid-{{section.id}}  .h4,
  #section-media-grid-{{section.id}}  .prose h4:not(.h0, .h1, .h2, .h3, .h4, .h5, .h6) {
    font-size: var(--text-h4-n) ;
    line-height: 1.3;
  }

  #section-media-grid-{{section.id}}  h5,
  #section-media-grid-{{section.id}}  .h5,
  #section-media-grid-{{section.id}}  .prose h5:not(.h0, .h1, .h2, .h3, .h4, .h5, .h6) {
    font-size: var(--text-h5-n) ;
    line-height: 1.4;
  }

  #section-media-grid-{{section.id}}  h6,
  #section-media-grid-{{section.id}}  .h6,
  #section-media-grid-{{section.id}}  .prose h6:not(.h0, .h1, .h2, .h3, .h4, .h5, .h6) {
    font-size: var(--text-h6-n) ;
    line-height: 1.4;
  }
  /* 重置样式 end*/
      #section-media-grid-{{section.id}} {
      width: 100%;
      margin: auto;
      overflow: hidden;
      }

      #section-media-grid-{{section.id}}  .features-puzzles-wrap {
      position: relative;
      display: flex;
      flex-direction: column;
      align-items: center;
      background-color: {{ section.settings.bg_color }};
      }

      #section-media-grid-{{section.id}}   .features-puzzles-container {
      display: flex;
      width: 100%;
      flex-direction: column;
      align-items: center;
      padding: {{ section.settings.padding_top | times: 1.0 | divided_by: 19.2 | round: 1 }}vw {{ section.settings.container_padding_right | times: 1.0 | divided_by: 19.2 | round: 1 }}vw {{ section.settings.padding_bottom | times: 1.0 | divided_by: 19.2 | round: 1 }}vw {{ section.settings.container_padding_left | times: 1.0 | divided_by: 19.2 | round: 1 }}vw;


      }
     #section-media-grid-{{section.id}} a:not([href]) {
        cursor: auto;
    }
  #section-media-grid-{{section.id}}  .features-puzzles-container  .features-puzzles-info{
      text-align: center;
      width: 100%;

    }
      #section-media-grid-{{section.id}}  .features-puzzles-container .features-puzzles-title {
      text-align: {{section.settings.title_alignment}};
      color: {{ section.settings.title_color }};
      width: 100%;
      margin-bottom: {{ section.settings.title_to_content_padding }}px;
      }
      #section-media-grid-{{section.id}}  .features-puzzles-container  .features-puzzles-subtitle{
        color: {{ section.settings.subtitle_color }};
        text-align: {{section.settings.title_alignment}};

    }

      #section-media-grid-{{section.id}}  .features-puzzles-container .features-puzzles-grid {
      display: grid;
      grid-template-columns: repeat({{ section.settings.max_columns_number_pc }}, minmax(0, 1fr));
      grid-template-rows: {{ section.settings.min_rows_height | times: 1.0 | divided_by: 19.2 | round: 1 }}vw;
      grid-auto-rows: {{ section.settings.min_rows_height | times: 1.0 | divided_by: 19.2 | round: 1 }}vw;
      grid-gap: {{ section.settings.grid_gap }}px;
      width: 100%;
      margin: 0 auto;
      padding: 0;
      {% if section.settings.is_fullScreen %}
      max-width: none;
      {% else %}
        max-width: 1200px;
      {% endif %}


      }

      #section-media-grid-{{section.id}}  .features-puzzles-container .features-puzzles-grid .grid-item {
      overflow: hidden;
      position: relative;
      background-color: {{section.settings.grid_bg_color}};
      cursor: pointer;
      border-radius: {{ section.settings.border-radius }}px;

      }
       #section-media-grid-{{section.id}}  .features-puzzles-container .features-puzzles-grid .grid-item img{

         transition: transform .5s ease;
      }
      #section-media-grid-{{section.id}}  .features-puzzles-container .features-puzzles-grid .grid-item:hover img{
         transform: scale(1.03);
         transition: transform .5s ease;
      }

      /* 添加蒙层 */
      {% if section.settings.is_add_overlay %}
      #section-media-grid-{{section.id}}  .features-puzzles-container .features-puzzles-grid .grid-item:after {
      content: "";
      display: block;
      position: absolute;
      top: 0;
      left: 0;
      width: 100%;
      height: 100%;
      background-color: rgba(0, 0, 0, {{ section.settings.overlay_transparence }});

      }
      {% endif %}




      #section-media-grid-{{section.id}}  .features-puzzles-container .features-puzzles-grid .content {
      position: absolute;
      line-height: 1.6;
      z-index: 2;
      {% comment %} padding: 10px; {% endcomment %}
      }




  #section-media-grid-{{section.id}}  .features-puzzles-container .features-puzzles-grid .content .button1 {
      display: flex;
      align-items: center;
      gap: 10px;
      border: 0;

      color: {{ section.settings.button_text_color }};

  }
  #section-media-grid-{{section.id}}  .features-puzzles-container .features-puzzles-grid .content .button1 .icon-arrow-right{

    color:{{ section.settings.button_text_color }}
  }
      #section-media-grid-{{section.id}}  .features-puzzles-container .features-puzzles-grid  img,
      .features-puzzles-container .features-puzzles-grid video {
      position: absolute;
      display: block;
      border: 0;
      max-width: 100%;
      width: 100%;
      height: 100%;
      object-fit: cover;
      }
  /* ------------------------------------------------------视频样式-------------------------- */
  #section-media-grid-{{section.id}} .video-banner{
  position: relative;
    width: 100%;
    height: 100%;
  }
  #section-media-grid-{{section.id}}  .caption-body{
  position: absolute;
  bottom: 0;
  background: rgba(158,158,158,.6);
  width: 100%;
  padding: 8px;
  text-align: center;
  }
  #section-media-grid-{{section.id}}  .caption-body .caption-text{
  color: #fff;
  line-height: 1.4;
  }
  #section-media-grid-{{section.id}} .video-banner .czvdo-play-icon-svg {
    position: absolute;
    width: 50px;
    left: 50%;
    top: 50%;
    transform: translate(-50%, -50%);
  }
   #section-media-grid-{{section.id}} .video-banner:hover .czvdo-svg-path-outer {
    fill: #f93822;
    transition: all 0.3s ease;
  }
  /* ------------------------------------------------------查看全部按钮样式-------------------------- */
   #section-media-grid-{{section.id}}  .view-all-wrap{
       display: flex;
       justify-content: flex-end;
       width: 100%;
      margin-top: 20px;
      {% unless section.settings.is_fullScreen %}
         max-width: 1200px;
      {% endunless %}

     }
    #section-media-grid-{{section.id}} .view-all{
               display: flex;
               gap: 10px;
               align-items: center;
               color:#000 ;
           }

    #section-media-grid-{{section.id}}  .reversed-link{
               background: linear-gradient(to right,currentColor,currentColor) 0 min(100%,1.35em) / 0 1px no-repeat;
               transition: background-size .3s ease-in-out;
               margin: 0;
               font-family: ItalianPlateNo2Expanded-Bold;
           }
           @media screen and (pointer: fine){
       #section-media-grid-{{section.id}}  .view-all:hover .reversed-link,#section-media-grid-{{section.id}} .reversed-link.hover\:show:hover {
                   background-size: 100% 1px;
               }
           }

    #section-media-grid-{{section.id}}  .circle-chevron{
               width: 20px;
               height: 20px;
               border-radius: 50%;
               background: {{section.settings.bg_color}};
               flex-shrink: 0;
               place-items: center;
               transition: all .2s ease-in-out;
               display: grid;
           }

           @media screen and (pointer: fine){
        #section-media-grid-{{section.id}} .view-all:hover .circle-chevron {
                   background: #000;

               }
        #section-media-grid-{{section.id}}  .view-all:hover .circle-chevron svg{
               color: #fff;
               }
           }
      #section-media-grid-{{section.id}} .desktopShow,#section-media-grid-{{section.id}} .features-puzzles-container .features-puzzles-grid .desktopShow{
      display: block;
      }
      #section-media-grid-{{section.id}} .mobileShow,#section-media-grid-{{section.id}} .features-puzzles-container .features-puzzles-grid .mobileShow{
      display: none;
      }
      @media (max-width: 1400px){
      {% comment %} #section-media-grid-{{section.id}}  .features-puzzles-container .features-puzzles-grid .content .title {
        font-size: 20px;

        } {% endcomment %}
      }
      @media (max-width: 1024px) {
      #section-media-grid-{{section.id}}  .features-puzzles-container .features-puzzles-grid {
      display: grid;
      grid-template-columns: repeat(2, minmax(0, 1fr));
      grid-template-rows: {{ section.settings.min_rows_height_mobile | times: 1.0 | divided_by: 7.5 | round: 1 }}vw;
      grid-auto-rows: {{ section.settings.min_rows_height_mobile | times: 1.0 | divided_by: 7.5 | round: 1 }}vw;
      grid-gap: 15px;
      width: 100%;
      margin: 0 auto;
      padding: 0;
      max-width: 1175px;

      }
            {% comment %} #section-media-grid-{{section.id}}  .features-puzzles-container .features-puzzles-grid .content .title {
        font-size: 18px;

        } {% endcomment %}

      }

      @media (max-width: 768px) {
        #section-media-grid-{{section.id}}  .features-puzzles-container .features-puzzles-title {
            font-size: {{ section.settings.title_size_mobile }}px;
            margin-bottom: {{ section.settings.title_to_content_padding_mobile }}px;
            text-align: {{section.settings.title_alignment_mobile}};

            }
            #section-media-grid-{{section.id}}  .features-puzzles-container  .features-puzzles-subtitle{
                font-size: {{ section.settings.subtitle_size_mobile }}px;
                text-align: {{section.settings.title_alignment_mobile}};

            }
      #section-media-grid-{{section.id}} .desktopShow,#section-media-grid-{{section.id}} .features-puzzles-container .features-puzzles-grid .desktopShow{
      position: relative;
      display: none;
      }
      #section-media-grid-{{section.id}} .mobileShow,#section-media-grid-{{section.id}} .features-puzzles-container .features-puzzles-grid .mobileShow{
      position: relative;
      display: block;
      }
      #section-media-grid-{{section.id}}  .features-puzzles-container{
      padding: {{ section.settings.padding_top_mobile  }}px  24px {{ section.settings.padding_bottom_mobile  }}px 24px;


      }
      #section-media-grid-{{section.id}}  .features-puzzles-container .features-puzzles-grid .content .button1 {
        gap: 5px;



    }
      #section-media-grid-{{section.id}}  .features-puzzles-container .features-puzzles-grid .content .button1 .icon-arrow-right{

        color:{{ section.settings.button_text_color }}
      }


       #section-media-grid-{{section.id}}  .caption-body .caption-text{
  line-height: 1.1;
  }
      {% if section.settings.min_columns_number_mobile == '2' %}
      #section-media-grid-{{section.id}}  .features-puzzles-container .features-puzzles-grid {
      display: grid;
      grid-template-columns: repeat(2, minmax(0, 1fr));
      grid-template-rows: {{ section.settings.min_rows_height_mobile | times: 1.0 | divided_by: 7.5 | round: 1 }}vw;
      grid-auto-rows: {{ section.settings.min_rows_height_mobile | times: 1.0 | divided_by: 7.5 | round: 1 }}vw;
      grid-gap: {{ section.settings.grid_gap_mobile }}px;
      width: 100%;
      margin: 0 auto;
      padding: 0;


      }
      {% elsif section.settings.min_columns_number_mobile == '3' %}
      #section-media-grid-{{section.id}}  .features-puzzles-container .features-puzzles-grid {
      display: grid;
      grid-template-columns: repeat(3, minmax(0, 1fr));
      grid-template-rows: {{ section.settings.min_rows_height_mobile | times: 1.0 | divided_by: 7.5 | round: 1 }}vw;
      grid-auto-rows: {{ section.settings.min_rows_height_mobile | times: 1.0 | divided_by: 7.5 | round: 1 }}vw;
      grid-gap: {{ section.settings.grid_gap_mobile }}px;
      width: 100%;
      margin: 0 auto;
      padding: 0;


      }
      {% elsif section.settings.min_columns_number_mobile == '4' %}
      #section-media-grid-{{section.id}}  .features-puzzles-container .features-puzzles-grid {
      display: grid;
      grid-template-columns: repeat(4, minmax(0, 1fr));
      grid-template-rows: {{ section.settings.min_rows_height_mobile | times: 1.0 | divided_by: 7.5 | round: 1 }}vw;
      grid-auto-rows: {{ section.settings.min_rows_height_mobile | times: 1.0 | divided_by: 7.5 | round: 1 }}vw;
      grid-gap: {{ section.settings.grid_gap_mobile }}px;
      width: 100%;
      margin: 0 auto;
      padding: 0;


      }
      {% elsif section.settings.min_columns_number_mobile == '5' %}
      #section-media-grid-{{section.id}}  .features-puzzles-container .features-puzzles-grid {
      display: grid;
      grid-template-columns: repeat(5, minmax(0, 1fr));
      grid-template-rows: {{ section.settings.min_rows_height_mobile | times: 1.0 | divided_by: 7.5 | round: 1 }}vw;
      grid-auto-rows: {{ section.settings.min_rows_height_mobile | times: 1.0 | divided_by: 7.5 | round: 1 }}vw;
      grid-gap: {{ section.settings.grid_gap_mobile }}px;
      width: 100%;
      margin: 0 auto;
      padding: 0;


      }
      {% else %}
      #section-media-grid-{{section.id}}  .features-puzzles-container .features-puzzles-grid {
      display: flex;
      flex-flow: column;
      width: 100%;
      margin: 0 auto;
      padding: 0;


      }
      #section-media-grid-{{section.id}} .mobile-video-wrap{
       height: 200px;
      }
      {% endif %}


      #section-media-grid-{{section.id}}  .features-puzzles-container .features-puzzles-grid .content .title {
      font-size: {{ section.settings.heading_size_mobile }}px;
      margin-top: {{ section.settings.heading_margin_top_mobile }}px;
      margin-bottom: {{ section.settings.heading_margin_bottom_mobile }}px;

      }

      #section-media-grid-{{section.id}}  .features-puzzles-container .features-puzzles-grid .content .desc {
      font-size: {{ section.settings.subheading_size_mobile }}px;
      margin-bottom: {{ section.settings.subheading_margin_bottom_mobile }}px;
      }
  #section-media-grid-{{section.id}} .swiper-containerProduct{
  width: 100%;
  border-radius:{{ section.settings.border-radius }}px;
  }
  #section-media-grid-{{section.id}}  .features-puzzles-container .features-puzzles-grid .grid-item {

    border-radius: {{ section.settings.border-radius_mobile }}px;

    }
  #section-media-grid-{{section.id}} .circle-button {
  width: var(--spacing-12);
  height: var(--spacing-12);
  border-radius: var(--rounded-full);
  place-items: center;
  transition: opacity .15s ease-in;
  display: grid;
  }
  #section-media-grid-{{section.id}}  .ring {
    box-shadow: 0 0 0 1px rgb(26, 26, 26, .12);
    background-color: rgba(255,255,255,0.6);;
  }
  #section-media-grid-{{section.id}}  .animated-arrow--reverse {
  transform: rotate(180deg);
  }
  #section-media-grid-{{section.id}}  .animated-arrow {
  pointer-events: none;
  place-items: center;
  display: grid;
  }
  #section-media-grid-{{section.id}}  .animated-arrow:before, #section-media-grid-{{section.id}} .animated-arrow:after {
  content: "";
  grid-area: 1 / -1;
  transition: all .2s ease-in-out;
  box-sizing: border-box;
  border-style: solid;
  border-width: 0;
  border-color: rgb(26,26,26);
  }
  #section-media-grid-{{section.id}}  .animated-arrow:before {
  width: 8px;
  height: 8px;
  transform: rotate(calc(1 * 45deg));
  border-block-start-width: 2px;
  border-inline-end-width: 2px;
  position: relative;
  }
  #section-media-grid-{{section.id}}  .animated-arrow:after {
  width: var(--spacing-2-5);
  height: var(--spacing-0-5);
  transform-origin: center;
  opacity: 0;
  background: rgb(26,26,26);
  transform: scaleX(.5);
  }
  #section-media-grid-{{section.id}}  .group:hover .animated-arrow:after {
  opacity: 1;
  transform: scaleX(1);
  }
  #section-media-grid-{{section.id}}  .group:hover .animated-arrow:before {
  transform: matrix(0.707107, 0.707107, -0.707107, 0.707107, 2, 0);
  }
      }
</style>

<div id="section-media-grid-{{section.id}}" class="custom-wrap ">
  <div class="features-puzzles-wrap">
    <div class="features-puzzles-container">
      {% if section.settings.title != blank %}
        <div class="features-puzzles-info">
          <h2
            class="features-puzzles-title  {{ section.settings.title_size }} wow fadeInUp desktopShow"
            data-wow-delay=".8s"
          >
            {{ section.settings.title }}
          </h2>
          {% if section.settings.title_mobile != empty %}
            <h2
              class="features-puzzles-title  {{ section.settings.title_size }} wow fadeInUp mobileShow"
              data-wow-delay=".8s"
            >
              {{ section.settings.title_mobile }}
            </h2>
          {% else %}
            <h2
              class="features-puzzles-title  {{ section.settings.title_size }} wow fadeInUp mobileShow"
              data-wow-delay=".8s"
            >
              {{ section.settings.title }}
            </h2>
          {% endif %}
          {% if section.settings.subtitle != empty %}
            <div>
              <p
                class="features-puzzles-subtitle wow fadeInUp desktopShow {{ section.settings.subtitle_size }}"
                data-wow-delay=".8s"
              >
                {{ section.settings.subtitle }}
              </p>
            </div>
            {% if section.settings.subtitle_mobile != empty %}
              <div>
                <p
                  class="features-puzzles-subtitle wow fadeInUp mobileShow {{ section.settings.subtitle_size }}"
                  data-wow-delay=".8s"
                >
                  {{ section.settings.subtitle_mobile }}
                </p>
              </div>
            {% else %}
              <div>
                <p
                  class="features-puzzles-subtitle wow fadeInUp mobileShow {{ section.settings.subtitle_size }}"
                  data-wow-delay=".8s"
                >
                  {{ section.settings.subtitle }}
                </p>
              </div>
            {% endif %}
          {% endif %}
        </div>
      {% endif %}
      {% if section.settings.is_slide_on_mobile %}
        <div class="swiper-container  swiper-containerProduct swiper-template-{{section.id}} mobileShow">
          <div class="swiper-wrapper">
            {%- for block in section.blocks -%}
              {% case block.type %}
                {%- when 'imageItem' -%}
                  <div class="swiper-slide">
                    <div
                      {% if block.settings.link_url %}
                        onclick="window.location.href='{{- block.settings.link_url -}}';"
                      {% endif %}
                    >
                      <div
                        class="content"
                        style="text-align:{{ block.settings.button_align_mobile }};width:{{ block.settings.content_width_mobile }}%;top:{{ block.settings.content_position_top_mobile }}%;left:{{ block.settings.content_position_left_mobile }}%"
                      >
                        <div
                          class="title gb wow fadeInUp"
                          data-wow-delay="1s"
                          style="text-align:{{ block.settings.heading_align_mobile  }}"
                        >
                          {{ block.settings.heading }}
                        </div>
                        <div class="desc gothambook" style="text-align:{{ block.settings.subheading_align_mobile  }}">
                          {{ block.settings.subheading }}
                        </div>
                        {% if block.settings.link_url and block.settings.button_text != blank %}
                          <a class="button1" href="{{- block.settings.link_url -}}"
                            ><span> {{ block.settings.button_text }}</span
                            ><i
                              class="iconfont icon-arrow-right"
                            ></i
                          ></a>
                        {% endif %}
                      </div>
                      {% if block.settings.image_mobile %}
                        {% assign image_mobile = block.settings.image_mobile %}
                        <img
                          data-sizes="auto"
                          data-srcset="
                            {%- if image_mobile.width >= 375 -%}{{ image_mobile | image_url: width: 375 }} 375w,{%- endif -%}
                            {%- if image_mobile.width >= 750 -%}{{ image_mobile | image_url: width: 750 }} 750w,{%- endif -%}
                            {%- if image_mobile.width >= 1100 -%}{{ image_mobile | image_url: width: 1100 }} 1100w,{%- endif -%}
                            {%- if image_mobile.width >= 1500 -%}{{ image_mobile | image_url: width: 1500 }} 1500w,{%- endif -%}
                            {%- if image_mobile.width >= 1780 -%}{{ image_mobile | image_url: width: 1780 }} 1780w,{%- endif -%}
                            {%- if image_mobile.width >= 2000 -%}{{ image_mobile | image_url: width: 2000 }} 2000w,{%- endif -%}
                            {%- if image_mobile.width >= 3000 -%}{{ image_mobile | image_url: width: 3000 }} 3000w,{%- endif -%}
                            {%- if image_mobile.width >= 3840 -%}{{ image_mobile | image_url: width: 3840 }} 3840w,{%- endif -%}

                            {{ image_mobile | image_url }} {{ image_mobile.width }}w
                          "
                          data-src="{{ image_mobile | image_url: width: 1200 }}"
                          class="lazyload mobileShow"
                          loading="lazy"
                        >
                      {% else %}
                        {{ 'product-1' | placeholder_svg_tag }}
                      {% endif %}
