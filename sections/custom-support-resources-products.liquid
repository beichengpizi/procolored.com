<script>
  // 处理模态框标签页切换
  document.querySelectorAll('.modal-item-tab-link').forEach((link) => {
    // 添加点击和触摸事件监听器
    link.addEventListener('click', handleTabSwitch);
    link.addEventListener('touchstart', handleTabSwitch);

    function handleTabSwitch(e) {
      e.preventDefault(); // 阻止默认行为
      const modal = this.closest('.modal');
      const tabIndex = this.classList[1].split('-').pop();

      // 隐藏所有内容并显示目标内容
      const allContentItems = modal.querySelectorAll('.modal-content-item');
      const targetContent = modal.querySelector(`.modal-content-item-${tabIndex}`);

      allContentItems.forEach((item) => (item.style.display = 'none'));
      if (targetContent) targetContent.style.display = 'block';

      // 更新标签页激活状态
      const allTabs = modal.querySelectorAll('.modal-item-tab');
      allTabs.forEach((tab) => tab.classList.remove('active'));
      this.closest('.modal-item-tab').classList.add('active');
    }
  });
</script>

<script>
  /**
   * 节流函数 - 限制函数的执行频率
   * @param {Function} func - 需要执行的函数
   * @param {number} limit - 时间间隔(毫秒)
   * @returns {Function} 返回节流后的函数
   */
  function throttle(func, limit) {
    let inThrottle = false;
    return function (...args) {
      if (!inThrottle) {
        func.apply(this, args);
        inThrottle = true;
        setTimeout(() => (inThrottle = false), limit);
      }
    };
  }

  // 页面加载完成后初始化
  document.addEventListener('DOMContentLoaded', function () {
    const dropBtn = document.querySelector('.p-drop');
    const contentBox = document.querySelector('.content-box');
    const modBoxes = contentBox.querySelectorAll('.mod-box');
    const firstLi = document.querySelector('.pdc ul li:first-child');

    // 初始化下拉菜单和内容显示
    dropBtn.textContent = firstLi.textContent;
    modBoxes[0].style.display = 'block';

    // 处理标签切换 - 同时支持点击和触摸
    document.querySelectorAll('.tab-title .title-item').forEach((item) => {
      const handleClick = function (e) {
        e.preventDefault();
        const index = Array.from(this.parentNode.children).indexOf(this);

        // 更新标签和内容显示状态
        const allTitleItems = document.querySelectorAll('.tab-title .title-item');
        allTitleItems.forEach((el) => el.classList.remove('active'));
        this.classList.add('active');

        modBoxes.forEach((box, i) => {
          box.style.display = i === index ? 'block' : 'none';
        });
      };

      item.addEventListener('click', handleClick);
      item.addEventListener('touchstart', handleClick);
    });

    // 处理下拉菜单选项点击 - 合并重复代码并优化移动端支持
    document.querySelectorAll('.pdc li').forEach((li) => {
      const handleSelect = function (e) {
        e.preventDefault();
        const selectedText = this.textContent;
        const selectedIndex = Array.from(this.parentNode.children).indexOf(this);
        const pdc = document.querySelector('.pdc');

        // 更新下拉按钮文本并隐藏菜单
        dropBtn.textContent = selectedText;
        pdc.style.display = 'none';

        // 显示对应内容
        modBoxes.forEach((box, i) => {
          box.style.display = i === selectedIndex ? 'block' : 'none';
        });
      };

      li.addEventListener('click', handleSelect);
      li.addEventListener('touchstart', handleSelect);
    });

    // 下拉按钮点击处理 - 优化移动端支持
    const toggleDropdown = throttle(function (e) {
      e.preventDefault();
      const pdc = document.querySelector('.pdc');
      pdc.style.display = pdc.style.display === 'none' ? 'block' : 'none';
    }, 500);

    dropBtn.addEventListener('click', toggleDropdown);
    dropBtn.addEventListener('touchstart', toggleDropdown);

    // 点击其他区域关闭下拉菜单
    document.addEventListener('click', function (e) {
      if (!dropBtn.contains(e.target) && !document.querySelector('.pdc').contains(e.target)) {
        document.querySelector('.pdc').style.display = 'none';
      }
    });

    document.addEventListener('touchstart', function (e) {
      if (!dropBtn.contains(e.target) && !document.querySelector('.pdc').contains(e.target)) {
        document.querySelector('.pdc').style.display = 'none';
      }
    });
  });

  // 处理产品项点击事件
  document.querySelectorAll('.item-link').forEach((link) => {
    const handleItemClick = function (event) {
      event.preventDefault();
      const index = this.getAttribute('data-index');
      const modal = this.closest('.mod-box').querySelector('.modal');

      // 更新标签页状态
      const modalTabs = modal.querySelectorAll('.modal-item-tab');
      modalTabs.forEach((tab) => tab.classList.remove('active'));

      const correspondingTab = modal.querySelector(`.modal-item-tab-${index}`);
      if (correspondingTab) {
        correspondingTab.classList.add('active');
        // 触发标签页点击事件
        const modalItemTabLink = correspondingTab.querySelector('.modal-item-tab-link');
        modalItemTabLink?.click();
      }

      // 更新内容显示
      const allModalItems = modal.querySelectorAll('.modal-item');
      allModalItems.forEach((item) => (item.style.display = 'none'));

      const targetModalItem = modal.querySelector(`.modal-item[data-index="${index}"]`);
      if (targetModalItem) targetModalItem.style.display = 'block';

      modal.classList.add('active');
      document.body.style.overflow = 'hidden';
    };

    // 添加点击和触摸事件监听器
    link.addEventListener('click', handleItemClick);
  });

  // 处理关闭按钮点击
  document.querySelectorAll('.close').forEach((closeBtn) => {
    closeBtn.addEventListener('click', function () {
      this.closest('.modal').classList.remove('active');
      document.body.style.overflow = 'auto';
    });
  });

  // 点击模态框背景或关闭图标关闭
  window.addEventListener('click', function (event) {
    if (event.target.classList.contains('modal') || event.target.closest('.modal-close')) {
      const modal = event.target.classList.contains('modal') ? event.target : event.target.closest('.modal');
      modal.classList.remove('active');
      document.body.style.overflow = 'auto';
    }
  });
</script>
