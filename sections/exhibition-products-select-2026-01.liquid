                              <script>
                                $(function () {
                                  function hasParent(ele, p) {
                                    while (ele && ele !== document.body) {
                                      if (ele === p) return true;
                                      ele = ele.parentNode;
                                    }
                                    return false;
                                  }
                                  // 获取当前下拉元素
                                  const dropdown = document.getElementById(
                                    'exhibition-dropdown-mock-{{ current_product_id }}'
                                  );
                                  console.log('dropdown==', dropdown);
                                  const sectionRoot = dropdown.closest('.exhibition-products-select-content-item');
                                  console.log('sectionRoot==', sectionRoot);
                                  const cardContainer = sectionRoot.querySelectorAll('.exhibition-product-tab');
                                  console.log('cardContainer==', cardContainer);
                                  if (!dropdown || dropdown.getAttribute('data-bind')) return;
                                  dropdown.setAttribute('data-bind', '1');
                                  const selected = dropdown.querySelector('[data-selected]');
                                  const optionsUl = dropdown.querySelector('[data-options]');
                                  const options = optionsUl
                                    ? optionsUl.querySelectorAll('.exhibition-dropdown-mock-option')
                                    : [];
                                  // 点击展开收起
                                  function closeDropdown() {
                                    dropdown.classList.remove('open');
                                  }
                                  function openDropdown() {
                                    dropdown.classList.add('open');
                                  }
                                  selected &&
                                    selected.addEventListener('click', function (e) {
                                      e.preventDefault();
                                      e.stopPropagation();
                                      // 点击当前元素，获取父元素exhibition-products-select-{{ section.id }}
                                      const parent = dropdown.closest('.exhibition-products-select-{{ section.id }}');
                                      // 如需使用 parent，可在此处添加逻辑
                                      if (dropdown.classList.contains('open')) {
                                        closeDropdown();
                                      } else {
                                        openDropdown();
                                      }
                                    });
                                  // 选项点击
                                  options.forEach(function (option, idx) {
                                    option.addEventListener('click', function (e) {
                                      e.stopPropagation();
                                      // 激活当前
                                      options.forEach(function (opt) {
                                        opt.classList.remove('active');
                                      });
                                      option.classList.add('active');
                                      if (selected) selected.innerText = option.innerText;
                                      closeDropdown();
                                      // 分发事件
                                      // 如何触发该事件：直接通过下拉选项点击（option click）时会执行如下代码，从而触发事件
                                      // 如果你想在其他地方手动触发，可以调用 dropdown.dispatchEvent(new CustomEvent('christmasTabCustomDropdownChange', {...}))，传递需要的 detail
                                      const event = new CustomEvent('christmasTabCustomDropdownChange', {
                                        detail: {
                                          idx: idx,
                                          value: option.getAttribute('data-value'),
                                          text: option.innerText,
                                        },
                                        bubbles: true,
                                      });
                                      dropdown.dispatchEvent(event);

                                      // 联动select
                                      try {
                                        const tabContainers = dropdown
                                          .closest('section')
                                          .querySelectorAll('.exhibition-product-tab');
                                        cardContainer.forEach(function (tabCt, index) {
                                          console.log('tabCt', tabCt);

                                          if (index === idx) {
                                            tabCt.classList.add('active');
                                            // tabContainers 下的 .exhibition-product-tab 是否含有 active
                                            const activeTabs = Array.from(tabContainers).filter(function (ct) {
                                              return ct.classList.contains('active');
                                            });
                                            console.log('tabContainers 内包含 active 的元素:', activeTabs);
                                            // 遍历activeTabs 下的exhibition-dropdown-mock-option，当idx相等则激活
                                            activeTabs.forEach(function (activeTab) {
                                              const dropdownOptions = activeTab.querySelectorAll(
                                                '.exhibition-dropdown-mock-option'
                                              );
                                              var dropdownMockParent = null;
                                              if (dropdownOptions.length > 0) {
                                                dropdownMockParent =
                                                  dropdownOptions[0].closest('.exhibition-dropdown-mock');
                                              }
                                              console.log(
                                                'exhibition-dropdown-mock-{{ current_product_id }} 元素:',
                                                dropdownMockParent
                                              );
                                              dropdownOptions.forEach(function (opt, optIdx) {
                                                if (optIdx === idx) {
                                                  opt.classList.add('active');
                                                  console.log('data-value', opt.getAttribute('data-value'));
                                                  console.log('innerText', opt.innerText);
                                                  const selected = dropdownMockParent.querySelector('[data-selected]');
                                                  if (selected) selected.innerText = opt.getAttribute('data-value');
                                                  return;
                                                } else {
                                                  opt.classList.remove('active');
                                                }
                                              });
                                            });
                                            return; // 跳出forEach循环
                                          } else {
                                            tabCt.classList.remove('active');
                                          }
                                        });
                                      } catch (e) {}
                                    });
                                  });
                                  // 外部点击收起
                                  document.addEventListener('click', function (e) {
                                    if (!hasParent(e.target, dropdown)) {
                                      closeDropdown();
                                    }
                                  });
                                  // 键盘ESC收起
                                  dropdown.addEventListener('keydown', function (e) {
                                    if (e.key === 'Escape') {
                                      closeDropdown();
                                    }
                                  });
                                });
                              </script>
                            </div>
                          </div>
