<script>
  document.addEventListener('DOMContentLoaded', function () {
    const content = document.getElementById('exhibition-products-select-content-{{ section.id }}');
    const bar = document.getElementById('exhibition-products-select-content-scrollbar-bar-{{ section.id }}');
    if (!content || !bar) return;
    // 获取 page 数量
    const itemCount = content.querySelectorAll(
      '#exhibition-products-select-content-{{ section.id }} .exhibition-products-select-content-item'
    ).length;

    // 设置 bar 宽度
    bar.style.width = `${100 / itemCount}%`;

    content.addEventListener('scroll', () => {
      const scrollLeft = content.scrollLeft;
      const maxScroll = content.scrollWidth - content.clientWidth;
      const progress = scrollLeft / maxScroll || 0;

      const barMaxTranslate = content.clientWidth * (1 - bar.offsetWidth / content.clientWidth);

      bar.style.transform = `translateX(${progress * barMaxTranslate}px)`;
    });
  });
</script>
