<style>
  :root {
      /* Font sizes */
      --text-h0-n: 56px;
      --text-h1-n: 40px;
      --text-h2-n: 30px;
      --text-h3-n: 26px;
      --text-h4-n: 24px;
      --text-h5-n: 20px;
      --text-h6-n: 16px;
      --text-xs-n: 12px;
      --text-sm-n: 14px;
      --text-base-n: 18px;
      --text-lg-n: 19px;
    }

    .text-xxs {
      font-size: 10px;
      line-height: 1.3;
    }

    .text-xs {
      font-size: var(--text-xs-n);
      line-height: 1.3;
    }

    .text-sm {
      font-size: var(--text-sm-n);
      line-height: 1.3;
    }

    .text-base {
      font-size: var(--text-base-n);
      line-height: 1.3;
    }

    .text-md {
      font-size: var(--text-md-n);
      line-height: 1.2;
    }

    .text-lg {
      font-size: var(--text-lg-n);
      line-height: 1.2;
    }

    /* 重置样式 start */
    .Multi-column-video-autoplay-{{ section.id }} .h0 {
      font-size: var(--text-h0-n);
      line-height: 1;
    }

    .Multi-column-video-autoplay-{{ section.id }} h1,
    .Multi-column-video-autoplay-{{ section.id }} .h1,
    .Multi-column-video-autoplay-{{ section.id }} .prose h1:not(.h0, .h1, .h2, .h3, .h4, .h5, .h6) {
      font-size: var(--text-h1-n);
      line-height: 1.1;
    }

    .Multi-column-video-autoplay-{{ section.id }} h2,
    .Multi-column-video-autoplay-{{ section.id }} .h2,
    .Multi-column-video-autoplay-{{ section.id }} .prose h2:not(.h0, .h1, .h2, .h3, .h4, .h5, .h6) {
      font-size: var(--text-h2-n);
      line-height: 1.1;
    }

    .Multi-column-video-autoplay-{{ section.id }} h3,
    .Multi-column-video-autoplay-{{ section.id }} .h3,
    .Multi-column-video-autoplay-{{ section.id }} .prose h3:not(.h0, .h1, .h2, .h3, .h4, .h5, .h6) {
      font-size: var(--text-h3-n);
      line-height: 1.2;
    }

    .Multi-column-video-autoplay-{{ section.id }} h4,
    .Multi-column-video-autoplay-{{ section.id }} .h4,
    .Multi-column-video-autoplay-{{ section.id }} .prose h4:not(.h0, .h1, .h2, .h3, .h4, .h5, .h6) {
      font-size: var(--text-h4-n);
      line-height: 1.3;
    }

    .Multi-column-video-autoplay-{{ section.id }} h5,
    .Multi-column-video-autoplay-{{ section.id }} .h5,
    .Multi-column-video-autoplay-{{ section.id }} .prose h5:not(.h0, .h1, .h2, .h3, .h4, .h5, .h6) {
      font-size: var(--text-h5-n);
      line-height: 1.3;
    }

    .Multi-column-video-autoplay-{{ section.id }} h6,
    .Multi-column-video-autoplay-{{ section.id }} .h6,
    .Multi-column-video-autoplay-{{ section.id }} .prose h6:not(.h0, .h1, .h2, .h3, .h4, .h5, .h6) {
      font-size: var(--text-h6-n);
      line-height: 1.3;
    }
    /* 重置样式 end */

    .Multi-column-video-autoplay-{{ section.id }} .Multi-column-video-autoplay-wrap {
      background: {{ section.settings.background_color }};;
      padding-top: {{ section.settings.padding_top }}px;
      padding-bottom: {{ section.settings.padding_bottom }}px;
    }

    .Multi-column-video-autoplay-{{ section.id }} .Multi-column-video-autoplay-container {
      {% if section.settings.section_max_width != 'none' %}
        max-width: {{ section.settings.section_max_width | plus:48 }}px;
        margin: 0 auto;
      {% endif %}
      padding: 0 24px;
    }

    .Multi-column-video-autoplay-{{ section.id }} .Multi-column-video-autoplay-title-wrap {
      display: flex;
      flex-direction: column;
      align-items: {{section.settings.title_alignment}};
    }

    .Multi-column-video-autoplay-{{ section.id }} .Multi-column-video-autoplay-title {
      color:{{ section.settings.title_color }};
      text-align: {{section.settings.title_alignment}};
      margin-bottom: {{section.settings.title_margin_bottom}}px;
    }

    .Multi-column-video-autoplay-{{ section.id }} .Multi-column-video-autoplay-subtitle {
      color: {{ section.settings.subtitle_color }};
      text-align: {{section.settings.title_alignment}};
      line-height: 1.38;
      margin-bottom: {{section.settings.subtitle_margin_bottom}}px;
    }

    .Multi-column-video-autoplay-{{ section.id }} .Multi-column-video-autoplay-content {
      display: grid;
      grid-template-columns: repeat({{ section.settings.slides_per_view}}, 1fr);
      gap: {{section.settings.card_gap}}px;
    }

    .Multi-column-video-autoplay-{{ section.id }} .Multi-column-video-autoplay-item {
      display: flex;
      flex-direction: column;


    }
    .Multi-column-video-autoplay-{{ section.id }} .Multi-column-video-autoplay-item-video-container {
      display: flex;
      width: 100%;
      border-radius: 20px;
      overflow: hidden;
    }

    .Multi-column-video-autoplay-{{ section.id }} .Multi-column-video-autoplay-item-content {
      display: flex;
      flex-direction: column;
      align-items: {{section.settings.content_alignment}};
    }

    .Multi-column-video-autoplay-{{ section.id }} .Multi-column-video-autoplay-item-title {
      color: {{section.settings.heading_color}};
      line-height: 1.26;
      margin-top: {{section.settings.heading_margin_top}}px;
      margin-bottom: {{section.settings.heading_margin_bottom}}px;
    }

    .Multi-column-video-autoplay-{{ section.id }} .Multi-column-video-autoplay-item-desc {
      color:{{section.settings.subheading_color}};
      text-align: center;
      line-height: 1.38;
    }

    @media screen and (max-width: 768px) {
        .Multi-column-video-autoplay-{{ section.id }} .Multi-column-video-autoplay-wrap {
        padding-top: {{section.settings.padding_top_mobile}}px;
        padding-bottom: {{section.settings.padding_bottom_mobile}}px;
      }

      .Multi-column-video-autoplay-{{ section.id }} .Multi-column-video-autoplay-title-wrap {
        align-items: center;

      }

      .Multi-column-video-autoplay-{{ section.id }} .Multi-column-video-autoplay-title {
        text-align: center;
        font-size: 26px;
        font-weight: 800;
        line-height: 1.27;
        margin-bottom: {{section.settings.title_margin_bottom_mobile}}px;
      }

      .Multi-column-video-autoplay-{{ section.id }} .Multi-column-video-autoplay-subtitle {
        font-size: 14px;
        font-weight: 400;
        line-height: 1.38;
        margin-bottom: {{section.settings.subtitle_margin_bottom_mobile}}px;
      }

      .Multi-column-video-autoplay-{{ section.id }} .Multi-column-video-autoplay-content {
        grid-template-columns: repeat({{ section.settings.slides_per_view_mobile}}, 1fr);
        gap: {{section.settings.card_gap_mobile}}px;
      }



      .Multi-column-video-autoplay-{{ section.id }} .Multi-column-video-autoplay-item-video-container {
        border-radius: 10px;
      }

      .Multi-column-video-autoplay-{{ section.id }} .Multi-column-video-autoplay-item-content {

        align-items: {{section.settings.content_alignment_mobile}};
      }
      .Multi-column-video-autoplay-{{ section.id }} .Multi-column-video-autoplay-item-title {
        font-size: {{section.settings.heading_size_mobile}}px;
        line-height: 1.26;
        margin-top: {{section.settings.heading_margin_top_mobile}}px;
        margin-bottom: {{section.settings.heading_margin_bottom_mobile}}px;
      }

      .Multi-column-video-autoplay-{{ section.id }} .Multi-column-video-autoplay-item-desc {
        font-size: {{section.settings.subheading_size_mobile}}px;
        line-height: 1.38;
      }
    }
</style>
<section
  id="{% if section.settings.nav_id != empty %}custom-nav-{{ section.settings.nav_id }}{% endif %}"
  class="Multi-column-video-autoplay-{{ section.id }}"
>
  <div class="Multi-column-video-autoplay-wrap">
    <div class="Multi-column-video-autoplay-container">
      {% if section.settings.title != empty %}
        <div class="Multi-column-video-autoplay-title-wrap">
          <div
            class="Multi-column-video-autoplay-title {{ section.settings.title_size }} {{ section.settings.title_font_family }}"
          >
            {{ section.settings.title }}
          </div>
          {% if section.settings.subtitle != empty %}
            <div class="Multi-column-video-autoplay-subtitle {{ section.settings.subtitle_size }} {{ section.settings.subtitle_font_family }}">
              {{ section.settings.subtitle }}
            </div>
          {% endif %}
        </div>
      {% endif %}
      <div class="Multi-column-video-autoplay-content">
        {% for block in section.blocks %}
          {% case block.type %}
            {% when 'shopifyVideo' %}
              {% assign video_pc = block.settings.video_pc %}
              {% assign video_mb = block.settings.video_mb | default: video_pc %}
              {% if video_pc %}
                <div class="Multi-column-video-autoplay-item">
                  <div class="Multi-column-video-autoplay-item-video-container">
                    <video
                      style="aspect-ratio: {{ video_pc.aspect_ratio }};"
                      class="lazy"
                      preload="none"
                      width="100%"
                      height="auto"
                      data-poster="{{ video_pc.preview_image | image_url: width: 700 }}"
                      autoplay
                      loop
                      muted
                      playsinline
                      webkit-playsinline
                      x-webkit-airplay="allow"
                      x5-playsinline
                      x5-video-player-type="h5"
                      x5-video-orientation="portrait"
                      x5-video-player-fullscreen="false"
                    >
                      {% for source in video_pc.sources %}
                        <source
                          data-src="{{ source.url }}"
                          type="{{source.mime_type }}"
                          media="(min-width: 768px)"
                        >
                      {% endfor %}
                      {% for source in video_mb.sources %}
                        <source
                          data-src="{{ source.url }}"
                          type="{{source.mime_type }}"
                          media="(max-width: 769px)"
                        >
                      {% endfor %}
                      Your browser does not support the video tag.
                    </video>
                  </div>
                  <div class="Multi-column-video-autoplay-item-content">
                    {% if block.settings.heading != empty %}
                      <div class="Multi-column-video-autoplay-item-title {{ section.settings.heading_size }} {{ section.settings.heading_font_family }}">
                        {{ block.settings.heading }}
                      </div>
                    {% endif %}
                    {% if block.settings.subheading != empty %}
                      <div class="Multi-column-video-autoplay-item-desc {{ section.settings.subheading_size }} {{ section.settings.subheading_font_family }}">
                        {{ block.settings.subheading }}
                      </div>
                    {% endif %}
                  </div>
                </div>
              {% endif %}
          {% endcase %}
        {% endfor %}
      </div>
    </div>
  </div>
</section>
