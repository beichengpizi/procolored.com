    .christmas-product-tab-{{ section.id }} .christmas-product-tab-product-label-discount-text {
      color: #fff;
      font-size: 16px;
      font-weight: 800;
      line-height: 1;
      display: flex;
      justify-content: center;
      gap: 4px;
      align-items: center;
    }

    .christmas-product-tab-{{ section.id }} .christmas-product-tab-product-label-discount-text-value {
      font-size: 20px;
    }

    .christmas-product-tab-{{ section.id }} .christmas-product-tab-product-label-text {
      color: #1F6B4D;
      font-size: 16px;
      font-weight: 700;
      line-height: 1.4;
    }

    .christmas-product-tab-{{ section.id }} .christmas-product-tab-product-image {
      flex: 1;
      overflow: hidden;
      padding: 0;
      width: 100%;
    }

    .christmas-product-tab-{{ section.id }} .christmas-product-tab-product-image img {
      width: 100%;
      max-width: 100%;
      object-fit: cover;
      aspect-ratio: 1;
    }

    .christmas-product-tab-{{ section.id }} .christmas-product-tab-product-image .pc {
      display: flex;
    }

    .christmas-product-tab-{{ section.id }} .christmas-product-tab-product-image .mb {
      display: none;
    }
    .christmas-product-tab-{{ section.id }}  .christmas-product-tab-product-main-image-mb{
      display: none;
    }
    .christmas-product-tab-card-mySwiper-{{section.id}} .pc{
      display: flex;
      {% if section.settings.position == 'row'%}
        border-top-left-radius: 20px;
        border-bottom-left-radius: 20px;
        {% else %}
        border-top-right-radius: 20px;
        border-bottom-right-radius: 20px;
    {% endif %}

  }
  .christmas-product-tab-card-mySwiper-{{section.id}} .mb{
      display: none;
  }
  .christmas-product-tab-card-mySwiper-{{section.id}}{
      width: 100%;
      overflow: hidden;
  }
  .christmas-product-tab-card-mySwiper-{{section.id}} .swiper-slide{
      width: 100% !important;
    }
  .christmas-product-tab-card-mySwiper-{{section.id}}    .swiper-pagination-bullet {
      width: 20%;
      height: 4px;
      display: inline-block;
      border-radius: 99px;
      background: #D9D9D9;
      opacity: 1;
  }
    /* 增加动画效果：5秒从左至右的加载条 */
    .christmas-product-tab-card-mySwiper-{{section.id}} .swiper-pagination-bullet-active {
        opacity: 1;
        background: #D9D9D9;
        position: relative;
        overflow: hidden;
    }
    .christmas-product-tab-card-mySwiper-{{section.id}} .swiper-pagination-bullet-active::after {
        content: '';
        display: block;
        position: absolute;
        left: 0;
        top: 0;
        height: 100%;
        width: 0;
        background: #fff;
        border-radius: 99px;
        animation: christmas-tab-bar-fill-{{section.id}} 3.2s linear forwards;
        z-index: 1;
    }

    @keyframes christmas-tab-bar-fill-{{section.id}} {
        from {
            width: 0%;
        }
        to {
            width: 100%;
        }
    }
    .christmas-product-tab-card-mySwiper-{{section.id}} .video-wrap{
        height: 100%;
        background: #000;
        }
    .christmas-product-tab-{{ section.id }} .christmas-product-tab-product-content {
      position: relative;
      padding-left: {{section.settings.content_padding_left_and_right}}px;
      padding-right: {{section.settings.content_padding_left_and_right}}px;
      flex: 0 0 {{section.settings.content_space_percent}}%;
      display: flex;
      align-items: center;
      gap: 33px;
      width: {{section.settings.content_space_percent}}%;

    }
    .christmas-product-tab-{{ section.id }} .christmas-product-tab-product-main-image{
        width: 200px;
        max-width: 100%;
        flex-shrink: 0;
    }
    .christmas-product-tab-{{ section.id }} .christmas-product-tab-product-main-image .pc{
         display: flex;
    }
    .christmas-product-tab-{{ section.id }} .christmas-product-tab-product-main-image .mb{
        display: none;
    }
    .christmas-product-tab-{{ section.id }} .christmas-product-tab-product-main-content{
        display: flex;
        flex-direction: column;
        justify-content: center;
        width: {{section.settings.content_box_text_percent}}%;
        {% comment %} flex:1; {% endcomment %}
    }
    .christmas-product-tab-{{ section.id }} .christmas-product-tab-product-name {
      color: {{ section.settings.product_title_color }};
      text-align: left;
      font-size: 20px;
      font-weight: 800;
      line-height: 1.3;
      max-width: {{section.settings.content_box_text_percent}}%;
      {% if section.settings.product_divider_line %}
        padding-bottom: 10px;
        border-bottom: 1px solid #E7E7E7;
      {% endif %}
    }

    .christmas-product-tab-{{ section.id }} .christmas-product-tab-product-features {
      margin-top: 4px;
      color: #3D3D3D;
