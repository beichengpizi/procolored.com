<style>
    /* black friday Official Promotion product1  start  */
    .christmas-product-tab-{{ section.id }} .christmas-product-tab-wrapper {
      background-color: {{ section.settings.bg_color }};
      padding: {{ section.settings.padding_top }}px 0px {{ section.settings.padding_bottom }}px 0px;
      overflow: hidden;
    }
    .christmas-product-tab-{{ section.id }}  .christmas-second-banner-top-1 {
      width: 100%;
      max-width: 100%;
      margin-bottom:60px;

  }

  .christmas-product-tab-{{ section.id }} .christmas-second-banner-top-1 .pc {
      display: flex;
  }

  .christmas-product-tab-{{ section.id }} .christmas-second-banner-top-1 .mb {
      display: none;
  }

  .christmas-product-tab-{{ section.id }}  .christmas-second-banner-top-1 img {
      width: 100%;
      max-width: 100%;
  }
    .christmas-product-tab-{{ section.id }} .christmas-product-tab-container {
        position: relative;
    }
    .christmas-product-tab-{{ section.id }} .christmas-product-tab-container-left-snow{
        position: absolute;
        left: 0;
        top: 0;
        width: 19.5%;
        height: 100%;

    }
    .christmas-product-tab-{{ section.id }} .christmas-product-tab-container-right-snow{
        position: absolute;
        right: 0;
        top: 0;
        width: 19.5%;
        height: 100%;
    }

    .christmas-product-tab-{{ section.id }} .christmas-product-tab-title-wrapper {
      width: 100%;
      display: flex;
      flex-direction: column;
      align-items: center;
      text-align: center;
      margin-bottom: 50px;
      max-width: 1248px;
      margin-left: auto;
      margin-right: auto;
      padding: 0 24px;
    }

    .christmas-product-tab-{{ section.id }} .christmas-product-tab-title {
      color: #fece67;
      text-align: center;
      font-size: 40px;
      font-weight: 800;
      line-height: 1.5;
      margin-bottom: 20px;
    }

    .christmas-product-tab-{{ section.id }} .christmas-product-tab-subtitle {
      color: rgba(255, 255, 255, 0.90);
      text-align: center;
      font-size: 20px;
      font-weight: 400;
      line-height: 1.3;
      {%- if section.settings.desc != empty or section.blocks.size > 1 -%}
      margin-bottom: 30px;
      {% endif %}
    }

    .christmas-product-tab-{{ section.id }} .christmas-product-tab-desc {
      color: #fff;
      text-align: center;
      font-size: 24px;
      font-weight: 800;
      line-height: 1.5;
      margin-bottom: 25px;
    }

    .christmas-product-tab-{{ section.id }} .christmas-product-tab-desc span {
      color: #fece67;
    }

    .christmas-product-tab-{{ section.id }} .christmas-products-main-banner-box {
      max-width: 639px;
      margin-bottom: 50px;
    }

    .christmas-product-tab-{{ section.id }} .christmas-products-main-banner-box img {
      display: flex;
      width: 100%;
      max-width: 100%;
    }

    .christmas-product-tab-{{ section.id }} .christmas-products-main-banner-box .pc {
      display: flex;
    }

    .christmas-product-tab-{{ section.id }} .christmas-products-main-banner-box .mb {
      display: none;
    }

    .christmas-product-tab-{{ section.id }} .christmas-product-tab-tabs {
      display: flex;
      align-items: center;
      justify-content: center;
      width: 100%;
      max-width: {{section.settings.content_box_text_percent}}%;
      {% comment %} margin-left: auto;
      margin-right: auto;
      white-space: nowrap;
      overflow-x: auto;
      -webkit-overflow-scrolling: touch;
      scrollbar-width: none;
      touch-action: pan-x; {% endcomment %}
      background: #fff;
      margin-top: 16px;
      {% comment %} border-radius: 99px; {% endcomment %}
    }
    .christmas-product-tab-{{ section.id }} .christmas-product-tab-tab-dropdown-wrapper{
        width: 100%;

    }
    .christmas-product-tab-{{ section.id }} .christmas-product-tab-tab {
      cursor: pointer;
      text-align: center;
      color: #000;
      font-size: 14px;
      font-weight: 400;
      line-height: 1;
      padding: 16px 20px;
      {% comment %} -webkit-tap-highlight-color: transparent;
      /* 消除移动端点击背景 */
      -webkit-touch-callout: none;
      outline: none; {% endcomment %}
    }

    .christmas-product-tab-{{ section.id }} .christmas-product-tab-tab.active {
      color: #000;
      font-weight: 700;
      {% comment %} background: #A31C22; {% endcomment %}
    }

    .christmas-product-tab-{{ section.id }} .christmas-product-tab-banner {
      width: 100%;
      max-width: 1232px;
      margin-left: auto;
      margin-right: auto;
    }

    .christmas-product-tab-{{ section.id }} .christmas-product-tab-banner-box {
      display: none;
      position: relative;
      padding: 0 16px;
    }

    .christmas-product-tab-{{ section.id }} .christmas-product-tab-banner-box.active {
      display: flex;
    }

    .christmas-product-tab-{{ section.id }} .christmas-product-tab-banner img {
      width: 100%;
      max-width: 100%;
      border-radius: 16px;
    }

    .christmas-product-tab-{{ section.id }} .christmas-product-tab-banner .pc {
      display: flex;
    }

    .christmas-product-tab-{{ section.id }} .christmas-product-tab-banner .mb {
      display: none;
    }

    .christmas-product-tab-{{ section.id }} .christmas-product-tab-banner-box-content {
      position: absolute;
      bottom: 40px;
      width: 100%;
      display: flex;
      flex-direction: column;
      align-items: center;
    }

    .christmas-product-tab-{{ section.id }} .christmas-product-tab-banner-box-content-title {
      color: #fff;
      text-align: center;
      font-size: 22px;
      font-weight: 400;
      line-height: 1.27;
      margin-bottom: 5px;
    }

    .christmas-product-tab-{{ section.id }} .christmas-product-tab-banner-box-content-desc {
      color: #fff;
      text-align: center;
      font-size: 24px;
      font-weight: 800;
      line-height: 1.16;
    }

    .christmas-product-tab-{{ section.id }} .christmas-product-tab-content {
      max-width: 1200px;
      margin-left: auto;
      margin-right: auto;

    }

    .christmas-product-tab-{{ section.id }} .christmas-product-tab {
      {% comment %} display: none; {% endcomment %}
       opacity: 0;
       height: 0;
      grid-template-columns: repeat(1, 1fr);
      grid-gap: {{ section.settings.card_gap }}px;
    }

    .christmas-product-tab-{{ section.id }} .christmas-product-tab.active {
      display: grid;
      opacity: 1;
      height: auto;
    }

    .christmas-product-tab-{{ section.id }} .christmas-product-tab-card {
      position: relative;
      display: flex;
      {% comment %} overflow: hidden; {% endcomment %}
      border-radius: 20px;
      background-color: {{ section.settings.card_bg_color }};
      background: {{ section.settings.card_background_gradient_color }};
      {% if section.settings.position == 'row'%}
        flex-direction: row;
        {% else %}
        flex-direction: row-reverse;
    {% endif %}
    }

    .christmas-product-tab-{{ section.id }} .christmas-product-tab-product-label {
      display: flex;
      margin-bottom: 4px;
      width: 100%;
    }

