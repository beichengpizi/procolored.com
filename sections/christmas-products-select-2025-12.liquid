<script>
  // 监听页面DOM加载，初始化swiper与tab交互
  {% comment %}
    修复：之前只作用于初始active的tab，切换后没有更新active类导致jQuery选择器始终只对初始active操作。
    方案：每次select切换，都要移除所有christmas-product-tab的active类，再切到对应的索引。
    - 这样每次切换都会正常触发显示和隐藏，无论当前active在哪。
  {% endcomment %}

  document.addEventListener('DOMContentLoaded', function () {
    // swiper实例管理，防止重复初始化
    let swiperInstances = [];

    function initAllSwipers() {
      // 销毁旧的swiper实例
      if (swiperInstances && swiperInstances.length > 0) {
        swiperInstances.forEach(function (swiper) {
          if (swiper && typeof swiper.destroy === 'function') {
            swiper.destroy(true, true);
          }
        });
        swiperInstances = [];
      } else {
        swiperInstances = [];
      }
      // 初始化所有当前可见的swiper
      document
        .querySelectorAll('.christmas-product-tab-card-mySwiper-{{section.id}}')
        .forEach(function (swiperEl) {
          var swiperInstance = new Swiper(swiperEl, {
            lazy: {
              loadPrevNext: true,
            },
            loop: true,
            autoplay: {
              delay: 3000,
              stopOnLastSlide: false,
              disableOnInteraction: true,
            },
            effect: 'fade',
            fadeEffect: {
              crossFade: true,
            },
            observer: true,
            observeParents: true,
            noSwiping: true,
            slidesPerView: 1,
            spaceBetween: 0,
            pagination: {
              el: swiperEl.closest('.christmas-product-tab')
                ? swiperEl
                    .closest('.christmas-product-tab')
                    .querySelector('.swiper-pagination-{{section.id}}')
                : '.swiper-pagination-{{section.id}}',
              clickable: true,
            },
          });
          swiperInstances.push(swiperInstance);
        });
    }

    // 首次初始化swiper（页面默认active的tab）
    initAllSwipers();

    // 获取所有christmas-product-tab-tabs容器（有多个section时都生效）
    {% comment %} const tabContainers = document.querySelectorAll('.christmas-product-tab-tabs');
    if (!tabContainers || tabContainers.length === 0) return;

    tabContainers.forEach(function (tabContainer) {
      // select直接取tabContainer下的select，因为.tabs外面包裹了wrapper
      const select = tabContainer.querySelector('select.christmas-product-tab-tab');
      if (!select) return;

      // 取到对应的card和banner盒子，但必须限制到本section范围，否则多个section会串
      // section 元素, .christmas-product-tab-tabs总会在某个section的唯一实例下
      const sectionRoot =
        tabContainer.closest('section[id^="christmas-product-tab"]') ||
        tabContainer.closest('section');
      // 获取本section下所有tab/main tab卡片和banner
      const cardContainer = sectionRoot
        ? sectionRoot.querySelectorAll('.christmas-product-tab')
        : [];
      const bannerBoxes = sectionRoot
        ? sectionRoot.querySelectorAll('.christmas-product-tab-banner-box')
        : [];
      // 如果有额外自定义tab元素高亮也一并处理
      const tabs = tabContainer.querySelectorAll('.christmas-product-tab-tab');

      // 移动端 select 切换
      select.addEventListener('change', function (e) {
        const idx = select.selectedIndex;
        // 清除所有tab active
        cardContainer.forEach((cardbox, i) => {
          cardbox.classList.toggle('active', i === idx);
        });
        // 切换banner高亮
        if (bannerBoxes && bannerBoxes.length > 0) {
          bannerBoxes.forEach((bannerBox, i) => {
            bannerBox.classList.toggle('active', i === idx);
          });
        }
        // 兼容有没有可单独tab高亮元素
        if (tabs && tabs.length > 0) {
            console.log('tabs', tabs);
          tabs.forEach((t, i) => {
            // 延迟加载 option 操作，提升性能

              // 移除 t 下所有 option 的 selected 属性
            const options = t.querySelectorAll('option');
              // 仅将选中的 option 变为 selected 状态
              options.forEach((option, optIdx) => {
                if (optIdx === idx) {
                    console.log('option', option);
                  option.setAttribute('selected', 'selected');
                } else {
                  option.removeAttribute('selected');
                }
              });
              t.classList.toggle('active', i === idx);

          });
        }

      });


    }); {% endcomment %}
  });
</script>

<script>
  function formatUtcTime(v) {
    let date = new Date(v);
    return (
      date.getFullYear() +
      '/' +
      (date.getMonth() + 1) +
      '/' +
      (date.getDate() < 10 ? '0' + date.getDate() : date.getDate()) +
      ' ' +
      (date.getHours() < 10 ? '0' + date.getHours() : date.getHours()) +
      ':' +
      (date.getMinutes() < 10 ? '0' + date.getMinutes() : date.getMinutes()) +
      ':' +
      (date.getSeconds() < 10 ? '0' + date.getSeconds() : date.getSeconds())
    );
  }
  $(function () {
    if ($('.christmas-product-tab-{{ section.id }}  [data-countdown]')) {
      $('.christmas-product-tab-{{ section.id }}  [data-countdown]').each(function () {
