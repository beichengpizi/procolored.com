<!-- 多个轮播 图文 -->
<div id="detail-section6-{{section.id}}">
  <div class="multiple-swiper-wrap">
    {% if section.settings.title != empty %}
      <h3
        class="heading wow fadeInUp {{ section.settings.title_size }} {{ section.settings.title_font_family }}"
        data-wow-delay=".8s"
      >
        {{ section.settings.title }}
      </h3>
    {% endif %}

    <!-- 多个轮播模式 -->
    <div class="detail-section6-mySwiper-{{section.id}}">
      <div class="swiper-wrapper">
        {%- for block in section.blocks -%}
          {% case block.type %}
            {%- when 'multipleImageTextSlider' -%}
              {% style %}
                #detail-section6-{{section.id}} .multiple-swiper-wrap .swiper-slide .img-wrap-{{block.id}}{

                    {% if section.settings.show_icon_model %}
                      width: {{block.settings.icon_width_size}}px;

                    {% else %}

                    {% endif %}
                }
                @media screen and (min-width: 320px) and (max-width: 769px){
                  #detail-section6-{{section.id}}  .icon-image-wrap{
                    min-height:{{  section.settings.icon_height_size_mobile  }}px;
                  }
                  #detail-section6-{{section.id}} .multiple-swiper-wrap .swiper-slide .img-wrap-{{block.id}}{

                    {% if section.settings.show_icon_model %}
                      width: {{ block.settings.icon_width_size_mobile }}px;

                    {% else %}

                    {% endif %}
                    }
                }
              {% endstyle %}
              <div class="swiper-slide">
                <div class="swiper-slide-content">
                  <!-- Required swiper-lazy class and image source specified in data-src attribute -->
                  {%- if block.settings.button_link -%}
                    <a
                      class="image-container"
                      href="{{block.settings.button_link}}"
                    >
                  {% endif %}
                  {%- if section.settings.show_icon_model -%}
                    <span
                      class="icon-image-wrap"
                    >
                      <span
                        class="icon-image-container"
                      >
                  {% endif %}
                  {%- if block.settings.image_pc -%}
                    {% assign image = block.settings.image_pc %}
                    {% assign size = 100 | divided_by: section.settings.display_number_per_line | round %}
                    <img
                      style="aspect-ratio: {{ block.settings.image_pc.aspect_ratio }};"
                      data-sizes="{{ size }}vw"
                      width="{{ size }}vw"
                      data-srcset="
                        {%- if image.width >= 200 -%}{{ image | image_url: width: 200 }} 200w,{%- endif -%}
                        {%- if image.width >= 375 -%}{{ image | image_url: width: 375 }} 375w,{%- endif -%}
