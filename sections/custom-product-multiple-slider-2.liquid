          #detail-section6-{{section.id}}  .h4,
          #detail-section6-{{section.id}}  .prose h4:not(.h0, .h1, .h2, .h3, .h4, .h5, .h6) {
            font-size: var(--text-h4-n) ;
            line-height: 1.3;
          }

          #detail-section6-{{section.id}}  h5,
          #detail-section6-{{section.id}}  .h5,
          #detail-section6-{{section.id}}  .prose h5:not(.h0, .h1, .h2, .h3, .h4, .h5, .h6) {
            font-size: var(--text-h5-n) ;
            line-height: 1.4;
          }

          #detail-section6-{{section.id}}  h6,
          #detail-section6-{{section.id}}  .h6,
          #detail-section6-{{section.id}}  .prose h6:not(.h0, .h1, .h2, .h3, .h4, .h5, .h6) {
            font-size: var(--text-h6-n) ;
            line-height: 1.4;
          }
          /* 重置样式 end*/
            /* 多个轮播 图文 */
            #detail-section6-{{section.id}}{
            background-color: {{section.settings.bg_color}};
            }
            #detail-section6-{{section.id}} .multiple-swiper-wrap {
              position: relative;
                {% if section.settings.show_full_width %}
                  max-width:100%;
                  padding: {{section.settings.padding_top}}px 50px {{section.settings.padding_bottom}}px 50px;
                {% else %}
                max-width:1200px;
                margin:auto;
                padding: {{section.settings.padding_top}}px 0 {{section.settings.padding_bottom}}px 0 ;
                {% endif %}
                {% if section.settings.swiper_button_position == '50' %}
                  padding-left: calc({{section.settings.swiper_button_margin}}px + {{section.settings.swiper_button_size}}px);
                  padding-right: calc({{section.settings.swiper_button_margin}}px + {{section.settings.swiper_button_size}}px);
                {% endif %}

            }

            #detail-section6-{{section.id}} .multiple-swiper-wrap .heading {
                text-align: center;
                color: {{section.settings.heading_text}};
                margin-bottom: {{section.settings.title_to_content_padding}}px;




            }

            #detail-section6-{{section.id}} .multiple-swiper-wrap .detail-section6-mySwiper-{{section.id}} {
                margin-left: auto;
                margin-right: auto;
                position: relative;
                overflow: hidden;
                list-style: none;
                z-index: 1;

            }
            {% if section.settings.swiper_button_position == '50' %}
              .detail-section6-mySwiper-{{section.id}} .swiper-slide:first-child {
                margin-left: 6px;
              }
            {% endif %}


            #detail-section6-{{section.id}} .multiple-swiper-wrap .swiper-slide-content {
                display: flex;
                flex-direction: column;
                align-items: center;
                justify-content: center;
                background: {{section.settings.card_bg_color}};
                padding: {{section.settings.card_padding_top_bottom}}px {{section.settings.card_padding_left_right}}px;
                border-radius: {{section.settings.card_radius_size}}px;
                width:99%;
            }



            #detail-section6-{{section.id}} .multiple-swiper-wrap .swiper-slide-content .content {

                text-align: {{ section.settings.text_position }};;
                color: {{section.settings.desc_text}};
                font-size:{{section.settings.desc_text_font_size}}px;
                line-height: 1.2;

            }
            #detail-section6-{{section.id}} .multiple-swiper-wrap .swiper-slide-content .image-container{
              position: relative;
              display: inline-block;
              overflow: hidden;
            }
            #detail-section6-{{section.id}} .multiple-swiper-wrap .swiper-slide-content .caption {
              position: absolute;
              bottom: {{section.settings.content_heading_top}}px;
              left: 0;
              right: 0;
              color: white;
              padding: 20px 0;
              transform: translateY(100%);
