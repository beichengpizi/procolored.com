<script defer type="text/javascript">
  $(document).ready(function () {
    const is_swiper_autoplay = {{section.settings.is_swiper_autoplay}};
    const show_swiper_mobile = {{section.settings.show_swiper_mobile}};
    const display_number_per_line = {{section.settings.display_number_per_line | plus: 0 }};
    const display_number_per_line_mobile = {{section.settings.display_number_per_line_mobile | plus: 0 }};
    const totalNums = {{section.blocks.size}};
    console.log('totalNums==', totalNums)
    console.log('display_number_per_line==', display_number_per_line)
    console.log(
      'display_number_per_line_mobile==',
      display_number_per_line_mobile
    )
    console.log('show_swiper_mobile==', show_swiper_mobile)
    // 鼠标悬停时显示文字
    $('.detail-section6-mySwiper-{{section.id}} .image-container').hover(
      function () {
        $(this).find('.caption').stop().animate(
          {
            transform: 'translateY(0)',
          },
          300
        )
      },
      function () {
        $(this).find('.caption').stop().animate(
          {
            transform: 'translateY(100%)',
          },
          300
        )
      }
    )
    if (!show_swiper_mobile && isMobile()) {
      console.log('移动端隐藏轮播')
      $('.detail-section6-mySwiper-{{section.id}}').css('display', 'none')
    } else {
      console.log('PC端显示轮播')
      $('.detail-section6-mySwiper-{{section.id}}').css('display', 'block')
      $('.multiple-flex-box-{{section.id}}').css('display', 'none')
    }

    if (!isMobile() || (show_swiper_mobile && isMobile())) {

      // 新增变量用于保存swiper实例
      var mySwiper = new Swiper('.detail-section6-mySwiper-{{section.id}}', {
        loop: true,
        loopedSlides: 10, // slidesPerView设置为非整数（如1.2）时，可能会出现循环问题添加此行，指定循环所需的额外幻灯片数量
        observer: true, //开启动态检查器，监测swiper和slide
        noSwiping: true,
        // autoplay: {
        //     delay: 5000,
        //     disableOnInteraction: false,
        // },
        slidesPerView: display_number_per_line,
        spaceBetween: 24,
        pagination: {
          el: '.swiper-pagination-{{section.id}}',
          clickable: true,
        },
        navigation: {
          nextEl: '.swiper-button-next-{{section.id}}',
          prevEl: '.swiper-button-prev-{{section.id}}',
        },

        scrollbar: {
          el: '.swiper-scrollbar-{{section.id}}',
          hide: false,
        },
        on: {
          init: function (swiper) {
            if (is_swiper_autoplay) {
              // 启用自动播放
              this.autoplay.start()
              this.params.autoplay.delay = 2000;
            } else {
              this.autoplay.stop()
            }
            if (!isMobile()) {
              if (totalNums === display_number_per_line) {
                console.log('禁止拖动')
                // 禁止拖动
                this.allowTouchMove = false
              } else {
                // 允许拖动
                console.log('允许拖动')
                this.allowTouchMove = true
              }
            } else {
              if (totalNums === display_number_per_line_mobile) {
                console.log('禁止拖动')
                // 禁止拖动
                this.allowTouchMove = false
              } else {
                // 允许拖动
                console.log('允许拖动')
                this.allowTouchMove = true
              }
            }
            // 新增：为分页点添加hover事件，鼠标移入切换到对应slide
            setTimeout(function() {
              var bullets = document.querySelectorAll('.swiper-pagination-{{section.id}} .swiper-pagination-bullet');
              bullets.forEach(function(bullet, idx) {
                bullet.addEventListener('mouseenter', function() {
                  // 切换到对应slide
                  mySwiper.slideToLoop(idx, 300, false);
                });
              });
            }, 0);
          },
          slideChangeTransitionEnd: function (swiper) {},
          imagesReady: function () {
            this.update(); // 更新 Swiper
          },
          slideChange: function () {
            // 移除所有分页点的动画类
            document.querySelectorAll('.swiper-pagination-{{section.id}} .swiper-pagination-bullet').forEach(bullet => {
                bullet.classList.remove('page-dot-progress');
            });
            // 为当前活动的分页点添加动画类
            const activeBullet = document.querySelector('.swiper-pagination-{{section.id}} .swiper-pagination-bullet-active');
            if (activeBullet) {
                activeBullet.classList.add('page-dot-progress');
            }
          }
        },

        breakpoints: {
          //当宽度小于等于768时显示
          768: {
            spaceBetween: 15,
            slidesPerView: display_number_per_line_mobile,
          },
        },
      })

      setTimeout(() => {
        if (totalNums > display_number_per_line) {
          $('.swiper-button-next-{{section.id}}').removeClass(
            'swiper-button-disabled'
          )
        }
      }, 0)

    }
  })
</script>

