<script>
  // 增加吸附底部功能
  // 增加当bottom-nav-jump-section顶部区域小于0时,取消吸附
  document.addEventListener('DOMContentLoaded', function () {
    // 目标区域ID
    const targetId = 'k13-lite-hassle-free-maintenance';
    const lastSection = document.getElementById('lucky-wheel-activity-faq-section');
    const navSection = document.getElementById('bottom-nav-jump-section');
    let lastFixed = false;

    function checkNavFixed() {
      const target = document.getElementById(targetId);
      if (!target || !navSection || !lastSection) return;
      const rect = target.getBoundingClientRect();

      // 获取bottom-nav-jump-section自身的rect
      const lastRect = lastSection.getBoundingClientRect();

      // rect.top < 0 说明目标区域已滚出视窗
      // navRect.top < 0 说明底部吸附条自身顶部也已滚出视窗（即页面已滚到底部吸附条之下）
      if (rect.top < 0 && lastRect.top >= 0) {
        if (!lastFixed) {
          navSection.classList.add('bottom-nav-jump-fixed-show');
          lastFixed = true;
        }
      } else {
        if (lastFixed) {
          navSection.classList.remove('bottom-nav-jump-fixed-show');
          lastFixed = false;
        }
      }
      // 如果bottom-nav-jump-section自身顶部小于0，强制移除吸附
      if (lastRect.top < 0) {
        navSection.classList.remove('bottom-nav-jump-fixed-show');
        lastFixed = false;
      }
    }

    // 初始检查
    checkNavFixed();
    // 监听滚动
    window.addEventListener('scroll', checkNavFixed);
    // 监听窗口大小变化
    window.addEventListener('resize', checkNavFixed);
  });
