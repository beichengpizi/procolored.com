            k13_lite_easy_animating = false;
            // 动画结束后，如果当前在边界则允许页面滚动
            const currentIsAtBoundary = (k13_lite_easy_index === 0 || k13_lite_easy_index === k13_lite_easy_slides.length - 1);
            k13_lite_easy_forcePrevent = !currentIsAtBoundary;
            k13_lite_easy_setPageScrollLock(!currentIsAtBoundary);

            // 新增：恢复滚动位置并解锁
            if (k13_lite_easy_scrollPositionLocked && !currentIsAtBoundary) {
              requestAnimationFrame(() => {
                window.scrollTo(0, k13_lite_easy_lastScrollY);
                k13_lite_easy_scrollPositionLocked = false;
              });
            } else {
              k13_lite_easy_scrollPositionLocked = false;
            }

            // 新增：动画结束后处理wheel队列
            k13_lite_easy_processWheelQueue();
          }, 350);
          return true;
        }

        function k13_lite_easy_setCooling(ms = 200) {
          k13_lite_easy_cooling = true;
          setTimeout(() => {
            k13_lite_easy_cooling = false;
          }, ms);
        }

        function k13_lite_easy_safePrevent(e) {
          if (e && e.cancelable) e.preventDefault();
        }

        // 新增：处理wheel队列
        function k13_lite_easy_processWheelQueue() {
          if (k13_lite_easy_wheelProcessing) return;
          if (!k13_lite_easy_shouldIntercept) {
            k13_lite_easy_wheelQueue = [];
            return;
          }
          if (k13_lite_easy_animating || k13_lite_easy_cooling) return;
          if (k13_lite_easy_wheelQueue.length === 0) return;
          k13_lite_easy_wheelProcessing = true;
          const dir = k13_lite_easy_wheelQueue.shift();
          const atBoundary = (k13_lite_easy_index === k13_lite_easy_slides.length - 1 && dir > 0) || (k13_lite_easy_index === 0 && dir < 0);
          if (!atBoundary) {
            k13_lite_easy_go(dir);
            k13_lite_easy_setCooling();
            // 动画结束后会再次调用processWheelQueue
          } else {
            // 边界时，允许页面滚动，清空队列
            k13_lite_easy_wheelQueue = [];
            // 确保在边界时允许页面滚动
            k13_lite_easy_forcePrevent = false;
          }
          k13_lite_easy_wheelProcessing = false;
        }

        // wheel事件拦截
        function k13_lite_easy_onWheel(e) {
          if (!k13_lite_easy_shouldIntercept) return;

          // 忽略主要为横向的滚动
          if (Math.abs(e.deltaY) < Math.abs(e.deltaX)) return;

          const dir = e.deltaY > 0 ? 1 : -1;
          const atBoundary = (k13_lite_easy_index === k13_lite_easy_slides.length - 1 && dir > 0) || (k13_lite_easy_index === 0 && dir < 0);

          // 如果在边界，始终允许页面滚动
          if (atBoundary) {
            // 边界时，允许页面滚动
            // 不阻止默认
            // 清空队列
            k13_lite_easy_wheelQueue = [];
            k13_lite_easy_forcePrevent = false;
            k13_lite_easy_setPageScrollLock(false);
            return;
          }

          // 动画或冷却中，阻止页面滚动
          if (k13_lite_easy_animating || k13_lite_easy_cooling) {
            k13_lite_easy_safePrevent(e);
            k13_lite_easy_wheelQueue.push(dir);
            k13_lite_easy_setPageScrollLock(true);
            return;
          }

          // 非边界且非动画状态，切换图片
          k13_lite_easy_safePrevent(e);
          // 新增：将滚动方向加入队列并立即处理
          k13_lite_easy_wheelQueue.push(dir);
          k13_lite_easy_processWheelQueue();
          k13_lite_easy_setPageScrollLock(true);
        }
        window.addEventListener('wheel', k13_lite_easy_onWheel, { passive: false, capture: true });

        // 触摸事件拦截
        k13_lite_easy_gallery.addEventListener('touchstart', (e) => {
          if (!k13_lite_easy_shouldIntercept) return;
          const t = e.touches && e.touches[0];
          if (!t) return;
          k13_lite_easy_touchStartY = t.clientY;
          k13_lite_easy_touchTriggered = false;
        }, { passive: true });

        k13_lite_easy_gallery.addEventListener('touchmove', (e) => {
          if (!k13_lite_easy_shouldIntercept) return;

          const t = e.touches && e.touches[0];
          if (!t) return;
          const dy = t.clientY - k13_lite_easy_touchStartY;
          const threshold = 30;

          let dir = 0;
          if (dy <= -threshold) dir = 1;
          if (dy >= threshold)  dir = -1;

          const atBoundary = (k13_lite_easy_index === k13_lite_easy_slides.length - 1 && dir > 0) || (k13_lite_easy_index === 0 && dir < 0);

          // 如果在边界，始终允许页面滚动
          if (atBoundary) {
            // 边界时，允许页面滚动
            // 不阻止默认
            k13_lite_easy_forcePrevent = false;
            k13_lite_easy_setPageScrollLock(false);
            return;
          }

          // 动画或冷却中，阻止页面滚动
          if (k13_lite_easy_animating || k13_lite_easy_cooling) {
            k13_lite_easy_safePrevent(e);
            k13_lite_easy_setPageScrollLock(true);
            return;
          }

          if (dir !== 0 && !k13_lite_easy_touchTriggered) {
            k13_lite_easy_safePrevent(e);
            k13_lite_easy_go(dir);
            k13_lite_easy_touchTriggered = true;
            k13_lite_easy_setCooling(150);
            k13_lite_easy_setPageScrollLock(true);
          }
        }, { passive: false });

        k13_lite_easy_gallery.addEventListener('touchend', () => { k13_lite_easy_touchTriggered = false; });

        // 键盘事件拦截
        window.addEventListener('keydown', (e) => {
          if (!k13_lite_easy_shouldIntercept) return;
          let dir = 0;
          if (e.key === 'ArrowDown' || e.key === 'PageDown' || e.key === ' ') dir = 1;
          if (e.key === 'ArrowUp'   || e.key === 'PageUp')                 dir = -1;

          const atBoundary = (k13_lite_easy_index === k13_lite_easy_slides.length - 1 && dir > 0) || (k13_lite_easy_index === 0 && dir < 0);

          // 如果在边界，始终允许页面滚动
          if (atBoundary) {
            // 边界时，允许页面滚动
            // 不阻止默认
            k13_lite_easy_forcePrevent = false;
            k13_lite_easy_setPageScrollLock(false);
            return;
          }

          if ((k13_lite_easy_animating || k13_lite_easy_cooling) && dir !== 0) {
            k13_lite_easy_safePrevent(e);
            k13_lite_easy_setPageScrollLock(true);
            return;
          }

          if (dir !== 0) {
            k13_lite_easy_safePrevent(e);
            if (k13_lite_easy_go(dir)) {
              k13_lite_easy_setCooling();
            }
            k13_lite_easy_setPageScrollLock(true);
          }
        });

        // 直接跳转功能
        window.goToSlide = function(slideIndex) {
          if (slideIndex >= 0 && slideIndex < k13_lite_easy_slides.length) {
            k13_lite_easy_slides[k13_lite_easy_index].classList.remove('k13_lite_easy_start_with_liteHead_active');
            k13_lite_easy_slides[slideIndex].classList.add('k13_lite_easy_start_with_liteHead_active');
            k13_lite_easy_index = slideIndex;
            k13_lite_easy_setHUD();
          }
        };

        // 新增：全局阻止页面滚动（防止动画未完成时页面滚动）
        function k13_lite_easy_globalScrollBlocker(e) {
          // 若页面处于滚动锁定状态，优先全局阻止滚动（覆盖一切滚动来源）
          if (k13_lite_easy_scrollLocked) {
            if (e && e.cancelable) e.preventDefault();
            return;
          }
          // 只有在动画进行中且不在边界时才阻止滚动
          if (k13_lite_easy_forcePrevent && k13_lite_easy_shouldIntercept) {
            // 检查是否在边界
            const isAtFirstSlide = k13_lite_easy_index === 0;
            const isAtLastSlide = k13_lite_easy_index === k13_lite_easy_slides.length - 1;

            // 只有在中间图片时才阻止滚动
            if (!isAtFirstSlide && !isAtLastSlide) {
              if (e && e.cancelable) e.preventDefault();
            }
          }

          // 新增：在滚动位置锁定时，强制保持位置
          if (k13_lite_easy_scrollPositionLocked && k13_lite_easy_shouldIntercept) {
            const isAtFirstSlide = k13_lite_easy_index === 0;
            const isAtLastSlide = k13_lite_easy_index === k13_lite_easy_slides.length - 1;

            if (!isAtFirstSlide && !isAtLastSlide) {
              if (e && e.cancelable) e.preventDefault();
              // 立即恢复滚动位置
              requestAnimationFrame(() => {
                window.scrollTo(0, k13_lite_easy_lastScrollY);
              });
            }
          }
        }
        // 针对所有被动滚动事件
        window.addEventListener('touchmove', k13_lite_easy_globalScrollBlocker, { passive: false, capture: true });
        window.addEventListener('wheel', k13_lite_easy_globalScrollBlocker, { passive: false, capture: true });

        k13_lite_easy_setHUD();
      })();

    }); {% endcomment %}
</script>
