            try {
              sessionStorage.setItem('k13_contact_form_submitted', '1');
            } catch (err) {}
          });
          // 页面加载后检测是否刚刚提交过
          if (window.location.pathname === '/contact' && window.location.hash === '#contact_form') {
            try {
              if (sessionStorage.getItem('k13_contact_form_submitted') === '1') {
                sessionStorage.removeItem('k13_contact_form_submitted');
                if (typeof TriplePixel === 'function') {
                  TriplePixel('custom', 'Click_Subscribe', { value: 1 });
                }
              }
            } catch (err) {}
          }
        }
      });
    })();
