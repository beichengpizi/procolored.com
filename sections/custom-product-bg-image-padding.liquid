<style>
  :root {
    /* Font sizes */
    --text-h0-n: 56px;
    --text-h1-n: 40px;
    --text-h2-n: 30px;
    --text-h3-n: 26px;
    --text-h4-n: 24px;
    --text-h5-n: 20px;
    --text-h6-n: 16px;
    --text-xs-n: 12px;
    --text-sm-n: 14px;
    --text-base-n: 18px;
    --text-lg-n: 19px;
  }

  .text-xxs {
    font-size: 10px;
    line-height: 1.2;
  }

  .text-xs {
    font-size: var(--text-xs-n);
    line-height: 1.2;
  }

  .text-sm {
    font-size: var(--text-sm-n);
    line-height: 1.2;
  }

  .text-base {
    font-size: var(--text-base-n);
    line-height: 1.2;
  }
  .text-md {
    font-size: var(--text-md-n);
    line-height: 1.2;
  }

  .text-lg {
    font-size: var(--text-lg-n);
    line-height: 1.2;
  }
   /* 重置样式 start*/
  #detail-section3{{section.id}}  .h0 {
    font-size: var(--text-h0-n) ;
    line-height: 1;
    font-family: ItalianPlateNo2Expanded-Bold, sans-serif ;
  }

  #detail-section3{{section.id}}  h1,
  #detail-section3{{section.id}}  .h1,
  #detail-section3{{section.id}}  .prose h1:not(.h0, .h1, .h2, .h3, .h4, .h5, .h6) {
    font-size: var(--text-h1-n) ;
    line-height: 1.1;
    font-family: ItalianPlateNo2Expanded-Bold, sans-serif ;
  }

  #detail-section3{{section.id}}  h2,
  #detail-section3{{section.id}}  .h2,
  #detail-section3{{section.id}}  .prose h2:not(.h0, .h1, .h2, .h3, .h4, .h5, .h6) {
    font-size: var(--text-h2-n);
    line-height: 1.1;
    font-family: ItalianPlateNo2Expanded-Bold, sans-serif ;
  }

  #detail-section3{{section.id}}  h3,
  #detail-section3{{section.id}}  .h3,
  #detail-section3{{section.id}}  .prose h3:not(.h0, .h1, .h2, .h3, .h4, .h5, .h6) {
    font-size: var(--text-h3-n) ;
    line-height: 1.2;
  }

  #detail-section3{{section.id}}  h4,
  #detail-section3{{section.id}}  .h4,
  #detail-section3{{section.id}}  .prose h4:not(.h0, .h1, .h2, .h3, .h4, .h5, .h6) {
    font-size: var(--text-h4-n) ;
    line-height: 1.2;
  }

  #detail-section3{{section.id}}  h5,
  #detail-section3{{section.id}}  .h5,
  #detail-section3{{section.id}}  .prose h5:not(.h0, .h1, .h2, .h3, .h4, .h5, .h6) {
    font-size: var(--text-h5-n) ;
    line-height: 1.2;
  }

  #detail-section3{{section.id}}  h6,
  #detail-section3{{section.id}}  .h6,
  #detail-section3{{section.id}}  .prose h6:not(.h0, .h1, .h2, .h3, .h4, .h5, .h6) {
    font-size: var(--text-h6-n) ;
    line-height: 1.2;
  }
  /* 重置样式 end*/
  #detail-section3{{section.id}}{
    {% if section.settings.image_height == 'small' %}
      min-height: 290px;
      {% elsif section.settings.image_height == 'medium' %}
      min-height: 560px;
      {% elsif section.settings.image_height == 'large' %}
      min-height:700px;
      {% endif %}
      background:{{ section.settings.container_bg_color }};
  }

  #detail-section3{{section.id}}>.custom-product-image {
    position: relative;
    display: block;
    overflow: hidden;
    box-sizing: border-box;
    width: 100%;
    background:{{ section.settings.container_bg_color }};
    {% if section.settings.is_show_content_text %}
      padding:{{ section.settings.padding_top }}px 0 {{ section.settings.padding_bottom }}px 0;
    {% else %}
      margin: {{ section.settings.padding_top }}px 0 {{ section.settings.padding_bottom }}px 0;
    {% endif %}
    {% if section.settings.image_height == 'small' %}
      min-height: 290px;
    {% elsif section.settings.image_height == 'medium' %}
      min-height: 560px;
    {% elsif section.settings.image_height == 'large' %}
      min-height:700px;
    {% endif %}
  }
  /*没有背景图*/
  #detail-section3{{section.id}}>.noBgImg {
    width: 100%;
    height: auto;
    background-image: none;

  }
  #detail-section3{{section.id}} .custom-product-image-content-wrap{
    {% if section.settings.is_cancel_video_suspension and section.settings.show_product    %}
      position: relative;
      {% else %}
        {% if section.settings.is_show_content_text %}
          border-radius: {{ section.settings.content_border_radius }}px;
          position: relative;
        {% else %}
          position: absolute;
        {% endif %}

    {% endif %}
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    background-color: {{ section.settings.bg_color }};
    line-height: 0;
    {%if section.settings.is_display_content_safe_zone %}
      max-width:1200px;
      margin: 0 auto;
      {%if section.settings.is_show_border_line %}
        border: 2px dashed #000;
      {%endif%}
    {%endif%}
  }
  #detail-section3{{section.id}} .custom-product-image-content {
    width: {{section.settings.content_width}}%;
    text-align: {{section.settings.text_position}};

    margin-top: {{section.settings.content_top_padding}}vw;
    position: relative;
    z-index: 1;

    {% unless section.settings.is_display_content_safe_zone %}
      {% unless section.settings.is_display_content_text_safe_zone %}
        margin-left: {{section.settings.content_left_margin}}vw;
      {% else %}
        width: 100%;
        margin-left: auto;
        margin-right: auto;
        max-width: 1200px;
      {% endunless %}
    {% else %}
      {% if section.settings.safe_zone_alignment  == 'left'%}
        margin-left: 0;
      {% elsif section.settings.safe_zone_alignment  == 'right' %}
        margin-left: auto;
        margin-right: 0;
      {% else %}
        margin-left: auto;
        margin-right: auto;
      {% endif %}

    {% endunless %}

  }

  {% unless section.settings.show_product_video_fullscreen %}
    #detail-section3{{section.id}} .custom-product-image-content:after {
      content: '';
      width: 100%;
      display: inline-block;
    }
  {% endunless %}


  #detail-section3{{section.id}} .custom-heading-3 {

      color: {{section.settings.heading_text}};
      padding-top: {{section.settings.heading_margin_top}}px;
      padding-bottom: {{section.settings.heading_margin_bottom}}px;
      line-height: 1.1;
      margin:0px;
      {% if section.settings.heading_gradient_color  contains 'linear-gradient' %}
        background: {{section.settings.heading_gradient_color}};
        -webkit-background-clip: text;/*将设置的背景颜色限制在文字中*/
        -webkit-text-fill-color: transparent;/*给文字设置成透明*/
      {% endif %}
      {% if section.settings.is_display_content_text_safe_zone %}
        width: {{section.settings.content_width}}%;
      {% endif %}

  }

  #detail-section3{{section.id}} .custom-heading-4 {
    color: {{section.settings.subheading_text}};
    margin-bottom: {{section.settings.subheading_margin_bottom}}px;
    {% if section.settings.subheading_gradient_color  contains 'linear-gradient' %}
      background: {{section.settings.subheading_gradient_color}};
      -webkit-background-clip: text;/*将设置的背景颜色限制在文字中*/
      -webkit-text-fill-color: transparent;/*给文字设置成透明*/
    {% endif %}
    {% if section.settings.is_display_content_text_safe_zone %}
      width: {{section.settings.content_width}}%;
    {% endif %}

  }

  #detail-section3{{section.id}} .custom-decs {
    color: {{section.settings.desc_text}};
    margin-bottom: {{section.settings.desc_margin_bottom}}px;
    {% if section.settings.desc_gradient_color  contains 'linear-gradient' %}
      background: {{section.settings.desc_gradient_color}};
      -webkit-background-clip: text;/*将设置的背景颜色限制在文字中*/
      -webkit-text-fill-color: transparent;/*给文字设置成透明*/
    {% endif %}
    {% if section.settings.is_display_content_text_safe_zone %}
      width: {{section.settings.content_width}}%;
    {% endif %}


  }
  #detail-section3{{section.id}} .custom-button-container{
    display: flex;
    gap: 10px;
    align-items: center;
    justify-content: {{section.settings.button_position}};
  }
  #detail-section3{{section.id}} .product_jump__button{
    display: inline-block;
    padding: 7px 20px;
    border: 0;
    border-radius:{{section.settings.button_border_radius}}px;
    background-color: {{section.settings.button_background_color}};
    color: {{section.settings.button_text_color}};
  }
  #detail-section3{{section.id}} .product_jump__button_2{
    display: inline-block;
    padding: 7px 20px;
    border: 0;
    border-radius:{{section.settings.button_border_radius_2}}px;
    background-color: {{section.settings.button_background_color_2}};
    color: {{section.settings.button_text_color_2}};
  }
  #detail-section3{{section.id}}  .product_video-popup__button{
    display: flex;
    flex-wrap: wrap;
    background-color: transparent;
    width: 220px;
    height: 35px;
    border: 1px solid {{section.settings.desc_text}};
    border-radius: 17px;
    align-items: center;
    padding: 0 36px;
  }
  #detail-section3{{section.id}} .product_video-popup__button span {
    margin-right: 5px;
  }
  #detail-section3{{section.id}} .product_video-popup__button p {
    font-size: var(--text-sm);
    color: {{section.settings.desc_text}} ;
  }
  #detail-section3{{section.id}} .product_video-popup__button span svg {
    width: 100%;
    max-width: 20px;
    fill: {{section.settings.desc_text}} !important;
  }
  #detail-section3{{section.id}} .custom-icon {
    display: flex;
    flex-wrap: wrap;
    justify-content:{{section.settings.icon_position_align}};
    margin-top: {{section.settings.icon_margin_top}}%;
    {% if section.settings.icon_position_align != 'space-around' %}
      row-gap: {{section.settings.icon_spacing}}px;
      column-gap: {{section.settings.icon_spacing}}px;
    {% else %}

    {% endif %}


  }
  #detail-section3{{section.id}} .custom-icon .percentage-container{
    width:{{section.settings.percentage_value_size_pc}}%;
    color: {{section.settings.heading_text}};
  }
  #detail-section3{{section.id}} .custom-icon .percentage-value{
    font-size: 46px;

  }
  #detail-section3{{section.id}} .custom-icon .percentage-desc{
    font-size: 17px;
    line-height: 1.3;
    color: {{section.settings.percentage_desc_color}};

  }

  #detail-section3{{section.id}} .custom-icon img {
    margin: 0 ;
    width: {{section.settings.icon_size_pc}}vw;
    max-width:100%;

  }

  #detail-section3{{section.id}}  .video_wrap img{
    display: block;
    width: 100%;
    max-width: 100%;
    height: 100%;

  }
  #detail-section3{{section.id}} .video_cover_wrap{
    position: relative;
  }
  #youtube-{{section.id}}-pro-videos{

    {% if section.settings.preview_video_img %}
    display: none;
    {% else %}
    display: block;
    {% endif %}
  }
  #detail-section3{{section.id}}  .video_wrap .czvdo-play-icon-svg {
    position: absolute;
    width: 145px;
    left: 50%;
    top: 50%;
    transform: translate(-50%, -50%);
    cursor: pointer;
  }
  #detail-section3{{section.id}}  .video_wrap:hover .czvdo-svg-path-outer {
    fill: #f93822;
    transition: all 0.3s ease;
  }
  #detail-section3{{section.id}}  .video_wrap {
    position: relative;
    width:100%;
    {% if section.settings.show_product_video_fullscreen %}
      max-width: none;
      padding: 0px;
      height: auto;
    {% else %}
      padding: 0 ;
      height: 100%;
      max-width: 117.5rem;
    {% endif %}

    {% if section.settings.is_cancel_video_suspension and section.settings.show_product %}
      {% if section.settings.show_product_video_fullscreen %}
        margin: 0 auto 0 auto;
      {% else %}
        margin: 0 auto 0 auto;
      {% endif %}

    {% else %}
      margin: -18% auto 0 auto;
    {% endif %}

  }
  #detail-section3{{section.id}}  .video_wrap iframe{
    width: 100%;
    aspect-ratio: 16 / 9;
  }
  #detail-section3{{section.id}} .mobile-image{
    display: none;
  }
  #detail-section3{{section.id}} .desktop-image{
    display: block;

    {% if section.settings.is_display_content_safe_zone %}
      max-width: 1200px;
      margin: auto;
    {% endif %}

  }
  #detail-section3{{section.id}}  .custom-product-image .desktop-image img{
    width: 100%;
    position: relative;
    z-index: 1;
    {% if section.settings.image_height !='adapt' %}
      object-fit: cover;
      object-position: center center;

    {% endif %}
    {% if section.settings.is_show_banner %}
      {% if section.settings.image_height == 'small' %}
        min-height: 290px;
      {% elsif section.settings.image_height == 'medium' %}
        min-height: 560px;
      {% elsif section.settings.image_height == 'large' %}
        min-height:700px;
      {% endif %}
    {% endif %}

  }
  @media screen and (max-width: 992px) and (min-width: 768px){
    #detail-section3{{section.id}}  .video_wrap{
      {% if section.settings.show_product_video_fullscreen %}
          max-width: none;
      {% else %}
          max-width:99.2rem;
      {% endif %}
    }
    .percentage-value{
      font-size: 40px;

    }
    .percentage-desc{
      font-size: 14px;
      line-height: 1.3;
    }
  }
  /* iPad、平板电脑 */
  @media screen and (min-width: 481px) and (max-width: 768px) {
    #detail-section3{{section.id}} .mobile-image{
      display: block;
    }
    #detail-section3{{section.id}} .desktop-image{
      display: none;
    }
    #detail-section3{{section.id}}  .video_wrap{
      {% if section.settings.show_product_video_fullscreen %}
        max-width: none;
      {% else %}
        max-width: 76.8rem;
      {% endif %}
    }

  }

  /* 移动设备 */
  @media screen and (min-width: 320px) and (max-width: 480px) {
    #detail-section3{{section.id}}{
      {%if section.settings.is_display_content_safe_zone %}
        {%if section.settings.is_display_all_screen_on_mobile %}
          padding-left:0;
          padding-right:0;
        {% else %}
          padding-left:24px;
          padding-right:24px;
        {%endif%}

      {%endif%}
      min-height: auto;
    }
    #detail-section3{{section.id}} .custom-button-container{

      justify-content: {{section.settings.button_position_mobile}};
    }
    #detail-section3{{section.id}}>.custom-product-image {
      min-height: auto;
      {% if section.settings.is_show_content_text %}
        padding:{{ section.settings.padding_top_mobile }}px 0 {{ section.settings.padding_bottom_mobile }}px 0;
      {% else %}
        margin: {{ section.settings.padding_top_mobile }}px 0 {{ section.settings.padding_bottom_mobile }}px 0;
        padding: 0;
      {% endif %}



    }
    #detail-section3{{section.id}} .custom-product-image-content-wrap{
      {% if section.settings.content_in_image  %}
        position: absolute;
      {% else %}
        position: relative;
      {% endif %}
      {% if section.settings.is_show_content_text %}
        border-radius: {{ section.settings.content_border_radius_mobile }}px;
      {% endif %}
      top: 0;
      left: 0;
      right: 0;
      bottom: 0;
    }
    #detail-section3{{section.id}} .mobile-image img{
      width:100%;
    }
    #detail-section3{{section.id}} .mobile-image{
      display: block;
    }
    #detail-section3{{section.id}} .desktop-image{
      display: none;
    }


    #detail-section3{{section.id}} .custom-heading-3 {
      {% if section.settings.content_in_image %}
      {% else %}
        color: {{section.settings.heading_text_mobile}};
        line-height:1.3;
      {% endif %}
        font-size: {{section.settings.heading_size_mobile}}px;
        text-align:{{section.settings.content_in_image_text_position}};
        padding-top: {{section.settings.heading_margin_top_mobile}}px;
        padding-bottom: {{section.settings.heading_margin_bottom_mobile}}px;
        {% if section.settings.is_display_content_text_safe_zone %}
          width: 100%;
        {% endif %}


    }
    #detail-section3{{section.id}} .custom-heading-3 p{
      margin-bottom: 0px;
    }

    #detail-section3{{section.id}} .custom-heading-4 {
      {% if section.settings.content_in_image %}

      {% else %}
        color: {{section.settings.subheading_text_mobile}};
      {% endif %}
      line-height:1.3;
      text-align:{{section.settings.content_in_image_text_position}};
      margin-bottom: {{section.settings.subheading_margin_bottom_mobile}}px;
      font-size: {{section.settings.subheading_size_mobile}}px;
      {% if section.settings.is_display_content_text_safe_zone %}
        width: 100%;
      {% endif %}


    }

    #detail-section3{{section.id}} .custom-decs {
      {% if section.settings.content_in_image %}

      {% else %}
        color:{{section.settings.desc_text_mobile}};
      {% endif %}
      line-height:1.2;
      font-size: {{section.settings.desc_size_mobile}}px;
      text-align:{{section.settings.content_in_image_text_position}};
      margin-bottom: {{section.settings.desc_margin_bottom_mobile}}px;
      {% if section.settings.is_display_content_text_safe_zone %}
        width: 100%;
      {% endif %}


    }

    #detail-section3{{section.id}}  .product_video-popup__button, #detail-section3{{section.id}} .product_jump__button, #detail-section3{{section.id}} .product_jump__button_2{
      margin-left: auto;
      margin-right: auto;
      font-size: {{section.settings.button_text_mobile}}px;
      {% if section.settings.is_button_adapt %}
        width: 100%;
      {% endif %}
      {% if section.settings.content_in_image %}

      {% else %}
        color:{{section.settings.button_text_color_mobile}};
        background-color: {{section.settings.button_background_mobile}};
      {% endif %}
      margin-bottom: 20px;
    }

    #detail-section3{{section.id}} .custom-product-image-content {
      width: 100%;
      margin-bottom: 0;
      {% if section.settings.content_in_image %}
        position: absolute;
        top: {{section.settings.mobile_content_top_position}}vw;
        margin-top: 0;
        padding: 0 20px;
      {% else %}
        position: relative;
        margin-top:{{section.settings.mobile_content_top_position}}vw;
        {% if section.settings.is_show_content_text %}
          padding: 0 20px;
        {% else %}
          padding: 0 24px;
        {% endif %}
      {% endif %}
      margin-left:0;
      left:0;

      text-align:{{section.settings.content_in_image_text_position}};
    }

    #detail-section3{{section.id}} .custom-icon {
      {% if section.settings.content_in_image %}
        position: absolute;
        left: 0;
        top: {{section.settings.icon_top_position_mobile}}vw;
        justify-content: {{section.settings.mobile_icon_align}};
        padding: 0 290px;
        margin-top: 0;
      {% else %}
        position: relative;
        flex-direction: column;
        align-items: center;
        row-gap: 1rem;
        padding: 0;
        margin-top: 0;
      {% endif %}

      width: 100%;

    }
    #detail-section3{{section.id}} .custom-icon .percentage-container{
      width:100%;
      color: #333;
    }
    #detail-section3{{section.id}} .custom-icon .percentage-value{
      font-size: 32px;

    }
    #detail-section3{{section.id}} .custom-icon .percentage-desc{
      font-size: 15px;
      line-height: 1.3;
      color:#676767;

    }
    #detail-section3{{section.id}} .custom-icon img {
      {% if section.settings.mobile_icon_align == 'space-around' or section.settings.mobile_icon_align == 'space-between' %}
        margin:1.6rem 0 3rem 0;
      {% elsif section.settings.mobile_icon_align == 'center' %}
        margin: 1.6rem calc({{section.settings.icon_spacing}}px / 2) 3rem calc({{section.settings.icon_spacing}}px / 2);
      {% elsif section.settings.mobile_icon_align == 'flex-end' %}
        margin: 1.6rem 0 3rem {{section.settings.icon_spacing}}px;
      {% endif %}
      width: {{section.settings.icon_size_mobile}}%;
      max-width:100%;

    }

    #detail-section3{{section.id}}  .video_wrap{

      max-width: 100%;

    }
    #detail-section3{{section.id}}  .video_wrap .czvdo-play-icon-svg {

      width: 50px;

    }

  }
</style>
{% assign image_pc = section.settings.image_pc %}
{% assign image_mobile = section.settings.image_mobile %}
{% comment %}
  {% if localization.language.iso_code == 'en' %}
    {% assign image_pc = section.settings.image_pc %}
    {% assign image_mobile = section.settings.image_mobile %}
  {% elsif localization.language.iso_code == 'de' %}
    {% assign image_pc = section.settings.image_pc_de | default: section.settings.image_pc %}
    {% assign image_mobile = section.settings.image_mobile_de | default: section.settings.image_mobile %}
  {% elsif localization.language.iso_code == 'fr' %}
    {% assign image_pc = section.settings.image_pc_fr | default: section.settings.image_pc %}
    {% assign image_mobile = section.settings.image_mobile_fr | default: section.settings.image_mobile %}
  {% endif %}
{% endcomment %}
<!-- 主图banner -->
<div id="detail-section3{{section.id}}">
  <div
    class="
      custom-product-image {% unless section.settings.image_pc and  section.settings.image_mobile %}

      {% endunless %}
    "
  >
    {% if section.settings.is_show_banner %}
      <div class="desktop-image" style="aspect-ratio: {{ section.settings.image_pc.aspect_ratio }};">
        {% if image_pc %}
          <img
            width="100vw"

            {% if section.settings.is_open_lazyload %}
              class="lazyload"
              loading="lazy"
              data-sizes="100vw"
              data-src="{{ image_pc | image_url:width:1000 }}"
              data-srcset="
                {%- if image_pc.width >= 375 -%}{{ image_pc | image_url: width: 375 }} 375w,{%- endif -%}
                {%- if image_pc.width >= 550 -%}{{ image_pc | image_url: width: 550 }} 550w,{%- endif -%}
                {%- if image_pc.width >= 750 -%}{{ image_pc | image_url: width: 750 }} 750w,{%- endif -%}
                {%- if image_pc.width >= 1100 -%}{{ image_pc | image_url: width: 1100 }} 1100w,{%- endif -%}
                {%- if image_pc.width >= 1500 -%}{{ image_pc | image_url: width: 1500 }} 1500w,{%- endif -%}
                {%- if image_pc.width >= 1780 -%}{{ image_pc | image_url: width: 1780 }} 1780w,{%- endif -%}
                {%- if image_pc.width >= 2000 -%}{{ image_pc | image_url: width: 2000 }} 2000w,{%- endif -%}
                {%- if image_pc.width >= 3000 -%}{{ image_pc | image_url: width: 3000 }} 3000w,{%- endif -%}
                {%- if image_pc.width >= 3840 -%}{{ image_pc | image_url: width: 3840 }} 3840w,{%- endif -%}
                {{ image_pc | image_url }} {{ image_pc.width }}w
              "
