<script>
  /* 加封面图 */
  $(function () {
    $('#youtube-{{section.id}}-playVideos,#detail-section3{{section.id}} .video_wrap .czvdo-play-icon-svg').on(
      'click',
      function (e) {
        $('#youtube-{{section.id}}-playVideos').css('display', 'none');
        $('#detail-section3{{section.id}} .video_wrap .czvdo-play-icon-svg').css('display', 'none');
        e.preventDefault();
        e.stopPropagation();
        $('#youtube-{{section.id}}-pro-videos').css('display', 'block');

        const newurl = $('#youtube-{{section.id}}-pro-videos').attr('src') + '?autoplay=1';
        console.log('youtube视频地址==', $('#youtube-{{section.id}}-pro-videos').attr('src'), newurl);
        //  将当前的 iframe 的 src 属性设置为自己当前的 src 属性，从而强制浏览器重新加载该 iframe,从而实现点击预览图后自动播放
        $('#youtube-{{section.id}}-pro-videos').attr('src', newurl);
      }
    );

    const elements = $('#detail-section3{{section.id}} .percentage-container>.percentage-value');

    elements.each(function () {
      const element = $(this);
      const targetValue = parseInt(element.data('target'));
      element.data('start', false);

      $(window).on('scroll', function () {
        const elementPosition = element.offset().top;
        const topOfWindow = $(window).scrollTop();
        const windowHeight = $(window).height();

        // 检查元素是否在视窗内
        if (elementPosition < topOfWindow + windowHeight && !element.data('start')) {
          element.data('start', true);
          animateToTarget(element, targetValue);
        }
      });
    });
    function animateToTarget(element, targetValue) {
      let currentValue = 0;
      const increment = 1;
      const interval = setInterval(function () {
        if (currentValue >= targetValue) {
          clearInterval(interval);
          element.text(targetValue + '%');
          return;
        }
        currentValue += increment;
        element.text(currentValue + '%');
      }, 30); // 调整 20 的值可以改变动画速度
    }
  });
</script>

