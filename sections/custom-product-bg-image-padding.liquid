  "name": "Major Banner",
  "tag": "section",
  "settings": [
    {
      "type": "checkbox",
      "id": "is_display_content_safe_zone",
      "label": "内容安全区域显示",
      "info": "内容安全区域大小为1200px",
      "default": false
    },
    {
      "type": "checkbox",
      "id": "is_open_lazyload",
      "label": "开启图片懒加载",
      "info": "顶部banner图，建议关闭",
      "default": true
    },
    {
      "type": "checkbox",
      "id": "is_display_content_text_safe_zone",
      "label": "文字内容安全区域显示",
      "info": "文字内容安全区域大小为1200px",
      "default": false
    },
    {
      "type": "checkbox",
      "id": "is_display_all_screen_on_mobile",
      "label": "移动端全屏",
      "default": false
    },
    {
      "type": "checkbox",
      "id": "is_show_border_line",
      "label": "显示边框线",
      "default": true
    },
    {
      "type": "select",
      "id": "image_height",
      "options": [
        {
          "value": "adapt",
          "label": "自适应"
        },
        {
          "value": "small",
          "label": "小"
        },
        {
          "value": "medium",
          "label": "中"
        },
        {
          "value": "large",
          "label": "大"
        }
      ],
      "default": "medium",
      "label": "横幅高度",
      "info": "若要获得最佳效果，请使用纵横比为 3:2 的图片。[了解详细信息](https:\/\/help.shopify.com\/manual\/shopify-admin\/productivity-tools\/image-editor#understanding-image-aspect-ratio)"
    },
    {
      "type": "checkbox",
      "id": "is_show_content_text",
      "label": "仅显示内容文本",
      "default": false
    },
    {
      "type": "checkbox",
      "id": "is_show_banner",
      "label": "显示图片",
      "default": true
    },
    {
      "type": "checkbox",
      "id": "content_in_image",
      "label": "内容在图片里面(移动端生效)",
      "default": false
    },
    {
      "type": "checkbox",
      "id": "show_product",
      "label": "显示商品视频",
      "default": false
    },
    {
      "type": "checkbox",
      "id": "show_product_video_fullscreen",
      "label": "商品视频全屏显示",
      "default": false,
      "info": "商品视频全屏显示需取消勾选内容安全区域显示"
    },
    {
      "type": "checkbox",
      "id": "is_cancel_video_suspension",
      "label": "取消视频悬浮",
      "default": false
    },
    {
      "type": "video_url",
      "id": "product_introduction_video",
      "label": "商品介绍视频链接",
      "info": "当显示商品视频被选中时，该链接生效",
      "accept": ["youtube", "vimeo"]
    },
    {
      "type": "image_picker",
      "id": "preview_video_img",
      "label": "视频封面图",
      "info": "建议使用16:9的图片"
    },
    {
      "type": "text",
      "id": "description",
      "default": "视频描述",
      "label": "视频描述"
    },

    {
      "type": "image_picker",
