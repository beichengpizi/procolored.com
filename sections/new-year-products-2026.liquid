
{% for block in section.blocks %}
  {% if block.settings.product.available == true %}
    {% assign product_num = product_num | plus: 1 %}
  {% endif %}
{% endfor %}

<style>
        /* christmas-products-wrapper start */
        .christmas-products-{{ section.id }}{
          padding-top:{{ section.settings.wrap_padding_top }}px;
          padding-bottom:{{ section.settings.wrap_padding_bottom }}px;
          background: {{ section.settings.bg_color }};
          overflow: hidden;
          background-size: cover;
          {% if section.settings.background != empty %}background-image: url({{ section.settings.background_image | image_url }}){% endif %};
        }
        .christmas-products-{{ section.id }}  .christmas-products-wrapper {
          position: relative;
          border-top-left-radius: {{ section.settings.top_left_radius }}px;
          border-top-right-radius: {{ section.settings.top_right_radius }}px;
          border-bottom-left-radius: {{ section.settings.bottom_left_radius }}px;
          border-bottom-right-radius: {{ section.settings.bottom_right_radius }}px;
          background-size: 500px;
          max-width: 1400px;
          margin: 0 auto;
          background-color: {{ section.settings.content_bg_color }};
          padding: {{ section.settings.padding_top }}px 24px {{ section.settings.padding_bottom }}px 24px;
        }

        .christmas-products-{{ section.id }} .christmas-products-container {
          z-index: 2;
          position: relative;
        }

        .christmas-products-{{ section.id }}  .christmas-products-wrapper::after {
          content: "";
          background-image: url('https://cdn.shopify.com/s/files/1/0509/3454/6613/files/3_1_d1ccdbdf-e58e-497f-978e-0e9efba53360.png?v=1766738097');
          background-repeat: repeat;
          opacity: 0.2;
          position: absolute;  
          top: 0;
          left: 0;
          bottom: 0;
          right: 0;
          background-size: cover;
        }

        .christmas-products-{{ section.id }}  .christmas-products-title-wrapper {
          display: flex;
          flex-direction: column;
          align-items: center;
          width: 100%;
        }

  .christmas-products-{{ section.id }}  .christmas-products-title {

          text-align: center;
          font-size: 40px;
          font-weight: 800;
          line-height: 1.5;
          /* 150% */
          margin-bottom: 80px;
        }

        .christmas-products-{{ section.id }}  .christmas-products-subtitle {
          color: #fff;
          text-align: center;

          font-size: 22px;

          font-weight: 400;
          line-height: 1;
          /* 272.727% */
          margin-bottom: 24px;
        }

        .christmas-products-{{ section.id }}  .christmas-products-desc {
          color: #fff;
          text-align: center;
          max-width: 650px;
          font-size: 24px;
          font-weight: 800;
          line-height: 1.16;
          margin-bottom: 24px;
        }

        .christmas-products-{{ section.id }}  .christmas-products-desc .christmas-products-desc-key {
          display: block;
          color: #fece67;
        }

        .christmas-products-{{ section.id }}  .christmas-products-content {
          display: flex;
          flex-direction: column;
        }

        .christmas-products-{{ section.id }}  .christmas-products-products {
          --product-num: {{- product_num | minus: 0 -}};
          --gap-num: {{- product_num | minus: 1 -}};
          display: grid;
          grid-template-columns: repeat({{- product_num | minus: 0 -}}, calc(calc(100% - var(--gap-num) * 24px) / var(--product-num)));
          grid-gap: {{ section.settings.card_gap }}px;
        }

        .christmas-products-{{ section.id }}  .christmas-products-products-card {
          position: relative;
          display: flex;
          flex-direction: column;
          gap: 12px;
          border-radius: 20px;
          padding: 20px {{ section.settings.card_padding_lr }}px;
          background-color: {{ section.settings.card_bg_color }};
          background: {{ section.settings.card_background_gradient_color }};
          border: 1px solid #FFD7B4;
          height: 100%;
        }



        .christmas-products-{{ section.id }}  .christmas-products-product-label-discount {
          position: absolute;
          top: 24px;
          left: 24px;
          background-repeat: no-repeat;
          background-position: center;
          background-size: 100% 100%;
          display: flex;
          justify-content: center;
          align-items: center;
          gap: 16px;
        }

        {% if section.settings.show_starpick %}
          .christmas-products-{{ section.id }}  .christmas-products-product-label-discount {
            left: 24px;
          }
        {% endif %}

        {% if section.settings.discount_label_center %}
          .christmas-products-{{ section.id }}  .christmas-products-product-label-discount {
            width: 100%;
          }
        {% endif %}
        .christmas-products-{{ section.id }}  .christmas-products-product-label-discount img {
          width: 48px;
        }

