        .christmas-products-{{ section.id }}  .christmas-products-product-label-discount-text {
          color: #fff;
          font-size: 12px;
          font-weight: 400;
          line-height: 1;
          display: flex;
          justify-content: center;
          gap: 4px;
          align-items: center;
          background: #CC2A1F;
          padding: 4px 12px;
