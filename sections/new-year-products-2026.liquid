<script>
  function formatUtcTime(v) {
    let date = new Date(v);
    return (
      date.getFullYear() +
      '/' +
      (date.getMonth() + 1) +
      '/' +
      (date.getDate() < 10 ? '0' + date.getDate() : date.getDate()) +
      ' ' +
      (date.getHours() < 10 ? '0' + date.getHours() : date.getHours()) +
      ':' +
      (date.getMinutes() < 10 ? '0' + date.getMinutes() : date.getMinutes()) +
      ':' +
      (date.getSeconds() < 10 ? '0' + date.getSeconds() : date.getSeconds())
    );
  }
  $(function () {
    if(document.querySelectorAll('.white-button').length != 0) {
      document.querySelectorAll('.white-button').forEach((item) => {
        item.addEventListener('click', function() {
          item.closest(".christmas-products-products-items").querySelector('.white-product').style.display = 'flex';
          item.closest(".christmas-products-products-items").querySelector('.pink-product').style.display = 'none';
        });
      });
    }

    if(document.querySelectorAll('.pink-button').length != 0) {
      document.querySelectorAll('.pink-button').forEach((item) => {
        item.addEventListener('click', function() {
          item.closest(".christmas-products-products-items").querySelector('.pink-product').style.display = 'flex';
          item.closest(".christmas-products-products-items").querySelector('.white-product').style.display = 'none';
        });
      });
    }
    if ($('.christmas-products-{{ section.id }} [data-countdown]')) {
      $('.christmas-products-{{ section.id }} [data-countdown]').each(function () {
        const UTCDate = $(this).data('countdown'); // 活动开始倒计时
        const UTCDate_end = $(this).data('countdown-end'); // 活动截止倒计时
