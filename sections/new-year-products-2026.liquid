<section
  id="{%- if section.settings.nav_id != empty %}custom-nav-{{- section.settings.nav_id -}}{% endif -%}"
  class="christmas-products-{{ section.id }}"
>
  {%- if section.settings.title != empty -%}
    <div class="christmas-products-title">{{ section.settings.title }}</div>
  {%- endif -%}

  <div class="christmas-products-wrapper">
    {% assign bow_image = section.settings.bow_image %}
    {% if bow_image %}
      <div class="christmas-products-Bow">
        <img
          style="aspect-ratio: {{ bow_image.aspect_ratio }}"
          class="lazyload"
          data-sizes="10vw"
          data-srcset="
            {%- if bow_image.width >= 200 -%}{{ bow_image | image_url: width: 200 }} 200w,{%- endif -%}
            {%- if bow_image.width >= 375 -%}{{ bow_image | image_url: width: 375 }} 375w,{%- endif -%}
            {%- if bow_image.width >= 550 -%}{{ bow_image | image_url: width: 550 }} 550w,{%- endif -%}
            {%- if bow_image.width >= 750 -%}{{ bow_image | image_url: width: 750 }} 750w,{%- endif -%}
            {%- if bow_image.width >= 1100 -%}{{ bow_image | image_url: width: 1100 }} 1100w,{%- endif -%}
            {%- if bow_image.width >= 1500 -%}{{ bow_image | image_url: width: 1500 }} 1500w,{%- endif -%}
            {%- if bow_image.width >= 1780 -%}{{ bow_image | image_url: width: 1780 }} 1780w,{%- endif -%}
            {%- if bow_image.width >= 2000 -%}{{ bow_image | image_url: width: 2000 }} 2000w,{%- endif -%}
            {%- if bow_image.width >= 3000 -%}{{ bow_image | image_url: width: 3000 }} 3000w,{%- endif -%}
            {%- if bow_image.width >= 3840 -%}{{ bow_image | image_url: width: 3840 }} 3840w,{%- endif -%}
            {{ bow_image | image_url }} {{ bow_image.width }}w
          "
          data-src="{{ bow_image | image_url:width:200 }}"
          alt="{{ bow_image.alt }}"
        >
      </div>
    {%- endif -%}
    <div class="christmas-products-title-wrapper">
      {%- if section.settings.subtitle != empty -%}
        <div class="christmas-products-subtitle">{{ section.settings.subtitle }}</div>
      {%- endif -%}
      {%- if section.settings.desc != empty -%}
        <div class="christmas-products-desc">{{ section.settings.desc }}</div>
      {%- endif -%}
      <!-- 秒杀倒计时 模块 -->
      {%- if section.settings.flash_sale_countdown != empty or section.settings.flash_sale_countdown_end != empty -%}
        <div
          class="product-flash-sale-countdown"
        >
          {%- render 'flash-sale-countdown',
            countdown_style: 2,
            flash_sale_countdown_date: section.settings.flash_sale_countdown,
            flash_sale_countdown_end_date: section.settings.flash_sale_countdown_end
          -%}
        </div>
      {%- endif -%}
    </div>
    {%- if section.settings.show_product_banner -%}
      <!-- star products banner  start -->
      <div class="christmas-products-banner-wrapper">
        <div class="christmas-products-banner-containner">
          <div class="christmas-products-banner-content">
            {%- if section.settings.product_banner_tips != empty -%}
              <div class="christmas-products-banner-content-tips">{{ section.settings.product_banner_tips }}</div>
            {%- endif -%}
            {%- if section.settings.product_banner_title != empty -%}
              <div class="christmas-products-banner-content-title">
                {{ section.settings.product_banner_title }}
              </div>
            {%- endif -%}
          </div>
          <div class="christmas-products-banner-img">
            {% assign product_banner_image_pc = section.settings.product_banner_image_pc %}
            {% assign product_banner_image_mb = section.settings.product_banner_image_mb %}
            {% if product_banner_image_pc %}
              <img
                style="aspect-ratio: {{ product_banner_image_pc.aspect_ratio }}"
                class="pc lazyload"
                data-sizes="50vw"
                data-srcset="
                  {%- if product_banner_image_pc.width >= 200 -%}{{ product_banner_image_pc | image_url: width: 200 }} 200w,{%- endif -%}
                  {%- if product_banner_image_pc.width >= 375 -%}{{ product_banner_image_pc | image_url: width: 375 }} 375w,{%- endif -%}
