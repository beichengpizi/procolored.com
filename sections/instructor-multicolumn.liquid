      padding: 14px 28px;
      justify-content: center;
      align-items: center;
      border-radius: 75px;
      font-weight: 700;
      background: {{section.settings.button_bg_color}};
      font-size: 14px;
      line-height: 1;
      color: {{section.settings.button_font_color}};
      min-width: 146px;
    }
    .member-benefits-wrap-{{ section.id }}__footer{
      display: flex;
      margin-top: 40px;
      justify-content: center;
    }
  .member-benefits-wrap-{{ section.id }}_footer_button{
    display: flex;
    padding: 20px 60px;
    justify-content: center;
    align-items: center;
    line-height: 1;
    font-size: 14px;
    font-weight: 700;
    color: {{section.settings.button_font_color}};
    border-radius: 40px;
    background: {{section.settings.button_bg_color}};
  }
        [data-section-type='member-benefits'] .placeholder_svg_tag {
          width: 56px;
          height: 56px;
        }
        .member-benefits-container-{{ section.id }}  .member-benefits-content .item  .mobileShow{
          display: none;
        }
        .member-benefits-container-{{ section.id }}  .member-benefits-content .item  .desktopShow{
          display: block;
          {% if section.settings.show_icon_max_height %}
            max-height: {{section.settings.icon_max_height}}px;
          {% endif %}
        }
      /* 移动设备 */
      @media screen and (min-width: 320px) and (max-width: 480px){
        .member-benefits-wrap-{{ section.id }}{
          padding-top:{{section.settings.padding_top_mobile}}px;
          padding-bottom:{{section.settings.padding_bottom_mobile}}px;
          padding-left: 0;
          padding-right: 0;
          overflow: hidden;
        }

        .member-benefits-container-{{ section.id }} .swiper-pagination {
          position: static;
          display: block;
        }

        .member-benefits-container-{{ section.id }}  .heading{
          font-size: {{section.settings.headding_size_mobile}}px;
          margin-bottom: {{section.settings.title_margin_bottom_mobile}}px;
          text-align: {{section.settings.title_alignment_mobile}};
          padding: 0 16px;
          }
          .member-benefits-container-{{ section.id }}  .member-benefits-content{
            gap:{{ section.settings.item_gap_mobile }}px;
          }
          .member-benefits-container-{{ section.id }}  .member-benefits-content .item{
            padding:{{section.settings.item_padding_top_mobile}}px {{section.settings.item_padding_right_mobile}}px {{section.settings.item_padding_bottom_mobile}}px {{section.settings.item_padding_left_mobile}}px;
            min-height: {{section.settings.bg_image_min_height_mobile}}px;
            border-radius: {{section.settings.item_border_radius_mobile}}px;
            min-height: unset;
            height: 100%;
            display: flex;
            flex-direction: column;
          }
          .member-benefits-container-{{ section.id }}  .member-benefits-content .item .img-wrap{
            padding:{{section.settings.image_padding_top_mobile}}px {{section.settings.image_padding_right_mobile}}px {{section.settings.image_padding_bottom_mobile}}px {{section.settings.image_padding_left_mobile}}px;
          }
          .member-benefits-container-{{ section.id }}  .member-benefits-content .item .headding-wrap{
            display: flex;
            align-items: center;
            column-gap: 16px;
              
            margin-top: {{section.settings.headding_margin_top_mobile}}px;
            margin-bottom: {{section.settings.headding_margin_bottom_mobile}}px;
          }
          .member-benefits-container-{{ section.id }}  .member-benefits-content .item h3{
           
            width: 100%;
            font-size: {{section.settings.title_size_mobile}}px;
            {% unless section.settings.show_icon_in_headding %}
              margin-top: {{section.settings.headding_margin_top_mobile}}px;
              margin-bottom: {{section.settings.headding_margin_bottom_mobile}}px;
            {% else %}
              margin-top: 0;
              margin-bottom: 0;
            {% endunless %}
            line-height: 1.2;

          }
          .member-benefits-container-{{ section.id }}  .member-benefits-content .item  .description{
            font-size: {{section.settings.desc_size_mobile}}px;

          }
          .member-benefits-container-{{ section.id }}  .member-benefits-content .item .buttom-wrap{
            display: flex;
            margin-top: 14px;

        }
          .member-benefits-container-{{ section.id }}  .member-benefits-content .item .buttom-wrap a{

            padding: 12px 28px;
            min-width: 108px;

            font-size: 12px;

          }
          .member-benefits-wrap-{{ section.id }}__footer{

            margin-top: 20px;
          }
        .member-benefits-wrap-{{ section.id }}_footer_button{

          padding: 20px 48px;

        }
        .member-benefits-container-{{ section.id }}  .member-benefits-content .item  .mobileShow{
          display: block;
          {% if section.settings.show_icon_max_height %}
            {% if section.settings.show_icon_in_headding %}
              width: 20%;
              max-height: max-content;
            {% else %}
              max-height: {{section.settings.icon_max_height_mobile}}px;
            {% endif %}
          {% endif %}
        }
        .member-benefits-container-{{ section.id }}  .member-benefits-content .item  .desktopShow{
          display: none;
        }
      }
</style>

{% schema %}
{
  "name": "Instructor multicolumn",
  "tag": "section",
  "settings": [
    {
      "type": "header",
      "content": "配置项"
    },
    {
      "type": "select",
      "id": "section_max_width",
      "label": "模块最大宽度",
      "options": [
        {
          "value": "none",
          "label": "全屏"
        },
        {
          "value": "1400",
          "label": "1400px"
        },
        {
          "value": "1200",
          "label": "1200px"
        },
        {
          "value": "980",
          "label": "980px"
        },
     
      ],
      "default": "1200"
    },
    {
      "type": "checkbox",
      "id": "show_label",
      "label": "显示标签",
      "default": true
    },
    {
      "type": "checkbox",
      "id": "show_icon_max_height",
      "label": "开启图标最大高度",
      "default": false
    },
    {
      "type": "checkbox",
      "id": "show_icon_in_headding",
      "label": "开启图标在主标题内部",  
      "default": false,
      "info": "目前仅支持移动端，开启后，图标会显示在主标题内部，而不是在主标题外部"
    },
    {
      "type": "color_background",
      "id": "bg_color",
      "label": "背景颜色",
      "default": "#f5f5f5"
    },
    {
      "type": "range",
      "id": "item_border_radius",
      "min": 0,
      "max": 100,
      "step": 1,
      "unit": "px",
      "label": "卡片边框圆角大小",
      "default": 20
    
    },
    {
      "type": "range",
      "id": "item_border_radius_mobile",
      "min": 0,
      "max": 100,
      "step": 1,
      "unit": "px",
      "label": "卡片边框圆角大小（移动端）",
      "default": 20
    
    },
    {
      "type": "range",
      "id": "icon_max_height",
      "min": 0,
      "max": 200,
      "step": 2,
      "unit": "px",
      "label": "图标最大高度",
      "default": 44,
