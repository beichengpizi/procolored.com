{% liquid
  if section.settings.row == 2
  assign row = 'lg:grid-cols-2'
  endif
  if section.settings.row == 3
    assign row = 'lg:grid-cols-3'
  endif

  if section.settings.row == 4
    assign row = 'lg:grid-cols-4'
  endif

  if section.settings.row == 5
    assign row = 'lg:grid-cols-5'
  endif
%}

<div
  class="member-benefits-wrap-{{ section.id }}"
  data-section-type="member-benefits"
  style="background-color: {{ section.settings.bg_color }};"
>
  <div class="member-benefits-container-{{ section.id }} swiper">
    <h2 class="heading  m-0  {{ section.settings.heading_font_family }}">{{ section.settings.headding }}</h2>
    {% if section.settings.descriptiton != blank %}
    <p>{{ section.settings.descriptiton }}</p>
    {% endif %}

    <div class="member-benefits-content w-full   inline-grid md:grid-cols-2  {{ row }} swiper-wrapper">
      {%- for block in section.blocks -%}
        {%- case block.type -%}
          {%- when 'member' -%}
            <div class="swiper-slide">
              {% render 'member-benefits', type: 'member', block: block %}
            </div>
          {%- else -%}
            <div class="swiper-slide">
              {% render 'member-benefits', type: 'item', block: block %}
            </div>
        {%- endcase -%}
      {%- endfor -%}
    </div>
    {% if section.settings.button_text != empty %}
      < {%- if customer -%}div{%- else -%}modal-login client:idle{%- endif %}  class="member-benefits-wrap-{{ section.id }}__footer">
        
        <a
          data-login
          class="member-benefits-wrap-{{ section.id }}_footer_button"
          href="{%- if customer -%}
          {{ section.settings.button_link | default: '#' }}
        {%- else -%}
         /account
        {% endif %}"
        >
          {{ section.settings.button_text }}
        </a>
      </{%- if customer -%}div{%- else -%}modal-login{%- endif -%}>
    {% endif %}

    <div class="swiper-pagination"></div>
  </div>
</div>

<script>
  var swiper = new Swiper(".member-benefits-container-{{ section.id }}", {
    slidesPerView: 4,
    spaceBetween: 16,
    slidesOffsetAfter: 0,
    slidesOffsetBefore: 0,
    allowTouchMove: false,
    pagination: {
      el: ".member-benefits-container-{{ section.id }} .swiper-pagination",
      clickable: true,
    },
    breakpoints: {
      768: {
        slidesPerView: 1.2,
        spaceBetween: 16,
        slidesOffsetAfter: 24,
        slidesOffsetBefore: 24,
        allowTouchMove: true,
      },
    },
  });
</script>
<style>
      .member-benefits-wrap-{{ section.id }}{
        padding-top:{{section.settings.padding_top}}px;
        padding-bottom:{{section.settings.padding_bottom}}px;

      }
      .member-benefits-container-{{ section.id }}{
