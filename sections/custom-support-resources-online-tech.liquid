<style>
    .custom-support-resources-tech{
      background: #fff;
    }
    .online-container-{{ section.id }} {
      max-width: 1248px;
      margin: 100px auto 100px;
      text-align: center;
      padding: 0 24px;
    }
    .online-container-{{ section.id }} .online-title{
      max-width: 960px;
      margin: 0 auto;
    }
    .online-container-{{ section.id }} .online-title h3.title {
      color: #000;
      font-size: 40px;
      font-weight: 800;
      line-height: 1.2;
      margin-bottom: 14px;
    }

    .online-container-{{ section.id }} .info-spec-item {
      display: flex;
      align-items: center;
    }

    .online-container-{{ section.id }} .info-spec-item svg {
      width: 30px;
      height: 30px;
    }

    .online-container-{{ section.id }} .online-title div.spec p {
      color: #000;
      font-size: 18px;
      line-height: 1.38;
      margin: 0;
    }

    .online-container-{{ section.id }} .online-title div.spec p span {
      font-weight: bold;
      color: #000;
    }

    .online-container-{{ section.id }} .s-time-table {
      margin: 40px 0;
    }

    .online-container-{{ section.id }} .time-table {
      width: 100%;
      font-size: 18px;
      color: #000;
      border: none;
      background-color: #F9F9F9;
      text-align: center;
      margin: 0;
      border-radius: 20px;
      border-collapse: collapse;
    }

  /* 移除最外层边框 */
  .online-container-{{ section.id }} .time-table tr:first-child th,
  .online-container-{{ section.id }} .time-table tr:first-child td {
    border-top: none;
  }

  .online-container-{{ section.id }} .time-table tr:last-child td {
    border-bottom: none;
  }

  .online-container-{{ section.id }} .time-table td:first-child,
  .online-container-{{ section.id }} .time-table th:first-child {
    border-left: none;
  }

  .online-container-{{ section.id }} .time-table td:last-child,
  .online-container-{{ section.id }} .time-table th:last-child {
    border-right: none;
  }
    .online-container-{{ section.id }} .time-table tr td {
      padding: 26px 0;
      line-height: 1.33;
      border-color: rgba(153, 153, 153, 0.10);
    }

    .online-container-{{ section.id }} .time-table .bg0 {
      background-color: #000;
      color: #fff;
      font-weight: 700;
      font-size: 20px;
    }

    .online-container-{{ section.id }} .time-table .fw {
      font-weight: 700;
      font-size: 20px;
    }

    .online-container-{{ section.id }} .time-table tr:first-child td {
      padding: 26px 0;
      border: none;
    }

    .online-container-{{ section.id }} .time-table tr:first-child td:last-child {
      border-top-right-radius: 20px;
    }

    .online-container-{{ section.id }} .time-table.t-pc tr td:first-child,
    .online-container-{{ section.id }} .time-table.t-mb tr:first-child {
      background-color: #000;
      color: #fff;
    }

    .online-container-{{ section.id }} .s-info {
      display: flex;
      gap: 20px;
    }

    .online-container-{{ section.id }} .s-info .info-item {
      border-radius: 20px;
      background-color: #F9F9F9;
      width: 50%;
      text-align: left;
      margin: 0;
      padding: 40px 50px;
    }

    .online-container-{{ section.id }} .info-spec-item {
      display: flex;
      align-items: center;
      gap: 12px;
      margin-bottom: 8px;
    }

    .online-container-{{ section.id }} .s-info .info-item p {
      margin: 0;
      line-height: 1.38;
      color: #000;
      font-size: 18px;
    }

    .online-container-{{ section.id }} .s-info .info-item p.info-t {
      font-size: 24px;
      font-weight: 700;
      line-height: 1.33;
      margin-bottom: 20px;
    }

    .online-container-{{ section.id }} .info-spec p span {
      color: #000;

    }

    @media only screen and (max-width: 768px) {
      .online-container-{{ section.id }}  {
        margin: 50px auto 60px;
        padding: 0 24px;
      }

      .online-container-{{ section.id }} .online-title h3.title {
        font-size: 24px;
        margin-bottom: 10px;
        font-weight: 700;
        line-height: 1.33;
        text-align: left;
      }

      .online-container-{{ section.id }} .online-title div.spec p {
        font-size: 14px;
        text-align: left;
      }

      .online-container-{{ section.id }} .s-info {
        flex-direction: column;
      }

      .online-container-{{ section.id }} .s-info .info-item {
        width: 100%;
        margin: 0;
        padding: 20px;
        font-size: 14px;
        border-radius: 10px;
      }

      .online-container-{{ section.id }} .time-table {
        font-size: 14px;
        border-radius: 10px;
      }

      .online-container-{{ section.id }} .s-time-table {
        margin: 20px 0;
      }

