        text-align: left;
      }

      .online-container-{{ section.id }} .time-table tr td {
        padding: 13px 21px;
        text-align: center;
        border: 1px solid #fff;
      }

      .online-container-{{ section.id }} .time-table tr:first-child td {
        padding: 6px 17px;
        text-align: center;
        font-size: 14px;
        border: 1px solid #fff;
      }
      .online-container-{{ section.id }} .time-table .fw {
        font-weight: 700;
        font-size: 14px;
    }
    .online-container-{{ section.id }} .time-table tr:first-child td:last-child {
      border-top-right-radius: 10px;
  }
      .online-container-{{ section.id }} .s-info .info-item p.info-t {
