      "section-support-faqs": {
        "type": "section-support-faqs",
        "blocks": {
          "faqs_tab_C6aFQf": {
            "type": "faqs_tab",
            "settings": {
              "faqs_tab_t": "General Questions",
              "faqs_q1": "How long will it take to receive the order from the purchase date?",
              "faqs_a1": "The lead time is about 3-5 days, the transit time is about 5-7 days, so you will receive the printer in 7-10 days.",
              "faqs_q2": "Are the printer, ink, and accessories delivered together?",
              "faqs_a2": "Nope, the ink and powder were shipped separately as they belongs to special cargo.",
              "faqs_q3": "Do you have a showroom where I can visit and learn how to use the machine?",
              "faqs_a3": "Yes, we have a showroom in CA.",
              "faqs_q4": "How to check my order?",
              "faqs_a4": "You will receive notification once your order gets shipped, and you can also log in your account on our website to check order status.",
              "faqs_q5": "",
              "faqs_a5": "",
              "faqs_q6": "",
              "faqs_a6": ""
            }
          },
          "faqs_tab_Uz7p6F": {
            "type": "faqs_tab",
            "settings": {
              "faqs_tab_t": "Product Questions",
              "faqs_q1": "Is the RIP software compatible with Mac OS?",
              "faqs_a1": "No, all the RIP softwares are not compatible with Mac OS, you need to install Parallels Software on Mac to use RIP software.",
              "faqs_q2": "How long does it take to print a T-shirt design using DTF L1800?",
              "faqs_a2": "It takes about 9 mins to finish a design with DTF L1800 printer.",
              "faqs_q3": "What materials can UV printers print on?",
              "faqs_a3": "Almost all the hard material can print as long as it has a flat surface, such as wood, metal, acrylic, etc.",
              "faqs_q4": "",
              "faqs_a4": "",
              "faqs_q5": "",
              "faqs_a5": "",
              "faqs_q6": "",
              "faqs_a6": ""
            }
          },
          "faqs_tab_RVeJUG": {
            "type": "faqs_tab",
            "settings": {
              "faqs_tab_t": "Warranty Questions",
              "faqs_q1": "How long is the warranty for the printer?",
              "faqs_a1": "It is 12 months.",
              "faqs_q2": "Does the warranty cover the print head?",
              "faqs_a2": "Yes, within 6 months after your purchase, the print head is eligible for complimentary repairing and one free replacement only.",
              "faqs_q3": "Is there any extended warranty service?",
              "faqs_a3": "Yes, you can purchase extend warranty for your printer.",
              "faqs_q4": "",
              "faqs_a4": "",
              "faqs_q5": "",
              "faqs_a5": "",
              "faqs_q6": "",
              "faqs_a6": ""
            }
          },
          "faqs_tab_tCRCtQ": {
            "type": "faqs_tab",
            "settings": {
              "faqs_tab_t": "Technical Questions",
              "faqs_q1": "Can I use DTF transfer on cups and wooden boards?",
              "faqs_a1": "No, it won’t stick.",
              "faqs_q2": "Can I use UV DTF stickers on clothes?",
              "faqs_a2": "No, it is not suitable.",
              "faqs_q3": "Is white ink necessary for DTF printing?",
              "faqs_a3": "Yes, it is necessary, otherwise the design can be seen through.",
              "faqs_q4": "What is the correct DTF process?",
              "faqs_a4": "check print head status→print your design→add powder→bake the film→heat press and tranfer the film onto fabric→heat press again.",
              "faqs_q5": "",
              "faqs_a5": "",
              "faqs_q6": "",
              "faqs_a6": ""
            }
          },
          "faqs_tab_NiPaFw": {
            "type": "faqs_tab",
            "settings": {
              "faqs_tab_t": "Support Questions",
              "faqs_q1": "If online tech support cannot fix the issue, is there any other way to fix it?",
              "faqs_a1": "You can send your machine to our CA support center to get it repaired.",
              "faqs_q2": "Is Procolored’s tech support for free?",
              "faqs_a2": "Yes, it is free for the first 12 month.",
              "faqs_q3": "Whether tech support is provided when purchasing used machines?",
              "faqs_a3": "Sure, we will provide basic tech support.",
              "faqs_q4": "",
              "faqs_a4": "",
              "faqs_q5": "",
              "faqs_a5": "",
              "faqs_q6": "",
              "faqs_a6": ""
            }
          }
