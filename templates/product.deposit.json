    "section_faqs_aHjJaP": {
      "type": "section-faqs",
      "blocks": {
        "faq_QBtNKt": {
          "type": "faq",
          "settings": {
            "faq_q": "What is Kickstarter?",
            "faq_a": "<p>Kickstarter is the world’s largest & reliable online funding platform for creative projects. Over 10 million people have backed the Kickstarter project and get innovative hardware at a HUGE discount in return. And you will not be charged until the end of the project.</p>"
          }
        },
        "faq_Cxwtxx": {
          "type": "faq",
          "settings": {
            "faq_q": "What is the difference between Deposit and Non-Deposit prices?",
            "faq_a": "<p><strong>$50 Deposit VIPs: </strong>Unlock the exclusive VIP price of <strong>$1299 on Procolored K8</strong> and <strong>$2599 on Procolored K13</strong> during our time-limited VIP Special. </p>"
          }
        },
        "faq_BP8fcG": {
          "type": "faq",
          "settings": {
            "faq_q": "When will Deposits and Discounts be refunded?",
            "faq_a": "<p>Your refundable <strong>$50 deposit</strong> and <strong>additional $100 discount for K8 or $200 for K13</strong> will be refunded <strong>within 14 days</strong> after the Kickstarter campaign concludes.</p>"
          }
        },
        "faq_mJhjGa": {
          "type": "faq",
          "settings": {
            "faq_q": "What if I change my mind after placing the deposit?",
            "faq_a": "<p>No worries! Just email us <a href=\"mailto:tech@procolored.com\" target=\"_blank\" title=\"mailto:tech@procolored.com\"><strong>tech@procolored.com</strong></a> from the email used to place your deposit, and we'll process your refund with 7 working days.</p>"
          }
        },
        "faq_FzA78r": {
          "type": "faq",
          "settings": {
            "faq_q": "Why do I need to use the same email for the deposit and Kickstarter order?",
            "faq_a": "<p>To ensure a smooth refund, we’ll verify the Kickstarter email matches the email used for your deposit. This helps us confirm you qualify for the refund and bonus.</p>"
          }
        },
        "faq_CRhTLB": {
          "type": "faq",
          "settings": {
            "faq_q": "If I want to buy both the K8 and K13, do I need to pay two deposits?",
            "faq_a": "<p>Yes, to secure the prices of both models—$1299 for the K8 and $2599 for the K13—you’ll need to pay a total deposit of $100.</p>"
          }
        }
      },
