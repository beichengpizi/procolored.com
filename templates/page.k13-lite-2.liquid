        <script>
          // 判断元素是否完全在视窗内
          function isElementFullyInViewport(el) {
            const rect = el.getBoundingClientRect();
            console.log('rect.top==', rect.top);
            console.log('rect.bottom ==', rect.bottom);
            return rect.top < 30 && rect.top > -100;
          }

          // 只播放一次的标志
          let k13LiteVideoPlayed = false;

          function handleVideoPlayOnce() {
            if (k13LiteVideoPlayed) return;
            const videoContainer = document.getElementById('k13-lite-video-container');
            console.log('isElementFullyInViewport=====', isElementFullyInViewport(videoContainer));
            if (!videoContainer) return;
            const desktopVideo = videoContainer.querySelector('.desktop-video');
            const mobileVideo = videoContainer.querySelector('.mobile-video');
            const isMobile = window.innerWidth <= 768;

            if (isElementFullyInViewport(videoContainer)) {
              // 只播放一次
              if (desktopVideo && !isMobile) {
                desktopVideo.play && desktopVideo.play();
              }
              if (mobileVideo && isMobile) {
                mobileVideo.play && mobileVideo.play();
              }
              k13LiteVideoPlayed = true;
              // 播放一次后移除监听
              window.removeEventListener('scroll', handleVideoPlayOnce, false);
              window.removeEventListener('resize', handleVideoPlayOnce, false);
            }
          }

          // 监听滚动和窗口变化
          window.addEventListener('scroll', handleVideoPlayOnce, false);
          window.addEventListener('resize', handleVideoPlayOnce, false);

          // DOM加载后初始化
          document.addEventListener('DOMContentLoaded', function () {
            // 移除autoplay属性，防止未进入视窗时自动播放
            const videoContainer2 = document.getElementById('k13-lite-video-container');
            if (videoContainer2) {
              const desktopVideo2 = videoContainer2.querySelector('.desktop-video');
              const mobileVideo2 = videoContainer2.querySelector('.mobile-video');
              if (desktopVideo2) desktopVideo2.removeAttribute('autoplay');
              if (mobileVideo2) mobileVideo2.removeAttribute('autoplay');
              // 提前加载视频资源
              if (desktopVideo2) {
                desktopVideo2.load();
              }
              if (mobileVideo2) {
                mobileVideo2.load();
              }
            }
            handleVideoPlayOnce();
          });
        </script>
