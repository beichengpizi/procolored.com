<script>
  (function () {
    let hasSentDesignOption = false;
    function sendDesignOptionOnce() {
      if (!hasSentDesignOption) {
        hasSentDesignOption = true;
        if (typeof TriplePixel === 'function') {
          TriplePixel('custom', 'Select_DesignOption', {
            value: 1,
          });
        }
      }
    }
    document.addEventListener('DOMContentLoaded', function () {
      const designImgs = document.querySelectorAll('.k13-lite-can-really-do-content-options-item-img');
      designImgs.forEach(function (img) {
        img.addEventListener('click', sendDesignOptionOnce, { passive: true });
      });
    });
  })();
</script>
