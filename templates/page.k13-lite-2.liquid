      const target = document.getElementById(targetId);
      if (!target || !navSection || !lastSection) return;
      const rect = target.getBoundingClientRect();

      // 获取bottom-nav-jump-section自身的rect
      const lastRect = lastSection.getBoundingClientRect();

      // rect.top < 0 说明目标区域已滚出视窗
      // navRect.top < 0 说明底部吸附条自身顶部也已滚出视窗（即页面已滚到底部吸附条之下）
      if (rect.top < 0 && lastRect.top >= 0) {
        if (!lastFixed) {
          navSection.classList.add('bottom-nav-jump-fixed-show');
          lastFixed = true;
        }
      } else {
        if (lastFixed) {
          navSection.classList.remove('bottom-nav-jump-fixed-show');
          lastFixed = false;
