<script>
  (function () {
    // 记录每个深度是否已触发
    let scrollDepth25Sent = false;
    let scrollDepth50Sent = false;
    let scrollDepth75Sent = false;

    function handleScrollDepth() {
      const scrollTop = window.scrollY || window.pageYOffset || document.documentElement.scrollTop || 0;
      const docHeight = Math.max(
        document.body.scrollHeight,
        document.documentElement.scrollHeight,
        document.body.offsetHeight,
        document.documentElement.offsetHeight,
        document.body.clientHeight,
        document.documentElement.clientHeight
      );
      const winHeight = window.innerHeight || document.documentElement.clientHeight || document.body.clientHeight;
      const totalScrollable = docHeight - winHeight;
      if (totalScrollable <= 0) return; // 页面无法滚动

      const percent = scrollTop / totalScrollable;

      if (!scrollDepth25Sent && percent >= 0.25) {
        scrollDepth25Sent = true;
        if (typeof TriplePixel === 'function') {
          TriplePixel('custom', 'ScrollDepth_25', { value: 1 });
        }
      }
      if (!scrollDepth50Sent && percent >= 0.5) {
        scrollDepth50Sent = true;
        if (typeof TriplePixel === 'function') {
          TriplePixel('custom', 'ScrollDepth_50', { value: 1 });
        }
      }
      if (!scrollDepth75Sent && percent >= 0.75) {
        scrollDepth75Sent = true;
        if (typeof TriplePixel === 'function') {
          TriplePixel('custom', 'ScrollDepth_75', { value: 1 });
        }
      }
      // 如果全部已触发，移除监听
      if (scrollDepth25Sent && scrollDepth50Sent && scrollDepth75Sent) {
        window.removeEventListener('scroll', handleScrollDepth);
      }
    }

    document.addEventListener('DOMContentLoaded', function () {
      window.addEventListener('scroll', handleScrollDepth, { passive: true });
      // 首次加载时也检测一次（防止页面已在某个深度）
      handleScrollDepth();
    });

    // 监听id为contact_form的表单提交成功事件，触发TriplePixel事件
    document.addEventListener('DOMContentLoaded', function () {
      var contactForm = document.getElementById('contact_form');
      if (contactForm) {
        // 监听表单提交
        contactForm.addEventListener('submit', function (e) {
          // 由于表单是action到/contact#contact_form，提交后会刷新页面
          // 这里采用在提交前存储一个标记，页面加载后检测是否有该标记
          try {
            sessionStorage.setItem('k13_contact_form_submitted', '1');
          } catch (err) {}
        });
        // 页面加载后检测是否刚刚提交过
        if (window.location.pathname === '/contact' && window.location.hash === '#contact_form') {
          try {
            if (sessionStorage.getItem('k13_contact_form_submitted') === '1') {
              sessionStorage.removeItem('k13_contact_form_submitted');
              if (typeof TriplePixel === 'function') {
                TriplePixel('custom', 'Click_Subscribe', { value: 1 });
              }
            }
          } catch (err) {}
        }
      }
    });
  })();
