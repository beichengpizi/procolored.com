    "custom_liquid_n9MCBc": {
      "type": "custom-liquid",
      "custom_css": [],
      "name": "Custom liquid",
      "settings": {
        "custom_liquid": "<style>\n#shopify-section-{{ section.id }} {\nbackground: linear-gradient(119deg, #FFD7B4 50%, #EEAA67 97.7%);\nposition: relative;\n}\n#shopify-section-{{ section.id }} .left-image {\nposition: absolute;\ntop: 24px;\nwidth: 15%;\ntransform: rotate(-13.546deg);\n\n}\n#shopify-section-{{ section.id }} .right-image {\nposition: absolute;\nright: 0;\nwidth: 15%;\n}\n@media screen and (max-width: 768px) {\n.left-image,\n.right-image,\n.Spend-More-Save-More-1-title-left-icon img {\ndisplay: none;\n}\n}\n</style>\n<div>\n<img class=\"left-image\" src=\"https://cdn.shopify.com/s/files/1/0509/3454/6613/files/image_1202354708.png?v=1766888660\" alt=\"\"/>\n<img class=\"right-image\" src=\"https://cdn.shopify.com/s/files/1/0509/3454/6613/files/image_1202354706.png?v=1766888713\" alt=\"\"/>\n</div>\n\n                            <script>\n                              document.addEventListener(\"DOMContentLoaded\", function(){\n                                const currentSection = document.querySelector(\n                                  '#shopify-section-{{ section.id }}'\n                                );\n\n                                if (!currentSection) return;\n\n                                const nextSection = document.querySelector('#custom-nav-P13-DTF-Printer');\n                                const nextNextSectionWithStand = document.querySelector('#custom-nav-UV-DTF-Hot-Deals-2-With-Stand');\n                                const nextNextSectionWithoutStand = document.querySelector('#custom-nav-UV-DTF-Hot-Deals-2-Without-Stand');\n                                currentSection.appendChild(nextSection);\n                                currentSection.appendChild(nextNextSectionWithStand);\n                                currentSection.appendChild(nextNextSectionWithoutStand);\n                                nextNextSectionWithoutStand.style.display = 'none';\n                                nextNextSectionWithStand.querySelectorAll('.without-stand-button').forEach((item) => {\n                                  item.addEventListener('click', function() {\n                                    nextNextSectionWithoutStand.style.display = 'block';\n                                    nextNextSectionWithStand.style.display = 'none';\n                                    nextNextSectionWithoutStand.querySelectorAll('.without-stand-button').forEach((button) => {\nbutton.classList.add('active');\n});\n                                    nextNextSectionWithoutStand.querySelectorAll('.with-stand-button').forEach((button) => {\nbutton.classList.remove('active');\n});\n                                  });\n                                });\n\n                                nextNextSectionWithoutStand.querySelectorAll('.with-stand-button').forEach((item) => {\n                                  item.addEventListener('click', function(event) {\n                                    nextNextSectionWithStand.style.display = 'block';\n                                    nextNextSectionWithoutStand.style.display = 'none'; \n                                    nextNextSectionWithStand.querySelectorAll('.without-stand-button').forEach((button) => {\nbutton.classList.remove('active');\n});\n                                    nextNextSectionWithStand.querySelectorAll('.with-stand-button').forEach((button) => {\nbutton.classList.add('active');\n})\n                                  });\n                                });\n                              });\n                            </script>"
      }
    },
