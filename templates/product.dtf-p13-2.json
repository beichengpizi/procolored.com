        "custom_liquid": "<style>\n      * {\n        margin: 0;\n        padding: 0;\n        box-sizing: border-box;\n      }\n.printer-features-wrapper{\n    background-color: #fff;\n}\n      .printer-features-container {\n        max-width: 1200px;\n        margin: 0 auto;\n        padding-top: 200px;\n        padding-bottom: 160px;\n      }\n\n      .printer-features-container .features-grid {\n        display: grid;\n        grid-template-columns: 39% 1fr;\n        gap: 16px;\n        margin-bottom: 16px;\n      }\n      .printer-features-container .column-2 {\n        display: grid;\n        grid-template-rows: auto auto;\n        gap: 16px;\n      }\n      .printer-features-container .feature-card {\n        position: relative;\n        border-radius: 20px;\n        overflow: hidden;\n        background: #fff;\n        /* box-shadow: 0 4px 6px rgba(0, 0, 0, 0.1); */\n      }\n\n      .printer-features-container .feature-card img,\n      .printer-features-container .small-feature img {\n        width: 100%;\n        height: 100%;\n        object-fit: cover;\n      }\n\n      .printer-features-container .feature-content {\n        position: absolute;\n        top: 26px;\n        color: white;\n      }\n      .printer-features-container .feature-card.column-1-item .feature-content {\n        left: 36px;\n      }\n      .printer-features-container\n        .feature-card.column-1-item\n        .feature-content\n        .feature-title {\n        font-size: 30px;\n        font-weight: 800;\n        margin-bottom: 12px;\n        line-height: 1.26;\n      }\n      .printer-features-container\n        .feature-card.column-1-item\n        .feature-content\n        .feature-description {\n        font-size: 20px;\nmargin: 0;\n      }\n      .printer-features-container\n        .column-2\n        .feature-card:first-child\n        .feature-content {\n        right: 36px;\n      }\n      .printer-features-container\n        .column-2\n        .feature-card:last-child\n        .feature-content {\n        left: 36px;\n      }\n      .printer-features-container .feature-title {\n        font-size: 24px;\n        font-weight: bold;\n        margin-bottom: 6px;\n        line-height: 1.25;\ncolor: #fff;\n      }\n\n      .printer-features-container .feature-description {\n        font-size: 16px;\n      }\n\n      .printer-features-container .small-features {\n        display: grid;\n        grid-template-columns: 29.3% 29.3% 1fr;\n        gap: 16px;\n      }\n\n      .printer-features-container .small-feature {\n        position: relative;\n        background: #fff9f2;\n        padding: 30px 10px 26px 10px;\n        border-radius: 20px;\n        text-align: center;\n        overflow: hidden;\n      }\n      .printer-features-container .small-feature:last-child {\n        padding: 0;\n      }\n      .printer-features-container .small-feature .small-feature-content {\n        position: absolute;\n        bottom: 26px;\n        left: 36px;\n        color: #fff;\n      }\n\n      .printer-features-container .small-feature svg {\n        min-height: 116px;\n        margin-bottom: 18px;\n      }\n      .printer-features-container .small-feature:first-child svg {\n        width: 128px;\n        height: 116px;\n      }\n      .printer-features-container .small-feature:nth-child(2) svg {\n        width: 83px;\n        height: 98px;\n      }\n\n      .printer-features-container .small-feature h3 {\n        font-size: 24px;\n        font-weight: 800;\n        margin-bottom: 6px;\n        color: #000;\nline-height: 1.2;\n      }\n\n      .printer-features-container .small-feature p {\n        font-size: 16px;\n        color: #000;\nmargin: 0;\nline-height: 1.2;\n      }\n      .printer-features-container .small-feature .small-feature-content h3 {\n        color: #fff;\n      }\n      .printer-features-container .small-feature .small-feature-content p {\n        color: #fff;\n      }\n\n     @media  only screen and (max-width: 768px){\n        .print