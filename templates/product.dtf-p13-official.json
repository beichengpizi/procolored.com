    "custom_liquid_EeHPNc": {
      "type": "custom-liquid",
      "settings": {
        "custom_liquid": "<style>\n\n\n  .image-sizes-wrapper {\n    background-color: #fff;\n  }\n  .image-sizes-wrapper .image-sizes-container {\n    max-width: 1200px;\n    margin: 0 auto;\n    padding-top: 100px;\n    padding-bottom: 100px;\n  }\n\n  .image-sizes-wrapper .image-sizes-container h1 {\n    text-align: left;\n    margin-bottom: 60px;\n    color: #000;\n    font-size: 56px;\n    font-weight: 800;\n    line-height: 1;\n  }\n\n  .image-sizes-wrapper .image-sizes-container .content {\n    display: flex;\n    gap: 120px;\n    align-items: center;\n  }\n\n  .image-sizes-wrapper .image-sizes-container .image-container {\n    flex: 1;\n  }\n\n  .image-sizes-wrapper .image-sizes-container .image-container img {\n    width: 100%;\n    height: auto;\n  }\n\n  .image-sizes-wrapper .image-sizes-container .table-container {\n    flex: 1;\n   \n  }\n\n  .image-sizes-wrapper .image-sizes-container table {\n    width: 100%;\n    border-collapse: collapse;\nborder: none;\n  }\n\n  .image-sizes-wrapper .image-sizes-container th,\n  .image-sizes-wrapper .image-sizes-container td {\n    padding: 20px 0;\n    text-align: left;\n    border: none;\n    border-bottom: 1px solid rgba(204, 204, 204, 0.4);\n  }\n\n  .image-sizes-wrapper .image-sizes-container th {\n    background-color: #fff;\n    font-size: 24px;\n    font-weight: 800;\n    line-height: 1;\n  }\n  .image-sizes-wrapper .image-sizes-container td {\n    font-size: 18px;\n    font-weight: 400;\n    line-height: 1;\n  }\n  .image-sizes-wrapper .desktop-image{\n      display: block;\n    }\n    .image-sizes-wrapper  .mobile-image{\n      display: none;\n    }\n  @media only screen and (max-width: 768px) {\n    .image-sizes-wrapper .image-sizes-container {\n      padding-top: 50px;\n      padding-bottom: 10px;\n      padding-left: 24px;\n      padding-right: 24px;\n    }\n\n    .image-sizes-wrapper .image-sizes-container h1 {\n      margin-bottom: 30px;\n\n      font-size: 26px;\n    }\n    .image-sizes-wrapper .image-sizes-container .content {\n      flex-direction: column;\n    }\n\n    .image-sizes-wrapper .image-sizes-container .image-container {\n      width: 64%;\n    }\n    .image-sizes-wrapper .image-sizes-container .table-container {\n      width: 100%;\n    }\n\n    .image-sizes-wrapper .image-sizes-container .table-container {\n      overflow-x: auto;\n    }\n    .image-sizes-wrapper .image-sizes-container .content {\n      gap: 20px;\n      align-items: center;\n    }\n    .image-sizes-wrapper .image-sizes-container .table-container {\n      margin-top: 0;\n    }\n    .image-sizes-wrapper .image-sizes-container th,\n    .image-sizes-wrapper .image-sizes-container td {\n      padding: 10px;\n      text-align: left;\n      border-bottom: 1px solid rgba(204, 204, 204, 0.4);\n    }\n\n    .image-sizes-wrapper .image-sizes-container th {\n      font-size: 14px;\nfont-weight: 700;\n      line-height: 1;\n    }\n    .image-sizes-wrapper .image-sizes-container td {\n      font-size: 12px;\n\n      line-height: 1;\n    }\n    .image-sizes-wrapper .image-sizes-container tr:last-child td {\n      border-bottom: none;\n    }\n\n   \n    .image-sizes-wrapper .desktop-image{\n        display: none;\n      }\n      .image-sizes-wrapper  .mobile-image{\n        display: block;\n      }\n    \n  }\n</style>\n<div class=\"image-sizes-wrapper\">\n  <div class=\"image-sizes-container\">\n    <h1>Print Sizes</h1>\n    <div class=\"content\">\n      <div class=\"image-container\">\n        <img\n          class=\"lazyload desktop-image\"\n          data-src=\"https://cdn.shopify.com/s/files/1/0509/3454/6613/files/pc_983c0d96-36aa-4c9e-8fff-351b24191a26.png?v=1744254490&width=600\"\n          alt=\"Family illustration with size indicators\"\n        />\n        <img\n          class=\"lazyload mobile-image\"\n          data-src=\"https://cdn.shopify.com/s/files/1/0509/3454/6613/files/pc_983c0d96-36aa-4c9e-8fff-351b24191a26.png?v=1744254490&width=300\"\n          alt=\"Family illustration with size indicators\"\n        />\n      </div>\n      <div class=\"table-container\">\n        <table>\n          <thead>\n            <tr>\n              <th>DTF Prints</th>\n              <th>Common Size</th>\n              <th>Print Speed</th>\n            </tr>\n          </thead>\n          <tbody>\n            <tr>\n              <td>Adult</td>\n              <td>8.5\"x11\"</td>\n              <td>8 pcs/h</td>\n            </tr>\n            <tr>\n              <td>Kid</td>\n              <td>8.5\"x8.5\"</td>\n              <td>11 pcs/h</td>\n            </tr>\n            <tr>\n              <td>Puppy</td>\n              <td>5.5\"x5.5\"</td>\n              <td>24 pcs/h</td>\n            </tr>\n            <tr>\n              <td>Heart</td>\n              <td>4.5\"x4.5\"</td>\n              <td>38 pcs/h</td>\n            </tr>\n            <tr>\n              <td>Sleeve</td>\n              <td>2\"x11.5\"</td>\n              <td>34 pcs/h</td>\n            </tr>\n            <tr>\n              <td>Leg</td>\n              <td>2.5\"x14\"</td>\n              <td>22 pcs/h</td>\n            </tr>\n            <tr>\n              <td>Cap</td>\n              <td>5\"x3\"</td>\n              <td>52 pcs/h</td>\n            </tr>\n            <tr>\n              <td>Visor</td>\n              <td>4.75\"x2\"</td>\n              <td>82 pcs/h</td>\n            </tr>\n          </tbody>\n        </table>\n      </div>\n    </div>\n  </div>\n</div>"
