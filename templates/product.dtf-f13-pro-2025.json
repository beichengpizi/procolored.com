      "settings": {
        "custom_liquid": "<style>\n        /* 参数表格模块 */\n        .complete-your-workflow-wrapper {\n            background: #f9f9f9;\n            padding-top: 100px;\n        }\n\n        .complete-your-workflow-title-wrap {\n            display: flex;\n            flex-direction: column;\n            align-items: center;\n            gap: 20px;\n            margin-bottom: 50px;\n        }\n\n        .complete-your-workflow-title {\n            color: #000;\n            text-align: center;\n            font-size: 40px;\n            font-weight: 800;\n            line-height: 1.22;\n\n        }\n\n        .complete-your-workflow-subtitle {\n            color: #3D3D3D;\n            text-align: center;\n            font-size: 18px;\n            font-weight: 400;\n            line-height: 1.38;\n        }\n\n        .complete-your-workflow-wrapper .comparison-table-container {\n            display: grid;\n            grid-template-columns: 1fr;\n            grid-template-rows: repeat(9, auto);\n            max-width: 1200px;\n            margin: 0 auto;\n            background: #ffffff;\n            border-radius: 20px;\n            overflow: hidden;\n            padding: 46px 55px 20px 55px;\n        }\n\n        .complete-your-workflow-wrapper .comparison-table-item {\n            background-color: #ffffff;\n            border-bottom: 1px solid #f5f5f5;\n            display: grid;\n            grid-template-columns: repeat(3, 1fr);\n        }\n\n        .complete-your-workflow-wrapper .comparison-table-item:first-child {\n            border-bottom: none;\n        }\n\n        .complete-your-workflow-wrapper .comparison-table-item:last-child {\n            border-bottom: none;\n        }\n\n        .complete-your-workflow-wrapper .cost-breakdown {\n            text-align: start;\n        }\n\n        .complete-your-workflow-wrapper .printer-image {\n            margin-bottom: 30px;\n            max-width: 100%;\n            width: 100%;\n        }\n\n        .complete-