        // 获取tab容器
        const tabContainer = document.getElementById("black-friday-winning-video-tabs");
        if (!tabContainer) return;

        // 获取所有tab元素
        const tabs = tabContainer.querySelectorAll(".black-friday-winning-video-tab");

        // 绑定每个tab的点击事件
        tabs.forEach((tab, idx) => {
            tab.addEventListener("click", function () {
                // 1. 移除所有tab上的active类（高亮），保证只有一个tab高亮
                tabs.forEach(t => t.classList.remove("active"));

                // 2. 当前点击的tab添加active类
                tab.classList.add("active");

                // 3. 获取当前tab对应的视频数据，若无则为空数组
                const currData = (videoData[idx] && videoData[idx].content) ? videoData[idx].content : [];

                // 4. 更新swiper轮播区的视频内容
                document.querySelectorAll('.black-friday-winning-video-swiper-slide').forEach((slide, i) => {
                    // 获取视频链接和缩略图元素
                    const videoLink = slide.querySelector('.black-friday-winning-video-video-link');
                    const videoImg = slide.querySelector('.black-friday-winning-video-video-thumbnail');
                    // 若存在视频数据则更新相关属性
                    if (videoLink && videoImg && currData[i]) {
                        videoLink.setAttribute('href', currData[i].video_url);
                        videoImg.setAttribute('src', currData[i].video_poster);
                        videoImg.setAttribute('alt', currData[i].video_alt);
                    }
                });

                // 5. 更新网格区视频内容
                document.querySelectorAll('.black-friday-winning-video-grid-item').forEach((item, i) => {
                    const videoLink = item.querySelector('.black-friday-winning-video-video-link');
                    const videoImg = item.querySelector('.black-friday-winning-video-video-thumbnail');
                    if (videoLink && videoImg && currData[i]) {
                        videoLink.setAttribute('href', currData[i].video_url);
                        videoImg.setAttribute('src', currData[i].video_poster);
                        videoImg.setAttribute('alt', currData[i].video_alt);
                    }
                });

               // 6. 使当前点击的tab横向滚动至容器中间（适配移动端横滑）
                    // 滚动逻辑抽出为函数，每次高亮active时都调用，包括初始化
                    function scrollTabIntoView(activeTab) {
                        const tabCenter = activeTab.offsetLeft + activeTab.offsetWidth / 2;
                        const scrollTo = tabCenter - tabContainer.clientWidth / 2;
                        tabContainer.scrollTo({
                            left: scrollTo,
                            behavior: "smooth"
                        });
                    }
                    scrollTabIntoView(tab);
            });
        });

    });
</script>

