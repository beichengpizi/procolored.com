    "custom_product_faqs_Wkbz3m": {
      "type": "custom-product-faqs",
      "blocks": {
        "faq_tab_JH8HA3": {
          "type": "faq_tab",
          "settings": {
            "question": "1. Can I use multiple gift cards on one order?",
            "answer": "<p>Yes. You can apply multiple gift cards to a single order at checkout. Simply enter each gift card code one by one.<br/></p>"
          }
        },
        "faq_tab_TtFDFP": {
          "type": "faq_tab",
          "settings": {
            "question": "2. Can I use my gift card for multiple orders?",
            "answer": "<p>Yes. If your gift card balance is not fully used in one order, the remaining balance can be used for future purchases until it is completely spent.</p>"
          }
        },
        "faq_tab_LAWVRW": {
          "type": "faq_tab",
          "settings": {
            "question": "3. How do I receive my gift card after purchase?",
            "answer": "<p>After a gift card is purchased, the customer or recipient will receive a digital gift card email containing a unique gift card code and a \"View gift card\" button.</p>"
          }
        },
        "faq_tab_ypAhra": {
          "type": "faq_tab",
          "settings": {
            "question": "4. How do I redeem a gift card online?",
            "answer": "<p>At checkout, enter your gift card code in the \"Gift card or discount code\" field and click Apply. The balance will be automatically deducted from your order total.</p>"
          }
        },
        "faq_tab_4rdWV4": {
          "type": "faq_tab",
          "settings": {
            "question": "5. Do gift cards expire?",
            "answer": "<p>Yes. Gift cards have an <strong>expiration date</strong>, which is clearly stated in the <strong>gift card notification email</strong>.<br/><br/>Please make sure to use your gift card <strong>before the expiration date</strong>, as expired gift cards cannot be redeemed.</p>"
          }
        },
        "faq_tab_cyDbWk": {
          "type": "faq_tab",
          "settings": {
            "question": "6. Can gift cards be refunded or exchanged for cash?",
            "answer": "<p>No. Gift cards cannot be refunded, exchanged for cash, or replaced if lost, so please keep your gift card code secure.</p>"
          }
        }
      },
