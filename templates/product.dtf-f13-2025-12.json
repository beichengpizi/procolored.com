      "blocks": {
        "print_speed_WbkHzd": {
          "type": "print_speed",
          "settings": {
            "tab_name": "Print Speed",
            "content_html": "<style>\n\n\n  .image-sizes-wrapper {\n    background-color: #fff;\n  }\n  .image-sizes-wrapper .image-sizes-container {\n    max-width: 1200px;\n    margin: 0 auto;\n \n  }\n\n  .image-sizes-wrapper .image-sizes-container h1 {\n    text-align: left;\n    margin-bottom: 60px;\n    color: #000;\n    font-size: 56px;\n    font-weight: 800;\n    line-height: 1;\n  }\n\n  .image-sizes-wrapper .image-sizes-container .content {\n    display: flex;\n    gap: 120px;\n    align-items: center;\n  }\n\n  .image-sizes-wrapper .image-sizes-container .image-container {\n    flex: 1;\n  }\n\n  .image-sizes-wrapper .image-sizes-container .image-container img {\n    width: 100%;\n    height: auto;\n  }\n\n  .image-sizes-wrapper .image-sizes-container .table-container {\n    flex: 1;\n   \n  }\n\n  .image-sizes-wrapper .image-sizes-container table {\n    width: 100%;\n    border-collapse: collapse;\nborder: none;\n  }\n\n  .image-sizes-wrapper .image-sizes-container th,\n  .image-sizes-wrapper .image-sizes-container td {\n    padding: 20px 0;\n    text-align: left;\n    border: none;\n    border-bottom: 1px solid rgba(204, 204, 204, 0.4);\n  }\n\n  .image-sizes-wrapper .image-sizes-container th {\n    background-color: #fff;\n    font-size: 24px;\n    font-weight: 800;\n    line-height: 1;\n  }\n  .image-sizes-wrapper .image-sizes-container td {\n    font-size: 18px;\n    font-weight: 400;\n    line-height: 1;\n  }\n  .image-sizes-wrapper .desktop-image{\n      display: block;\n    }\n    .image-sizes-wrapper  .mobile-image{\n      display: none;\n    }\n  @media only screen and (max-width: 768px) {\n    .image-sizes-wrapper .image-sizes-container {\n\n      padding-left: 24px;\n      padding-right: 24px;\n    }\n\n    .image-sizes-wrapper .image-sizes-container h1 {\n      margin-bottom: 30px;\n\n      font-size: 26px;\n    }\n    .image-sizes-wrapper .image-sizes-container .content {\n      flex-direction: column;\n    }\n\n    .image-sizes-wrapper .image-sizes-container .image-container {\n      width: 64%;\n    }\n    .image-sizes-wrapper .image-sizes-container .table-container {\n      width: 100%;\n    }\n\n    .image-sizes-wrapper .image-sizes-container .table-container {\n      overflow-x: auto;\n    }\n    .image-sizes-wrapper .image-sizes-container .content {\n      gap: 20px;\n      align-items: center;\n    }\n    .image-sizes-wrapper .image-sizes-container .table-container {\n      margin-top: 0;\n    }\n    .image-sizes-wrapper .image-sizes-container th,\n    .image-sizes-wrapper .image-sizes-container td {\n      padding: 10px;\n      text-align: left;\n      border-bottom: 1px solid rgba(204, 204, 204, 0.4);\n    }\n\n    .image-sizes-wrapper .image-sizes-container th {\n      font-size: 14px;\nfont-weight: 700;\n      line-height: 1;\n    }\n    .image-sizes-wrapper .image-sizes-container td {\n      font-size: 12px;\n\n      line-height: 1;\n    }\n    .image-sizes-wrapper .image-sizes-container tr:last-child td {\n      border-bottom: none;\n    }\n\n   \n    .image-sizes-wrapper .desktop-image{\n        display: none;\n      }\n      .image-sizes-wrapper  .mobile-image{\n        display: block;\n      }\n    \n  }\n</style>\n<div class=\"image-sizes-wrapper\">\n  <div class=\"image-sizes-container\">\n\n    <div class=\"content\">\n      <div class=\"image-container\">\n        <img\n          class=\"lazyload desktop-image\"\n          data-src=\"https://cdn.shopify.com/s/files/1/0509/3454/6613/files/pc_983c0d96-36aa-4c9e-8fff-351b24191a26.png?v=1744254490&width=600\"\n          alt=\"Family illustration with size indicators\"\n        />\n        <img\n          class=\"lazyload mobile-image\"\n          data-src=\"https://cdn.shopify.com/s/files/1/0509/3454/6613/files/pc_983c0d96-36aa-4c9e-8fff-351b24191a26.png?v=1744254490&width=300\"\n          alt=\"Family illustration with size indicators\"\n        />\n      </div>\n      <div class=\"table-container\">\n        <table>\n          <thead>