            let dragging = false;
            let resizing = false;
            let resizeDir = null;
            let startX = 0, startY = 0;
            let originLeft = 0, originTop = 0;
            let originWidth = 0, originHeight = 0;
            let scale = 1;
            let minScale = 0.3, maxScale = 3;
            let minWidth = 40, minHeight = 40, maxWidth = 800, maxHeight = 800;

            // 记录当前的百分比坐标和尺寸
            let lastPercentPosition = null;

            // 禁止容器被拖拽
            const container = document.querySelector('.ugc-upload-your-inspiration-content-not-uploaded-popup-detail-img-content');
            if (container) {
                container.addEventListener('mousedown', function(e) {
                    if (!e.target.closest('.ugc-upload-textures-img')) {
                        e.preventDefault();
                        e.stopPropagation();
                    }
                    console.log('container mousedown');
                });
                container.addEventListener('touchstart', function(e) {
                    if (!e.target.closest('.ugc-upload-textures-img')) {
                        e.preventDefault();
                        e.stopPropagation();
                        console.log('container touchstart');
                    }
                }, { passive: false });
            }

            function getContainerRect() {
                return modelImg.getBoundingClientRect();
            }
            function getDragRect() {
                return dragEl.getBoundingClientRect();
            }
            // clampPosition动态获取缩放后的newWidth和newHeight的值并除于2
            function clampPosition(left, top, width, height) {
                const cRect = getContainerRect();
                let newWidth = width;
                let newHeight = height;
                // 动态获取缩放后的newWidth和newHeight的值
                // 这里假设width和height已经是缩放后的值
                let newLeft = left;
                let newTop = top;
                // 关键：用缩放后的newWidth/2和newHeight/2来限制边界
                if (newLeft < newWidth / 2) newLeft = newWidth / 2;
                if (newTop < newHeight / 2) newTop = newHeight / 2;
                if (newLeft + newWidth / 2 > cRect.width - newWidth / 2) newLeft = cRect.width - newWidth / 2;
                if (newTop + newHeight / 2 > cRect.height - newHeight / 2) newTop = cRect.height - newHeight / 2;
                // 但通常left/top是左上角，所以我们需要保证left >= 0, top >= 0, left+width <= cRect.width, top+height <= cRect.height
                // 结合原有逻辑，采用如下方式
                if (newLeft < 0) newLeft = 0;
                if (newTop < 0) newTop = 0;
                if (newLeft + newWidth > cRect.width) newLeft = cRect.width - newWidth;
                if (newTop + newHeight > cRect.height) newTop = cRect.height - newHeight;
                if (newWidth < minWidth) newWidth = minWidth;
                if (newHeight < minHeight) newHeight = minHeight;
                if (newWidth > maxWidth) newWidth = maxWidth;
                if (newHeight > maxHeight) newHeight = maxHeight;
                return { left: newLeft, top: newTop, width: newWidth, height: newHeight };
            }
            // 获取相对于img的百分比定位坐标和尺寸
            function getRelativePositionAndSize() {
                const containerRect = getContainerRect();
                const containerWidth = containerRect.width;
                const containerHeight = containerRect.height;
                const left = parseFloat(dragEl.style.left) || 0;
                const top = parseFloat(dragEl.style.top) || 0;
                const width = parseFloat(dragEl.style.width) || dragEl.offsetWidth;
                const height = parseFloat(dragEl.style.height) || dragEl.offsetHeight;
                return {
                    left: containerWidth ? (left / containerWidth) * 100 : 0,
                    top: containerHeight ? (top / containerHeight) * 100 : 0,
                    width: containerWidth ? (width / containerWidth) * 100 : 0,
                    height: containerHeight ? (height / containerHeight) * 100 : 0
                };
            }
            // 根据百分比坐标和尺寸设置dragEl的位置和大小
            function setDragElByPercent(pos) {
                const containerRect = getContainerRect();
                if (!containerRect.width || !containerRect.height) return;
                dragEl.style.left = (pos.left / 100) * containerRect.width + 'px';
                dragEl.style.top = (pos.top / 100) * containerRect.height + 'px';
                dragEl.style.width = (pos.width / 100) * containerRect.width + 'px';
                dragEl.style.height = (pos.height / 100) * containerRect.height + 'px';
            }

            // 拖拽事件
            function onPointerDown(e) {
                if (e.target.classList.contains('resize-handle')) return;
                e.preventDefault();
                e.stopPropagation();
                dragging = true;
                dragEl.classList.add('dragging');
                if (e.type.startsWith('touch')) {
                    startX = e.touches[0].clientX;
                    startY = e.touches[0].clientY;
                } else {
                    startX = e.clientX;
                    startY = e.clientY;
                }
                var dataType = dragEl.getAttribute('data-type');
                if (dataType === 'clothes') {
                    const containerRect = getContainerRect();
                    originLeft = parseFloat(dragEl.style.left) || (containerRect.width * 0.5);
                    originTop = parseFloat(dragEl.style.top) || (containerRect.height * 0.448);
                } else if (dataType === 'hat') {
                    const containerRect = getContainerRect();
                    originLeft = parseFloat(dragEl.style.left) || (containerRect.width * 0.5);
                    originTop = parseFloat(dragEl.style.top) || (containerRect.height * 0.468);
                } else if (dataType === 'bag') {
                    const containerRect = getContainerRect();
                    originLeft = parseFloat(dragEl.style.left) || (containerRect.width * 0.5);
                    originTop = parseFloat(dragEl.style.top) || (containerRect.height * 0.658);
                } else {
                    originLeft = parseFloat(dragEl.style.left) || 0;
                    originTop = parseFloat(dragEl.style.top) || 0;
                }
                if (isMobile()) {
                    document.addEventListener('touchmove', onPointerMove, { passive: false });
                    document.addEventListener('touchend', onPointerUp, { passive: false });
                } else {
                    document.addEventListener('mousemove', onPointerMove);
                    document.addEventListener('mouseup', onPointerUp);
                }
                console.log('开始拖拽');
                console.log('当前坐标:', originLeft, originTop)
            }

            // 拖拽移动时，动态更新坐标和宽高的百分比值
            function updateDragPercentDisplay() {
                // 获取当前相对img的坐标和尺寸（百分比）
                const pos = getRelativePositionAndSize();
                // 记录当前百分比位置
                lastPercentPosition = pos;
                // 可以在此处将pos显示到页面上，或用于其他用途
                // 这里仅做console输出
                console.log('拖拽中，当前ugc-upload-textures-img相对于#ugc-upload-model-img的坐标和尺寸(百分比):', pos);
            }

            function onPointerMove(e) {
                if (!dragging) return;
                let clientX, clientY;
                if (e.type.startsWith('touch')) {
                    clientX = e.touches[0].clientX;
                    clientY = e.touches[0].clientY;
                } else {
                    clientX = e.clientX;
                    clientY = e.clientY;
                }
                let deltaX = clientX - startX;
                let deltaY = clientY - startY;
                let newLeft = originLeft + deltaX;
                let newTop = originTop + deltaY;
                let width = parseFloat(dragEl.style.width) || dragEl.offsetWidth;
                let height = parseFloat(dragEl.style.height) || dragEl.offsetHeight;
                const pos = clampPosition(newLeft, newTop, width, height);
                dragEl.style.left = pos.left + 'px';
                dragEl.style.top = pos.top + 'px';

                // 拖拽时动态输出当前坐标和宽高百分比
                updateDragPercentDisplay();
                console.log('onPointerMove', pos.left, pos.top)
            }

            function onPointerUp(e) {
                dragging = false;
                dragEl.classList.remove('dragging');
                if (isMobile()) {
                    document.removeEventListener('touchmove', onPointerMove);
                    document.removeEventListener('touchend', onPointerUp);
                } else {
                    document.removeEventListener('mousemove', onPointerMove);
                    document.removeEventListener('mouseup', onPointerUp);
                }
                // 先强制刷新dragEl的left/top样式，确保最新
                // 这里直接获取最新的pos
                let pos = getRelativePositionAndSize();

                // 记录当前百分比位置
                lastPercentPosition = pos;

                // 获取ID为ugc-upload-img-capture-area下的.ugc-upload-textures-Original-size-img元素
                // 将originalTextureEl对应的值改为pos对应的值，并输出
                let originalTextureEl = document.querySelector('#ugc-upload-img-capture-area .ugc-upload-textures-Original-size-img');
                if (originalTextureEl) {
                    // 直接将pos的百分比值赋值到originalTextureEl的style上
                    originalTextureEl.style.position = 'absolute';
                    originalTextureEl.style.top = pos.top + '%';
                    originalTextureEl.style.left = pos.left + '%';
                    originalTextureEl.style.width = pos.width + '%';
                    originalTextureEl.style.height = pos.height + '%';
                    // 输出设置后的style
                    console.log('已将originalTextureEl的样式设置为pos对应的百分比值:', {
                        top: originalTextureEl.style.top,
                        left: originalTextureEl.style.left,
                        width: originalTextureEl.style.width,
                        height: originalTextureEl.style.height
                    });
                }
        
                console.log('拖拽结束，当前ugc-upload-textures-img相对于#ugc-upload-model-img的坐标和尺寸(百分比):', pos);
            }
            dragEl.addEventListener('mousedown', onPointerDown);
            dragEl.addEventListener('touchstart', onPointerDown, { passive: false });

            // 缩放功能（鼠标滚轮缩放时与角拖拽缩放保持一致，直接改变宽高和位置）
            dragEl.addEventListener('wheel', function (e) {
                e.preventDefault();
                e.stopPropagation();
                // 以中心点为基准缩放
                let rect = dragEl.getBoundingClientRect();
                let parentRect = dragEl.parentElement.getBoundingClientRect();
                let originWidth = parseFloat(dragEl.style.width) || dragEl.offsetWidth;
                let originHeight = parseFloat(dragEl.style.height) || dragEl.offsetHeight;
                let originLeft = parseFloat(dragEl.style.left) || dragEl.offsetLeft;
                let originTop = parseFloat(dragEl.style.top) || dragEl.offsetTop;

                // 缩放比例
                let scaleDelta = e.deltaY < 0 ? 1.05 : 0.95;
                let minWidth = 40, minHeight = 40;
                let maxWidth = parentRect.width, maxHeight = parentRect.height;

                let newWidth = originWidth * scaleDelta;
                let newHeight = originHeight * scaleDelta;

                // 限制最小最大宽高
                if (newWidth < minWidth) newWidth = minWidth;
                if (newHeight < minHeight) newHeight = minHeight;
                if (newWidth > maxWidth) newWidth = maxWidth;
                if (newHeight > maxHeight) newHeight = maxHeight;

                // 以中心缩放，调整left/top
                let centerX = originLeft + originWidth / 2;
                let centerY = originTop + originHeight / 2;
                // 动态获取缩放后的newWidth和newHeight的值并除于2
                let newLeft = centerX - (newWidth / 2);
                let newTop = centerY - (newHeight / 2);

                // 限制在容器内
                if (newLeft < 0) newLeft = 0;
                if (newTop < 0) newTop = 0;
                if (newLeft + newWidth > parentRect.width) newLeft = parentRect.width - newWidth;
                if (newTop + newHeight > parentRect.height) newTop = parentRect.height - newHeight;

                dragEl.style.width = newWidth + 'px';
                dragEl.style.height = newHeight + 'px';
                dragEl.style.left = newLeft + 'px';
                dragEl.style.top = newTop + 'px';

                // 缩放时动态输出当前坐标和宽高百分比
                if (typeof updateResizePercentDisplay === 'function') {
                    updateResizePercentDisplay();
                }
                // 记录当前百分比位置
                lastPercentPosition = getRelativePositionAndSize();
                console.log('wheel:')
            });

            // 触摸缩放（双指缩放，逻辑同上）
            let lastDist = null;
            dragEl.addEventListener('touchstart', function (e) {
                if (e.touches.length === 2) {
                    lastDist = Math.hypot(
                        e.touches[0].clientX - e.touches[1].clientX,
