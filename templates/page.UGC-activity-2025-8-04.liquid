                        // 限制在容器内
                        if (newLeft < 0) newLeft = 0;
                        if (newTop < 0) newTop = 0;
                        if (newLeft + newWidth > parentRect.width) newLeft = parentRect.width - newWidth;
                        if (newTop + newHeight > parentRect.height) newTop = parentRect.height - newHeight;

                        dragEl.style.width = newWidth + 'px';
                        dragEl.style.height = newHeight + 'px';
                        dragEl.style.left = newLeft + 'px';
                        dragEl.style.top = newTop + 'px';

                        // 缩放时动态输出当前坐标和宽高百分比
                        if (typeof updateResizePercentDisplay === 'function') {
                            updateResizePercentDisplay();
                        }
                        // 记录当前百分比位置
                        lastPercentPosition = getRelativePositionAndSize();
                    }
                    lastDist = dist;
                }
            }, { passive: false });
            dragEl.addEventListener('touchend', function (e) {
                if (e.touches.length < 2) {
                    lastDist = null;
                }
                console.log('触摸双指缩放结束');
            });

            // 角拖拽缩放
            const handles = dragEl.querySelectorAll('.resize-handle');
            handles.forEach(handle => {
                handle.addEventListener('mousedown', function (e) {
                    e.preventDefault();
                    e.stopPropagation();
                    resizing = true;
                    resizeDir = handle.classList.contains('nw') ? 'nw' :
                                handle.classList.contains('ne') ? 'ne' :
                                handle.classList.contains('sw') ? 'sw' : 'se';
                    startX = e.clientX;
                    startY = e.clientY;
                    originLeft = parseFloat(dragEl.style.left) || 0;
                    originTop = parseFloat(dragEl.style.top) || 0;
                    originWidth = parseFloat(dragEl.style.width) || dragEl.offsetWidth;
                    originHeight = parseFloat(dragEl.style.height) || dragEl.offsetHeight;
                    document.addEventListener('mousemove', onResizeMove);
                    document.addEventListener('mouseup', onResizeUp);
                });
                handle.addEventListener('touchstart', function (e) {
                    e.preventDefault();
                    e.stopPropagation();
                    resizing = true;
                    resizeDir = handle.classList.contains('nw') ? 'nw' :
                                handle.classList.contains('ne') ? 'ne' :
                                handle.classList.contains('sw') ? 'sw' : 'se';
                    if (e.touches.length > 0) {
                        startX = e.touches[0].clientX;
                        startY = e.touches[0].clientY;
                    }
                    originLeft = parseFloat(dragEl.style.left) || 0;
                    originTop = parseFloat(dragEl.style.top) || 0;
                    originWidth = parseFloat(dragEl.style.width) || dragEl.offsetWidth;
                    originHeight = parseFloat(dragEl.style.height) || dragEl.offsetHeight;
                    document.addEventListener('touchmove', onResizeMove, { passive: false });
                    document.addEventListener('touchend', onResizeUp, { passive: false });
                }, { passive: false });
                console.log('handle', handle);
            });

            // 缩放时动态输出当前坐标和宽高百分比
            function updateResizePercentDisplay() {
                const pos = getRelativePositionAndSize();
                // 记录当前百分比位置
                lastPercentPosition = pos;
                // 将originalTextureEl对应的值改为pos对应的值，并输出
                let originalTextureEl = document.querySelector('#ugc-upload-img-capture-area .ugc-upload-textures-Original-size-img');
                if (originalTextureEl) {
                    // 直接将pos的百分比值赋值到originalTextureEl的style上
                    originalTextureEl.style.position = 'absolute';
                    originalTextureEl.style.top = pos.top + '%';
                    originalTextureEl.style.left = pos.left + '%';
                    originalTextureEl.style.width = pos.width + '%';
                    originalTextureEl.style.height = pos.height + '%';
                    // 输出设置后的style
                    console.log('已将originalTextureEl的样式设置为pos对应的百分比值:', {
                        top: originalTextureEl.style.top,
                        left: originalTextureEl.style.left,
                        width: originalTextureEl.style.width,
                        height: originalTextureEl.style.height
                    });
                }
                console.log('缩放中，当前ugc-upload-textures-img相对于#ugc-upload-model-img的坐标和尺寸(百分比):', pos);
            }

            function onResizeMove(e) {
                if (!resizing) return;
                let clientX, clientY;
                if (e.type.startsWith('touch')) {
                    clientX = e.touches[0].clientX;
                    clientY = e.touches[0].clientY;
                } else {
                    clientX = e.clientX;
                    clientY = e.clientY;
                }
                let deltaX = clientX - startX;
                let deltaY = clientY - startY;
                let newLeft = originLeft, newTop = originTop;
                let newWidth = originWidth, newHeight = originHeight;
                const aspect = dragImg.naturalWidth / dragImg.naturalHeight;
                if (resizeDir === 'nw') {
                    newWidth = originWidth - deltaX;
                    newHeight = originHeight - deltaY;
                    if (Math.abs(deltaX) > Math.abs(deltaY)) {
                        newHeight = newWidth / aspect;
                    } else {
                        newWidth = newHeight * aspect;
                    }
                    newLeft = originLeft + (originWidth - newWidth);
                    newTop = originTop + (originHeight - newHeight);
                } else if (resizeDir === 'ne') {
                    newWidth = originWidth + deltaX;
                    newHeight = originHeight - deltaY;
                    if (Math.abs(deltaX) > Math.abs(deltaY)) {
                        newHeight = newWidth / aspect;
                    } else {
                        newWidth = newHeight * aspect;
                    }
                    newTop = originTop + (originHeight - newHeight);
                } else if (resizeDir === 'sw') {
                    newWidth = originWidth - deltaX;
                    newHeight = originHeight + deltaY;
                    if (Math.abs(deltaX) > Math.abs(deltaY)) {
                        newHeight = newWidth / aspect;
                    } else {
                        newWidth = newHeight * aspect;
                    }
                    newLeft = originLeft + (originWidth - newWidth);
                } else if (resizeDir === 'se') {
                    newWidth = originWidth + deltaX;
                    newHeight = originHeight + deltaY;
                    if (Math.abs(deltaX) > Math.abs(deltaY)) {
                        newHeight = newWidth / aspect;
                    } else {
                        newWidth = newHeight * aspect;
                    }
                }
                const pos = clampPosition(newLeft, newTop, newWidth, newHeight);
                dragEl.style.left = pos.left + 'px';
                dragEl.style.top = pos.top + 'px';
                dragEl.style.width = pos.width + 'px';
                dragEl.style.height = pos.height + 'px';
                dragImg.style.width = '100%';
                dragImg.style.height = '100%';
                dragImg.style.transform = `scale(1)`;

                // 缩放时动态输出当前坐标和宽高百分比
                updateResizePercentDisplay();
            }
            function onResizeUp(e) {
                resizing = false;
                resizeDir = null;
                document.removeEventListener('mousemove', onResizeMove);
                document.removeEventListener('mouseup', onResizeUp);
                document.removeEventListener('touchmove', onResizeMove);
                document.removeEventListener('touchend', onResizeUp);
                // 输出当前相对img的坐标和尺寸（百分比）
                const pos = getRelativePositionAndSize();
                // 记录当前百分比位置
                lastPercentPosition = pos;
                console.log('缩放结束，当前ugc-upload-textures-img相对于#ugc-upload-model-img的坐标和尺寸(百分比):', pos);
            }

            // 初始化时输出一次坐标和尺寸
            window.getUgcTextureImgPosition = function () {
                return getRelativePositionAndSize();
            }

            // 监听modelImg的src变化，保持dragEl的坐标和尺寸不变
            // 兼容直接赋值和属性变化
            function observeModelImgSrcChange() {
                let lastSrc = modelImg.src;
                // 监听属性变化
                const observer = new MutationObserver(function (mutationsList) {
                    for (let mutation of mutationsList) {
                        if (mutation.type === 'attributes' && mutation.attributeName === 'src') {
                            let newSrc = modelImg.src;
                            if (newSrc !== lastSrc) {
                                lastSrc = newSrc;
                                // 保持dragEl的坐标和尺寸不变（百分比），重新设置
                                if (lastPercentPosition) {
                                    // 等待图片加载完成再设置
                                    if (modelImg.complete) {
                                        setDragElByPercent(lastPercentPosition);
                                    } else {
                                        modelImg.onload = function () {
                                            setDragElByPercent(lastPercentPosition);
                                        }
                                    }
                                }
                            }
                        }
                    }
                });
                observer.observe(modelImg, { attributes: true });
                // 也监听直接赋值
                modelImg.addEventListener('load', function () {
                    if (lastPercentPosition) {
                        setDragElByPercent(lastPercentPosition);
                    }
                });
            }
            // 初始化记录一次初始百分比位置
            window.addEventListener('DOMContentLoaded', function () {
                onPointerDown()
                lastPercentPosition = getRelativePositionAndSize();
                observeModelImgSrcChange();
            });
        })();
    </script>

    