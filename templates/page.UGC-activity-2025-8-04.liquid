                        let originTop = parseFloat(dragEl.style.top) || dragEl.offsetTop;

                        let scaleDelta = dist / lastDist;
                        let minWidth = 40, minHeight = 40;
                        let maxWidth = parentRect.width, maxHeight = parentRect.height;

                        let newWidth = originWidth * scaleDelta;
                        let newHeight = originHeight * scaleDelta;

                        // 限制最小最大宽高
                        if (newWidth < minWidth) newWidth = minWidth;
                        if (newHeight < minHeight) newHeight = minHeight;
                        if (newWidth > maxWidth) newWidth = maxWidth;
                        if (newHeight > maxHeight) newHeight = maxHeight;

                        // 以中心缩放，调整left/top
                        let centerX = originLeft + originWidth / 2;
                        let centerY = originTop + originHeight / 2;
