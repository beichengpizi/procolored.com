    "member_faqs_RVFVLy": {
      "type": "member-faqs",
      "blocks": {
        "head_b48pAk": {
          "type": "head",
          "settings": {
            "headding": "Membership﻿"
          }
        },
        "faq_6YD79n": {
          "type": "faq",
          "settings": {
            "faq_q": "1. What is the Procolored Loyalty Program?",
            "faq_a": "The Procolored Loyalty program is a program that allows customers to earn points on Procolored.com (excluding third-party retailers like Amazon, eBay, Walmart, etc.) that can be redeemed for rewards and member benefits as a thank-you for their loyalty."
          }
        },
        "faq_6QbgPc": {
          "type": "faq",
          "settings": {
            "faq_q": "2. Who is eligible to join the Program?",
            "faq_a": "Anyone over the age of 14 (or the age required by local law) can join the program."
          }
        },
        "faq_NN3YQJ": {
          "type": "faq",
          "settings": {
            "faq_q": "3. How can I enroll in the Program?",
            "faq_a": "If you already have an account on Procolored.com, you are already enrolled. Simply log in to your account. If you don't have an account, register for an account to start earning rewards."
          }
        },
        "faq_wdAWdn": {
          "type": "faq",
          "settings": {
            "faq_q": "4. Can I exchange my points for cash?",
            "faq_a": "Currently, the points cannot be exchanged for cash."
          }
        },
        "faq_86YX6N": {
          "type": "faq",
          "settings": {
            "faq_q": "5. Can I merge two different accounts?",
            "faq_a": "No, points earned on different accounts cannot be combined or aggregated for any reason."
          }
        },
        "head_3cfpJB": {
          "type": "head",
          "settings": {
            "headding": "Earning Points"
          }
        },
        "faq_fgLaEF": {
          "type": "faq",
          "settings": {
            "faq_q": "1. When will the points I earn by doing tasks be credited to my account?",
            "faq_a": "For creating account, 500 points will be credited to your account right after successful registration. For following us on social media, 200 points will be credited to your account right after following. For purchasing, your points will be credited to your account right after the payment of your order is successful."
          }
        },
        "faq_YYR6jm": {
          "type": "faq",
          "settings": {
            "faq_q": "2. Do the points have an expiration date?",
            "faq_a": "All your points will expire after 2 years in the calendar year. For example, if you earn the points in 2022, the points will expire at 23:59:59pm, 31st December, 2023 (PDT). Points will be cleared after the expiration date."
          }
        },
        "faq_Vtk8fd": {
          "type": "faq",
          "settings": {
            "faq_q": "3. Can shipping or tax fees earn points?",
            "faq_a": "No, Points are not earned for shipping charges or taxes."
          }
        },
        "faq_GJfgrJ": {
          "type": "faq",
          "settings": {
            "faq_q": "4. What happens to my points if I return a product or cancel an order?",
            "faq_a": "If you return a product or cancel an order, the points earned from that purchase will be reversed or decreased. Points will also be deducted for partial refunds due to price adjustments."
          }
        },
        "faq_PRtkMU": {
          "type": "faq",
          "settings": {
            "faq_q": "5. Can I transfer the points to someone else?",
            "faq_a": "No, the points are non-transferable."
          }
        }
      },
