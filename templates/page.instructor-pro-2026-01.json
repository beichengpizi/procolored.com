        "faq_tab_gLtNdk": {
          "type": "faq_tab",
          "disabled": true,
          "settings": {
            "title": "Technique Questions",
            "question1": "Can I use DTF transfer on cups and wooden boards?",
            "answer1": "No, it won't stick.",
            "question2": "Can I use UV DTF stickers on clothes?",
            "answer2": "No, it is not suitable.",
