    "custom_product_faqs_YnWQ6c": {
      "type": "custom-product-faqs",
      "blocks": {
        "faq_tab_kVia44": {
          "type": "faq_tab",
          "settings": {
            "question": "1.Can I combine \"Black Friday Exclusive\" promotional items with sitewide discounts?",
            "answer": "<p>No. \"Black Friday Exclusive\" promotional items cannot be combined with sitewide discounts. All other products will follow the standard sitewide discount rules for each promotional stage.</p>"
          }
        },
        "faq_tab_yfV98N": {
          "type": "faq_tab",
          "settings": {
            "question": "2.Can I get a refund for the price difference between different time periods for Black Friday?",
            "answer": "<p>No. Due to the limited-time nature of our Black Friday promotions, we are unable to offer price protection or refund differences between the Presale and Main Sale events. Each phase has its own exclusive pricing.</p>"
          }
        },
        "faq_tab_amdzDA": {
          "type": "faq_tab",
          "settings": {
            "question": "3.How do I join the Black Friday Lucky Draw?",
            "answer": "<p>Simply place and pay for a printer order between November 24 - November 30 to automatically receive one entry per order.</p>"
          }
        },
        "faq_tab_UVFPeD": {
          "type": "faq_tab",
          "settings": {
            "question": "4.When and how will prizes be delivered?",
            "answer": "<p>Prize distribution starts on December 15. Physical prizes will be shipped after the customer service team confirms shipping details; gift cards will be sent via the email used for your order.</p>"
          }
        },
        "faq_tab_Q8JftU": {
          "type": "faq_tab",
          "settings": {
            "question": "5.What is your return policy for Black Friday purchases?",
            "answer": "<p>All Black Friday purchases are covered by our standard return policy. If the printer is received within one week and no ink is added, it can be shipped back in its original packaging, and returns and exchanges are accepted. Please see our full Return Policy page for more details.</p>"
          }
        },
        "faq_tab_e7p3EU": {
          "type": "faq_tab",
          "settings": {
            "question": "6.What if I want to return my order after winning a prize?",
            "answer": "<p>If you request a return for an order that has won a prize, you must also return the prize or refund an amount equal to the total value of the order and the prize.</p>"
          }
        },
        "faq_tab_X3gTxw": {
          "type": "faq_tab",
          "settings": {
            "question": "7.What if I have issues with my order during the sale?",
            "answer": "<p>Our support team is here to help! Contact us anytime through <strong>live chat or Support@procolored.com</strong>, and we’ll assist you as soon as possible.</p>"
          }
        },
        "faq_tab_LxxRBB": {
          "type": "faq_tab",
          "settings": {
            "question": "8.When do all Black Friday promotion periods and sitewide discounts start and end?",
            "answer": "<p>All Black Friday promotional periods and sitewide discount rules are based on the official countdown timer displayed on our event page. Please refer to the timer for accurate closing times. </p>"
          }
        },
        "faq_tab_papaaP": {
          "type": "faq_tab",
          "settings": {
            "question": "9.Are the discounts applied automatically at checkout?",
            "answer": "<p>Yes. The final price and discounts will be confirmed at checkout. Please note:</p><ul><li>Some bundle prices may adjust slightly due to exchange rates or inventory changes.</li><li>What you see at checkout is the final payable price.</li></ul>"
          }
        },
        "faq_tab_a63fpH": {
          "type": "faq_tab",
          "settings": {
            "question": "10.Do the platform-specific offers follow the same rules as on the official website?",
            "answer": "<p>No. Platform-specific offers (e.g., on Amazon ) are governed by the respective platform’s rules and policies. Please check the offer details on each platform.</p>"
          }
        },
        "faq_tab_jPtH4k": {
          "type": "faq_tab",
          "settings": {
            "question": "11.Who has the final authority to interpret the terms and details of this event?",
            "answer": "<p>Procolored reserves the right to the final interpretation of all Black Friday event details and terms.</p>"
          }
        }
