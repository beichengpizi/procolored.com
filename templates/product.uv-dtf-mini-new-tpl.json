    "custom_liquid_Pqp7Hw": {
      "type": "custom-liquid",
      "name": "Custom liquid",
      "settings": {
        "custom_liquid": "<style>\n\n\n     #swiper-container-klklk  {\n        display: flex;\n        align-items: center;\n        justify-content: center;\n        background: #f8f9fd;\n        padding-bottom: 100px;\n      }\n      #swiper-container-klklk .title {\n        text-align: center;\n        font-weight: 800;\n        font-size: 56px;\n        color: #000000;\n        line-height: 1.14;\n        text-align: center;\n        margin-bottom: 20px;\n      }\n      #swiper-container-klklk .desc {\n        font-weight: 400;\n        font-size: 18px;\n        color: #000000;\n        line-height: 1.33;\n        text-align: center;\n        margin-bottom: 60px;\n      }\n      #swiper-container-klklk .center-area {\n        max-width: 980px;\n        width: 100%;\n        margin: 0 auto;\n\n        box-sizing: border-box;\n        position: relative;\n      }\n      #swiper-container-klklk .swiper-wrapper {\n        align-items: center;\n      }\n      #swiper-container-klklk .swiper-slide {\n        transition: opacity 0.3s, box-shadow 0.3s;\n        opacity: 0.4;\n        filter: blur(1px);\n        pointer-events: none;\n        /* 让未激活slide不可交互 */\n      }\n      #swiper-container-klklk .swiper-slide-active {\n        opacity: 1 !important;\n        filter: none;\n        pointer-events: auto;\n        z-index: 2;\n      }\n      #swiper-container-klklk .slide-content {\n        display: flex;\n        align-items: center;\n        justify-content: center;\n      }\n      #swiper-container-klklk .slide-img {\n        width: 100%;\n        height: auto;\n        object-fit: cover;\n        border-radius: 20px;\n      }\n      #swiper-container-klklk .slide-img.PC {\n        display: block;\n      }\n      #swiper-container-klklk .slide-img.mobile {\n        display: none;\n      }\n\n      #swiper-container-klklk .tab-bar {\n        display: flex;\n        justify-content: center;\n        align-items: flex-end;\n        position: relative;\n        margin-top: 24px;\n        background: transparent;\n        border-bottom: 1px solid #d9d9d9;\n        width: max-content;\n        margin: 0 auto;\n        gap: 20px;\n      }\n      #swiper-container-klklk .tab-bar .tab {\n        position: relative;\n\n        font-size: 16px;\n        color: #b4b4b4;\n        font-weight: 400;\n        cursor: pointer;\n        background: none;\n        outline: none;\n        z-index: 1;\n        padding-bottom: 12px;\n        margin-top: 36px;\n        line-height: 1.25;\n      }\n      #swiper-container-klklk .tab-bar .tab.active {\n        color: #000;\n        font-weight: 800;\n        border-bottom: 1px solid #000;\n      }\n      #swiper-container-klklk .tab-bar .tab-underline {\n        position: absolute;\n        bottom: 0;\n        left: 0;\n        height: 3px;\n        width: 100%;\n        background: transparent;\n        pointer-events: none;\n      }\n      #swiper-container-klklk .tab-bar .tab-underline::before {\n        content: \"\";\n        display: block;\n        position: absolute;\n        left: 0;\n        width: 80px;\n        height: 3px;\n        background: #222;\n        border-radius: 2px;\n        transition: transform 0.3s cubic-bezier(0.4, 0, 0.2, 1);\n        /* 默认在第一个tab下 */\n        transform: translateX(0);\n      }\n\n      @media (max-width: 768px) {\n        #swiper-container-klklk  {\n          padding-bottom: 50px;\n        }\n        #swiper-container-klklk .slide-content {\n          padding: 0px 24px;\n          flex-direction: column;\n        }\n        #swiper-container-klklk .title {\n          padding: 0px 24px;\n          font-size: 26px;\n\n          line-height: 1.31;\n          text-align: left;\n        }\n        #swiper-container-klklk .desc {\n          padding: 0px 24px;\n          font-size: 14px;\n\n          line-height: 1.43;\n          text-align: left;\n          margin-bottom: 30px;\n        }\n        #swiper-container-klklk .tab-bar {\n          display: flex;\n          justify-content: space-between;\n          align-items: center;\n          position: relative;\n          margin-top: 20px;\n     \n          width: auto;\n          gap: 0px;\n\n          box-sizing: border-box;\n          margin-left: 24px;\n          margin-right: 24px;\n        }\n        #swiper-container-klklk .tab-bar .tab {\n          font-size: 14px;\n\n          font-weight: 400;\n          cursor: pointer;\n\n          margin-top: 0;\n          line-height: 1.25;\n        }\n        #swiper-container-klklk .tab-bar .tab.active {\n          color: #000;\n          font-weight: 800;\n          border-bottom: 1px solid #000;\n        }\n        #swiper-container-klklk .tab-bar .tab-underline::before {\n          width: 60px;\n        }\n        #swiper-container-klklk .slide-img.PC {\n          display: none;\n        }\n        #swiper-container-klklk .slide-img.mobile {\n          display: block;\n        }\n      }\n    </style>\n    <div id=\"swiper-container-klklk\" class=\"swiper-container\">\n      <div class=\"center-area\">\n        <div class=\"title\">Expand Your Business with Versatile Stickers</div>\n        <div class=\"desc\">\n          The printer produces a variety of decals that adhere to flat, curved,\n          metal, plastic, wood, and other materials, making them ideal for\n          branding, packaging, customization, craft supplies, and decor.\n        </div>\n        <div class=\"swiper-wrapper\">\n          <!-- Slide 1 -->\n          <div class=\"swiper-slide\">\n            <div class=\"slide-content\">\n              <img\n              style=\"aspect-ratio: 1.899\"\n                class=\"lazyload slide-img PC\"\n                data-src=\"https://www.procolored.com/cdn/shop/files/32872f3858baa2a5ee494abc60d479c5_1960x_crop_center.png?v=1747637137\"\n                alt=\"Mug Seller\"\n              \n              />\n              <img\n        \n                class=\"lazyload slide-img mobile\"\n                data-src=\"https://www.procolored.com/cdn/shop/files/0f9c703cd05f364c27c02ed2448db5e0_996x_crop_center.png?v=1747637157\"\n                alt=\"Mug Seller\"\n              />\n            </div>\n          </div>\n          <!-- Slide 2 -->\n          <div class=\"swiper-slide\">\n            <div class=\"slide-content\">\n              <img\n              style=\"aspect-ratio: 1.899\"\n                class=\"lazyload slide-img PC\"\n                data-src=\"https://www.procolored.com/cdn/shop/files/a02562a98c5e202ec4ea3531959719c8_1960x_crop_center.png?v=1747637176\"\n                alt=\"Tumbler Seller\"\n              />\n              <img\n        \n                class=\"lazyload slide-img mobile\"\n                data-src=\"https://www.procolored.com/cdn/shop/files/081b4b61ccf508c37a549aee2622d743_981x_crop_center.png?v=1747637192\"\n                alt=\"Tumbler Seller\"\n              />\n            </div>\n          </div>\n          <!-- Slide 3 -->\n          <div class=\"swiper-slide\">\n            <div class=\"slide-content\">\n              <img\n              style=\"aspect-ratio: 1.899\"\n                class=\"lazyload slide-img PC\"\n                data-src=\"https://www.procolored.com/cdn/shop/files/254cec7983dbd19c783b2f1ae8d091a4_1960x_crop_center.png?v=1747637214\"\n                alt=\"Sticker Seller\"\n              />\n              <img\n        \n                class=\"lazyload slide-img mobile\"\n                data-src=\"https://www.procolored.com/cdn/shop/files/2963587280b534a4281e91ebab81ca15_981x_crop_center.png?v=1747637228\"\n                alt=\"Sticker Seller\"\n              />\n            </div>\n          </div>\n        </div>\n        <!-- Tab Bar -->\n        <div class=\"tab-bar\">\n          <span class=\"tab active\" data-index=\"0\">Mug Seller</span>\n          <span class=\"tab\" data-index=\"1\">Tumbler Seller</span>\n          <span class=\"tab\" data-index=\"2\">Sticker Seller</span>\n        </div>\n      </div>\n    </div>\n    <script>\n     const swiper_klklk = new Swiper(\"#swiper-container-klklk .center-area\", {\n        spaceBetween: 34,\n        slidesPerView: 1,\n        centeredSlides: true,\n        loop: false,\n        on: {\n          slideChange: function () {\n            // 切换tab高亮\n            var tabs_klklk = document.querySelectorAll(\"#swiper-container-klklk .tab\");\n            tabs_klklk.forEach(function (tab, idx) {\n              tab.classList.toggle(\"active\", idx === swiper_klklk.activeIndex);\n            });\n          },\n        },\n        breakpoints: {\n          798: {\n            slidesPerView: 1,\n          },\n        },\n      });\n\n      // 点击tab切换\n      document.querySelectorAll(\"#swiper-container-klklk .tab\").forEach(function (tab, idx) {\n        tab.addEventListener(\"click\", function () {\n          swiper_klklk.slideTo(idx);\n        });\n      });\n    </script>"
      }
    },
