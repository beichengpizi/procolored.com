    "custom_product_faqs_pcyH9G": {
      "type": "custom-product-faqs",
      "blocks": {
        "faq_tab_KwQpci": {
          "type": "faq_tab",
          "settings": {
            "question": "1.Why did Procolored create Dream Noa?",
            "answer": "<p>Dream Noa was created to help users realize their creative dreams and encourage every act of creation, not just as a mascot, but it is the embodiment of Procolored as a companion to users.<br/></p>"
          }
        },
        "faq_tab_zUFnnk": {
          "type": "faq_tab",
          "settings": {
            "question": "2.When and where will Dream Noa officially launch?",
            "answer": "<p>Dream Noa will make its <strong>official debut on October 21, 2025</strong>, during the <strong>Procolored Brand Meet-Up</strong> in Orlando, USA.</p>"
          }
        },
        "faq_tab_rRqcb4": {
          "type": "faq_tab",
          "settings": {
            "question": "3.Can I stack this Dream Noa debut discount with other promotions?",
            "answer": "<p>No, the Dream Noa debut discount cannot be stacked with other offers.</p>"
          }
        },
        "faq_tab_U3RbgK": {
          "type": "faq_tab",
          "settings": {
            "question": "4.Is the exhibition invitation really free to attend? How do I register?",
            "answer": "<p>Yes, it's completely free! Click the \"Free Pass\" button or use this direct link to register:<br/>https://www.compusystems.com/servlet/ar?evt_uid=284&InvitesVIP=727604</p>"
          }
        },
        "faq_tab_WEAV88": {
          "type": "faq_tab",
          "settings": {
            "question": "5.What is ColorChannels, and do I need to create a new account?",
            "answer": "<p>ColorChannels is Procolored's official online community for creators to share ideas, get inspired, and earn rewards. Yes, you will need to register a new account if you don't have one—it's quick, free, and your gateway to participating in the challenge and many future activities!</p>"
          }
        },
        "faq_tab_h7wMDe": {
          "type": "faq_tab",
          "settings": {
            "question": "6.How and when will the challenge winners be announced?",
            "answer": "<p>After the challenge ends on November 10th. Winners will be announced within <strong>7 business days</strong> on the ColorChannels community and via direct message and email to the winners.<br/><br/></p>"
          }
        },
        "faq_tab_AQUr8p": {
          "type": "faq_tab",
          "settings": {
            "question": "7.What are the terms for the $30 shopping card and grand prize draw?",
            "answer": "<p>Once you successfully register on ColorChannels and your challenge post is approved, you'll earn points and automatically qualify for the grand prize draw.</p><p><br/>Please note: The $30 shopping card will be distributed via email after the challenge concludes on November 10th. Specific terms and conditions, such as validity period and minimum spend, will be detailed in the email.</p>"
          }
        },
        "faq_tab_6XGYTk": {
          "type": "faq_tab",
          "settings": {
            "question": "8.What can I win in the community challenge giveaway?",
            "answer": "<ul><li>Grand Prizes: DTF Panda Oven, Heat Press Machine.</li><li>Essential Supplies: DTF White Ink (1*250ml), Nozzle Protection Fluid (1*250ml), Procolored DTF Transfers.</li><li>Exclusive Savings: Special Discount Coupon.</li></ul>"
          }
        }
      },
