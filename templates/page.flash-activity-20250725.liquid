<script>
  // ‰∏∫ÊâÄÊúâFAQÊ†áÈ¢òÊ∑ªÂä†ÁÇπÂáª‰∫ã‰ª∂ÁõëÂê¨Âô®
  document.querySelectorAll('.flash-activity-faq-item-content-item').forEach((header) => {
    header.addEventListener('click', (e) => {
      // Ëé∑ÂèñÂΩìÂâçÁÇπÂáªÁöÑÂÖÉÁ¥†
      const currentSection = e.currentTarget;
      // Ê£ÄÊü•ÂΩìÂâçÈÉ®ÂàÜÊòØÂê¶Â∑≤ÁªèÂ±ïÂºÄ
      const isActive = currentSection.classList.contains('active');

      // ÂÖ≥Èó≠ÊâÄÊúâÂ∑≤Â±ïÂºÄÁöÑsection
      document.querySelectorAll('.flash-activity-faq-item-content-item.active').forEach((section) => {
        // Â¶ÇÊûú‰∏çÊòØÂΩìÂâçÁÇπÂáªÁöÑsectionÂàôÂÖ≥Èó≠ÂÆÉ
        if (section !== currentSection) {
          toggleSection(section, false);
        }
      });

      // ÂàáÊç¢ÂΩìÂâçsectionÁöÑÂ±ïÂºÄ/Êî∂Ëµ∑Áä∂ÊÄÅ
      toggleSection(currentSection, !isActive);
    });
  });

  /**
   * Â§ÑÁêÜsectionÁöÑÂºÄÂÖ≥Áä∂ÊÄÅ
   * @param {HTMLElement} section - Ë¶ÅÂàáÊç¢Áä∂ÊÄÅÁöÑFAQÈÉ®ÂàÜ
   * @param {boolean} isOpen - ÊòØÂê¶Â±ïÂºÄ,trueË°®Á§∫Â±ïÂºÄ,falseË°®Á§∫Êî∂Ëµ∑
   */
  function toggleSection(section, isOpen) {
    // Ê†πÊçÆÂºÄÂÖ≥Áä∂ÊÄÅËÆæÁΩÆÂä®ÁîªÁ±ªÂêç
    const animationClass = isOpen ? 'opening' : 'closing';
    // ËÆæÁΩÆÂä®ÁîªÊåÅÁª≠Êó∂Èó¥,Â±ïÂºÄËæÉÊÖ¢,Êî∂Ëµ∑ËæÉÂø´
    const duration = isOpen ? 500 : 300;

    // Ê∑ªÂä†Âä®ÁîªÁ±ª
    section.classList.add(animationClass);

    // Ê†πÊçÆÂºÄÂÖ≥Áä∂ÊÄÅÊ∑ªÂä†ÊàñÁßªÈô§activeÁ±ª
    if (isOpen) {
      section.classList.add('active');
    } else {
      section.classList.remove('active');
    }

    // Âä®ÁîªÁªìÊùüÂêéÁßªÈô§Âä®ÁîªÁ±ª
    setTimeout(() => {
      section.classList.remove(animationClass);
    }, duration);
  }
</script>
